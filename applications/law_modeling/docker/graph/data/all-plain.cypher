UNWIND [{_id:71, properties:{index:"h", content:"buộc phải thực hiện ngay các biện pháp bảo đảm an toàn công trình đường sắt, bảo đảm an toàn giao thông đường sắt theo quy định;"}}, {_id:73, properties:{index:"i", content:"buộc phải dỡ bỏ vật che khuất biển hiệu, mốc hiệu, tín hiệu của công trình đường sắt hoặc đưa tấm đan bê tông, gỗ, sắt thép, các vật liệu khác (đặt trái phép) ra khỏi phạm vi bảo vệ công trình đường sắt hoặc hạ độ cao của cây trồng có chiều cao vượt quá quy định, di dời cây trồng không đúng quy định hoặc có ảnh hưởng đến an toàn công trình, an toàn giao thông đường sắt;"}}, {_id:75, properties:{index:"k", content:"buộc đưa phương tiện, thiết bị, vật liệu, hàng hóa, chất phế thải, các vật phẩm khác (để trái phép) ra khỏi phạm vi bảo vệ công trình đường sắt, hành lang an toàn giao thông đường sắt;"}}, {_id:1103, properties:{index:"a", content:"xếp hàng hóa lên mỗi xe ô tô (kể cả rơ moóc và sơ mi rơ moóc) vượt quá trọng tải (khối lượng hàng chuyên chở) cho phép tham gia giao thông được ghi trong giấy chứng nhận kiểm định an toàn kỹ thuật và bảo vệ môi trường của xe trên 10% đến 50% (trừ xe xi téc chở chất lỏng), trên 20% đến 50% đối với xe xi téc chở chất lỏng;"}}, {_id:3153, properties:{index:"đ", content:"các loại xe tương tự xe gắn máy là phương tiện giao thông đường bộ chạy bằng động cơ, có hai bánh hoặc ba bánh và vận tốc thiết kế lớn nhất không lớn hơn 50 km/h, trừ các xe quy định tại điểm e khoản này;"}}, {_id:1104, properties:{index:"b", content:"xếp hàng hóa lên xe ô tô mà không ký xác nhận việc xếp hàng hóa vào giấy vận tải theo quy định."}}, {_id:77, properties:{index:"l", content:"buộc phải tháo dỡ, di chuyển các công trình, nhà ở, lều, quán gây trở ngại cho việc xây dựng, cải tạo, mở rộng và bảo đảm an toàn công trình đường sắt hoặc buộc phải tháo dỡ, di chuyển lều, quán (dựng trái phép), biển quảng cáo, biển chỉ dẫn, các vật che chắn khác (đặt, treo trái phép) ra khỏi phạm vi đất dành cho đường sắt hoặc buộc phải phá dỡ công trình hết hạn sử dụng, tháo dỡ công trình bị thu hồi, hủy giấy phép;"}}, {_id:78, properties:{index:"m", content:"buộc phải gia cố, di chuyển hoặc cải tạo công trình gây ảnh hưởng đến an toàn giao thông đường sắt theo yêu cầu của cơ quan có thẩm quyền;"}}, {_id:1106, properties:{index:"a", content:"không niêm yết hoặc niêm yết không chính xác, đầy đủ tên, số điện thoại của đơn vị kinh doanh vận tải ở phần đầu mặt ngoài hai bên thân hoặc mặt ngoài hai bên cánh cửa xe ô tô chở hành khách theo quy định, trừ hành vi vi phạm quy định tại điểm d khoản 4 điều này;"}}, {_id:1107, properties:{index:"b", content:"không niêm yết hoặc niêm yết không chính xác, đầy đủ tên, số điện thoại của đơn vị kinh doanh vận tải hàng hóa, khối lượng bản thân xe, khối lượng hàng hóa cho phép chuyên chở, khối lượng toàn bộ cho phép tham gia giao thông trên cánh cửa xe ô tô tải theo quy định;"}}, {_id:1108, properties:{index:"c", content:"không niêm yết hoặc niêm yết không chính xác, đầy đủ tên, số điện thoại của đơn vị kinh doanh vận tải hàng hóa, khối lượng bản thân ô tô đầu kéo, khối lượng hàng hóa cho phép chuyên chở, khối lượng toàn bộ cho phép kéo theo trên cánh cửa xe ô tô đầu kéo theo quy định; không niêm yết hoặc niêm yết không chính xác, đầy đủ tên, số điện thoại của đơn vị kinh doanh vận tải hàng hóa, khối lượng hàng hóa cho phép chuyên chở, khối lượng toàn bộ cho phép tham gia giao thông trên rơ moóc, sơ mi rơ moóc theo quy định;"}}, {_id:1109, properties:{index:"d", content:"không niêm yết hoặc niêm yết không chính xác, đầy đủ tên, số điện thoại của đơn vị kinh doanh taxi tải, chữ taxi tải, tự trọng của xe, trọng tải được phép chở của xe ở mặt ngoài hai bên thành xe hoặc mặt ngoài hai bên cánh cửa buồng lái xe taxi tải theo quy định;"}}, {_id:82, properties:{index:"n", content:"buộc phải tổ chức sửa chữa, bổ sung, gia cố, thay thế các hư hỏng kết cấu hạ tầng đường sắt để bảo đảm chất lượng theo công lệnh tốc độ, công lệnh tải trọng đã công bố;"}}, {_id:83, properties:{index:"o", content:"buộc phải bố trí đủ thiết bị an toàn, tín hiệu, biển báo, tín hiệu phòng vệ theo quy định;"}}, {_id:1110, properties:{index:"đ", content:"không niêm yết hoặc niêm yết không chính xác, đầy đủ theo quy định trên xe ô tô chở hành khách về: biển số xe; khối lượng hành lý miễn cước; số điện thoại đường dây nóng;"}}, {_id:1111, properties:{index:"e", content:"không đánh số thứ tự ghế ngồi trên xe ô tô chở hành khách theo quy định;"}}, {_id:1112, properties:{index:"g", content:"sử dụng xe ô tô kinh doanh vận tải hành khách theo tuyến cố định, xe ô tô kinh doanh vận tải hành khách bằng xe buýt không có chỗ ưu tiên cho người khuyết tật, người cao tuổi và phụ nữ mang thai theo quy định;"}}, {_id:1113, properties:{index:"h", content:"sử dụng xe ô tô kinh doanh vận tải hành khách không có hướng dẫn cho hành khách về an toàn giao thông, thoát hiểm khi xảy ra sự cố trên xe theo quy định."}}, {_id:86, properties:{index:"p", content:"buộc phải để phương tiện, vật liệu, thiết bị thi công theo đúng quy định, không gây cản trở chạy tàu;"}}, {_id:1116, properties:{index:"a", content:"không cấp lệnh vận chuyển, giấy vận tải (giấy vận chuyển) cho lái xe theo quy định;"}}] AS row
