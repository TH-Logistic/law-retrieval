UNWIND [{_id:1076, properties:{index:"a", content:"chở hàng siêu trường, siêu trọng không có báo hiệu kích thước của hàng theo quy định;"}}, {_id:1077, properties:{index:"b", content:"không thực hiện đúng quy định trong giấy phép lưu hành, trừ các hành vi vi phạm quy định tại điểm b, điểm c, điểm d khoản 2 điều này."}}, {_id:1079, properties:{index:"a", content:"chở hàng siêu trường, siêu trọng không có giấy phép lưu hành hoặc có giấy phép lưu hành nhưng đã hết giá trị sử dụng hoặc sử dụng giấy phép lưu hành không do cơ quan có thẩm quyền cấp;"}}, {_id:1080, properties:{index:"b", content:"chở hàng siêu trường, siêu trọng có giấy phép lưu hành còn giá trị sử dụng nhưng tổng trọng lượng, kích thước bao ngoài của xe (sau khi đã xếp hàng lên xe) vượt quá quy định trong giấy phép lưu hành;"}}, {_id:1081, properties:{index:"c", content:"chở hàng siêu trường, siêu trọng có giấy phép lưu hành còn giá trị sử dụng nhưng đi không đúng tuyến đường quy định trong giấy phép lưu hành;"}}, {_id:53, properties:{index:"a", content:"buộc phải lắp đặt đúng, đủ và duy trì hoạt động bình thường của hệ thống báo hiệu, tín hiệu, thiết bị theo quy định;"}}, {_id:54, properties:{index:"b", content:"buộc phải tổ chức thử hãm hoặc tổ chức thực hiện phòng vệ theo quy định;"}}, {_id:1082, properties:{index:"d", content:"chở hàng siêu trường, siêu trọng có giấy phép lưu hành còn giá trị sử dụng nhưng chở không đúng loại hàng quy định trong giấy phép lưu hành."}}, {_id:56, properties:{index:"c", content:"buộc phải để toa xe chở hàng nguy hiểm (chất nổ, chất cháy) hoặc để ghi dẫn sang đường khác theo đúng quy định về dồn tàu;"}}, {_id:1084, properties:{index:"a", content:"thực hiện hành vi quy định tại khoản 1 điều này bị tước quyền sử dụng giấy phép lái xe từ 01 tháng đến 03 tháng;"}}, {_id:1085, properties:{index:"b", content:"thực hiện hành vi quy định tại khoản 2 điều này bị tước quyền sử dụng giấy phép lái xe từ 02 tháng đến 04 tháng;"}}, {_id:1086, properties:{index:"c", content:"thực hiện hành vi quy định tại điểm a khoản 2 điều này bị tịch thu giấy phép lưu hành đã hết giá trị sử dụng hoặc không do cơ quan có thẩm quyền cấp."}}, {_id:3147, properties:{index:"c", content:"các loại xe tương tự xe mô tô là phương tiện giao thông đường bộ chạy bằng động cơ, có hai bánh hoặc ba bánh, có dung tích làm việc của động cơ từ 50 cm3 trở lên, có vận tốc thiết kế lớn nhất lớn hơn 50 km/h, có khối lượng bản thân không lớn hơn 400 kg;"}}, {_id:60, properties:{index:"d", content:"buộc phải ra khỏi đường sắt, cầu, hầm dành riêng cho đường sắt;"}}, {_id:61, properties:{index:"đ", content:"buộc phải đưa đất, đá, cát, vật chướng ngại, rơm, rạ, nông sản, rác thải sinh hoạt, chất độc hại, chất phế thải, chất dễ cháy, dễ nổ các loại vật tư, vật liệu, vật phẩm khác ra khỏi đường sắt, công trình đường sắt khác hoặc phạm vi đất dành cho đường sắt;"}}, {_id:3148, properties:{index:"d", content:"xe máy điện là xe gắn máy được dẫn động bằng động cơ điện có công suất lớn nhất không lớn hơn 4 kw, có vận tốc thiết kế lớn nhất không lớn hơn 50 km/h;"}}, {_id:1091, properties:{index:"a", content:"vận chuyển hàng nguy hiểm mà dừng xe, đỗ xe ở nơi đông người, khu dân cư, công trình quan trọng; không có báo hiệu hàng nguy hiểm theo quy định, trừ các hành vi vi phạm quy định tại khoản 2 điều này;"}}, {_id:1092, properties:{index:"b", content:"vận chuyển các chất gây ô nhiễm môi trường không theo đúng quy định về bảo vệ môi trường, trừ các hành vi vi phạm quy định tại khoản 2 điều này."}}, {_id:67, properties:{index:"e", content:"buộc phải đưa bè, mảng, phương tiện vận tải thủy hoặc các vật thể khác ra khỏi phạm vi bảo vệ cầu đường sắt;"}}, {_id:68, properties:{index:"g", content:"buộc phải đưa phương tiện giao thông đường bộ, vật tư, vật liệu, máy móc thiết bị, hàng hóa, biển phòng vệ, biển báo tạm thời ra khỏi khổ giới hạn tiếp giáp kiến trúc đường sắt;"}}] AS row
