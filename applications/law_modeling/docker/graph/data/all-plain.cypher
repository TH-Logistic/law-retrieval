UNWIND [{_id:3116, properties:{index:"g", content:"tổ chức kinh tế được thành lập theo quy định của luật hợp tác xã gồm: hợp tác xã, liên hiệp hợp tác xã;"}}, {_id:30, properties:{index:"s", content:"buộc phải lập, cập nhật, lưu trữ đầy đủ, chính xác lý lịch phương tiện, lý lịch hành nghề của lái xe, các hồ sơ, tài liệu có liên quan trong quá trình quản lý, điều hành hoạt động vận tải của đơn vị theo quy định;"}}, {_id:31, properties:{index:"t", content:"buộc phải khôi phục lại nhãn hiệu, màu sơn ghi trong giấy đăng ký xe theo quy định hoặc buộc phải thực hiện đúng quy định về biển số, quy định về kẻ chữ trên thành xe và cửa xe;"}}, {_id:1038, properties:{index:"c", content:"không chốt, đóng cố định cửa sau thùng xe khi xe đang chạy."}}, {_id:34, properties:{index:"u", content:"buộc phải khôi phục lại hình dáng, kích thước, tình trạng an toàn kỹ thuật ban đầu của xe và đăng kiểm lại trước khi đưa phương tiện ra tham gia giao thông;"}}, {_id:3122, properties:{index:"h", content:"tổ chức được thành lập theo quy định của luật đầu tư gồm: nhà đầu tư trong nước, nhà đầu tư nước ngoài và tổ chức kinh tế có vốn đầu tư nước ngoài; chi nhánh, văn phòng đại diện của thương nhân, doanh nghiệp nước ngoài tại việt nam; văn phòng đại diện của tổ chức xúc tiến thương mại nước ngoài tại việt nam;"}}, {_id:36, properties:{index:"v", content:"buộc phải thực hiện điều chỉnh thùng xe theo đúng quy định hiện hành, đăng kiểm lại và điều chỉnh lại khối lượng hàng hóa cho phép chuyên chở ghi trong giấy chứng nhận kiểm định an toàn kỹ thuật và bảo vệ môi trường theo quy định hiện hành trước khi đưa phương tiện ra tham gia giao thông;"}}, {_id:3126, properties:{index:"i", content:"các tổ chức khác được thành lập theo quy định của pháp luật."}}, {_id:40, properties:{index:"x", content:"buộc phải làm thủ tục đăng ký xe, đăng ký sang tên hoặc thủ tục đổi lại, thu hồi giấy đăng ký xe, biển số xe, giấy chứng nhận kiểm định an toàn kỹ thuật và bảo vệ môi trường theo quy định;"}}, {_id:1040, properties:{index:"a", content:"điều khiển xe (kể cả rơ moóc và sơ mi rơ moóc) chở hàng vượt trọng tải (khối lượng hàng chuyên chở) cho phép tham gia giao thông được ghi trong giấy chứng nhận kiểm định an toàn kỹ thuật và bảo vệ môi trường của xe trên 10% đến 30% (trừ xe xi téc chở chất lỏng), trên 20% đến 30% đối với xe xi téc chở chất lỏng;"}}, {_id:1041, properties:{index:"b", content:"chở hàng trên nóc thùng xe; chở hàng vượt quá bề rộng thùng xe; chở hàng vượt phía trước, phía sau thùng xe trên 10% chiều dài xe;"}}, {_id:1042, properties:{index:"c", content:"chở người trên thùng xe trái quy định; để người nằm, ngồi trên mui xe, đu bám bên ngoài xe khi xe đang chạy;"}}, {_id:1043, properties:{index:"d", content:"điều khiển xe tham gia kinh doanh vận tải hàng hóa không có hoặc không mang theo giấy vận tải (giấy vận chuyển) bằng văn bản giấy theo quy định hoặc không có thiết bị để truy cập vào được phần mềm thể hiện nội dung của giấy vận tải (giấy vận chuyển) theo quy định hoặc có thiết bị để truy cập nhưng không cung cấp cho lực lượng chức năng khi có yêu cầu;"}}, {_id:1044, properties:{index:"đ", content:"điều khiển xe kéo theo rơ moóc, sơ mi rơ moóc mà khối lượng toàn bộ (bao gồm khối lượng bản thân rơ moóc, sơ mi rơ moóc và khối lượng hàng chuyên chở) của rơ moóc, sơ mi rơ moóc vượt khối lượng cho phép kéo theo được ghi trong giấy chứng nhận kiểm định an toàn kỹ thuật và bảo vệ môi trường của xe trên 10% đến 30%."}}, {_id:1046, properties:{index:"a", content:"điều khiển xe tham gia kinh doanh vận tải hàng hóa không gắn thiết bị giám sát hành trình của xe theo quy định, trừ hành vi vi phạm quy định tại điểm c khoản 5 điều này;"}}, {_id:1047, properties:{index:"b", content:"không sử dụng thẻ nhận dạng lái xe để đăng nhập thông tin theo quy định hoặc sử dụng thẻ nhận dạng lái xe của lái xe khác để đăng nhập thông tin khi điều khiển xe tham gia kinh doanh vận tải hàng hóa;"}}, {_id:1048, properties:{index:"c", content:"điều khiển xe ô tô kinh doanh vận tải hàng hóa không lắp camera theo quy định (đối với loại xe có quy định phải lắp camera) hoặc có lắp camera nhưng không ghi, không lưu trữ được hình ảnh của người lái xe trong quá trình điều khiển xe tham gia giao thông theo quy định."}}, {_id:1049, properties:{index:"a", content:"điều khiển xe taxi tải không lắp đồng hồ tính tiền cước hoặc lắp đồng hồ tính tiền cước không đúng quy định;"}}, {_id:42, properties:{index:"y", content:"buộc phải đưa phương tiện quay trở lại khu kinh tế thương mại đặc biệt, khu kinh tế cửa khẩu quốc tế."}}, {_id:1050, properties:{index:"b", content:"chở hàng vượt quá chiều cao xếp hàng cho phép đối với xe ô tô tải (kể cả rơ moóc và sơ mi rơ moóc);"}}] AS row
