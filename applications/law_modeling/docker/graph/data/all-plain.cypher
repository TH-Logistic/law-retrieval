UNWIND [{_id:1975, properties:{index:"b", content:"trước khi đưa vào sử dụng phải được cấp quyết định đầu tư, chủ đầu tư nghiệm thu đủ điều kiện theo quy định;"}}, {_id:1976, properties:{index:"c", content:"được kết nối với phần mềm giám sát, quản lý tập trung dữ liệu tải trọng xe của tổng cục đường bộ việt nam."}}, {_id:812, properties:{index:"a", content:"chăn dắt súc vật ở mái đường; buộc súc vật vào hàng cây hai bên đường hoặc vào cọc tiêu, biển báo, rào chắn, các công trình phụ trợ của giao thông đường bộ;"}}, {_id:813, properties:{index:"b", content:"tự ý leo trèo lên mố, trụ, dầm cầu."}}, {_id:1978, properties:{index:"a", content:"thiết bị ghi hình khi chụp hình ảnh thực tế phải bảo đảm hình ảnh có hiển thị ngày, tháng, năm, giờ, phút, giây và địa điểm chụp hình;"}}, {_id:1979, properties:{index:"b", content:"thiết bị ghi hình ảnh động (camera) khi ghi, thu hình ảnh thực tế, clip hình ảnh phải bảo đảm có hiển thị ngày, tháng, năm, giờ, phút, giây, địa điểm ghi, thu hình ảnh, clip;"}}, {_id:815, properties:{index:"a", content:"tự ý đốt lửa trên cầu, dưới gầm cầu; neo đậu tàu, thuyền dưới gầm cầu hoặc trong phạm vi hành lang an toàn cầu;"}}, {_id:1980, properties:{index:"c", content:"trường hợp thiết bị ghi hình không có chức năng xác định địa điểm thì trong phiếu xác nhận kết quả thiết bị ghi hình phải ghi rõ địa điểm ghi hình."}}, {_id:816, properties:{index:"b", content:"tự ý be bờ, tát nước qua mặt đường giao thông; đặt ống bơm nước, bơm cát qua đường, đốt lửa trên mặt đường."}}, {_id:818, properties:{index:"a", content:"không bổ sung hoặc sửa chữa kịp thời theo quy định các biển báo hiệu nguy hiểm đã bị mất, bị hư hỏng mất tác dụng; không có biện pháp khắc phục kịp thời theo quy định các hư hỏng của công trình đường bộ gây mất an toàn giao thông;"}}, {_id:819, properties:{index:"b", content:"không phát hiện hoặc không có biện pháp ngăn chặn, báo cáo kịp thời các hành vi lấn chiếm, sử dụng trái phép hành lang an toàn giao thông đường bộ, phần đất dọc hai bên đường bộ dùng để quản lý, bảo trì, bảo vệ công trình đường bộ;"}}, {_id:820, properties:{index:"c", content:"không cắm cột thủy chí và có biện pháp ngăn chặn phương tiện qua những đoạn đường bị ngập nước sâu trên 0,2 m;"}}, {_id:1985, properties:{index:"a", content:"phiếu cân ghi kết quả từ thiết bị cân kiểm tra tải trọng xe, có chữ ký và họ tên của người sử dụng thiết bị cân, góc trái phía trên của phiếu có dấu treo của đơn vị được giao trực tiếp quản lý, sử dụng thiết bị cân kiểm tra tải trọng xe;"}}, {_id:821, properties:{index:"d", content:"không có quy trình quản lý, vận hành, khai thác, bảo trì đường bộ theo quy định hoặc không thực hiện đầy đủ các nội dung quy định trong quy trình quản lý, vận hành, khai thác, bảo trì đường bộ đã được phê duyệt."}}, {_id:1986, properties:{index:"b", content:"phiếu cân gồm các thông tin cơ bản sau: tên lái xe, tên chủ xe hoặc đơn vị vận tải, tên sở giao thông vận tải nơi cấp phù hiệu xe; thông tin ghi trong giấy chứng nhận kiểm định an toàn kỹ thuật và bảo vệ môi trường của xe (biển số xe, loại xe, số trục, khối lượng bản thân, khối lượng hàng hóa chuyên chở, khối lượng kéo theo, chiều dài cơ sở, kích thước bao, kích thước thùng chở hàng); thông tin về tải trọng trục, tổng trọng lượng (khối lượng toàn bộ); kết quả và kết luận đo lường kiểm tra."}}, {_id:1988, properties:{index:"a", content:"phiếu xác nhận kết quả thiết bị ghi hình phải kèm bản ảnh hoặc clip ghi hình, có chữ ký và họ tên của người sử dụng thiết bị ghi hình, góc trái phía trên của phiếu có dấu treo của đơn vị được giao trực tiếp quản lý, sử dụng thiết bị ghi hình;"}}, {_id:1989, properties:{index:"b", content:"phiếu xác nhận kết quả thiết bị ghi hình gồm các thông tin cơ bản sau: tên của tổ chức, cá nhân thực hiện hành vi vi phạm (nếu xác minh được); tên phương tiện; hành vi vi phạm; địa điểm, thời gian vi phạm."}}, {_id:1991, properties:{index:"a", content:"đơn vị được giao trực tiếp quản lý, sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ có trách nhiệm mở sổ theo dõi việc sử dụng phiếu cân kiểm tra tải trọng xe và phiếu xác nhận kết quả thiết bị ghi hình;"}}, {_id:823, properties:{index:"a", content:"di chuyển chậm trễ các công trình, nhà, lều quán xây dựng trái phép hoặc cố tình trì hoãn việc di chuyển gây cản trở cho việc giải phóng mặt bằng để xây dựng, cải tạo, mở rộng và bảo vệ công trình đường bộ khi có quyết định của cơ quan nhà nước có thẩm quyền;"}}, {_id:1992, properties:{index:"b", content:"phiếu cân kiểm tra tải trọng xe và phiếu xác nhận kết quả thiết bị ghi hình kèm bản ảnh hoặc clip ghi hình được lưu hồ sơ xử phạt vi phạm hành chính;"}}] AS row
