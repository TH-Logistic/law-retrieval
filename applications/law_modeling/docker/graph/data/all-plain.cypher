UNWIND [{_id:739, properties:{index:"b", content:"dựng cổng chào hoặc các vật che chắn khác trái quy định trong phạm vi đất dành cho đường bộ gây ảnh hưởng đến trật tự, an toàn giao thông đường bộ;"}}, {_id:740, properties:{index:"c", content:"treo băng rôn, biểu ngữ trái phép trong phạm vi đất dành cho đường bộ gây ảnh hưởng đến trật tự an toàn giao thông đường bộ;"}}, {_id:741, properties:{index:"d", content:"đặt, treo biển hiệu, biển quảng cáo trên đất của đường bộ ở đoạn đường ngoài đô thị, trừ các hành vi vi phạm quy định tại điểm b khoản 8 điều này;"}}, {_id:742, properties:{index:"đ", content:"chiếm dụng dải phân cách giữa của đường đôi làm nơi để xe, trông, giữ xe;"}}, {_id:743, properties:{index:"e", content:"sử dụng trái phép đất của đường bộ ở đoạn đường ngoài đô thị làm nơi sửa chữa phương tiện, máy móc, thiết bị; rửa xe, bơm nước mui xe gây ảnh hưởng đến trật tự an toàn giao thông đường bộ."}}, {_id:1896, properties:{index:"e", content:"không bố trí đủ nhân viên công tác trên tàu theo quy định;"}}, {_id:1897, properties:{index:"g", content:"không xây dựng quy trình tác nghiệp đối với các chức danh nhân viên công tác trên tàu theo quy định."}}, {_id:1898, properties:{index:"a", content:"thực hiện hành vi quy định tại điểm a, điểm b khoản 1 điều này buộc phải đưa thi hài, hài cốt, động vật sống xuống tàu tại ga đến gần nhất để xử lý theo quy định;"}}, {_id:1899, properties:{index:"b", content:"thực hiện hành vi quy định tại điểm a khoản 3 điều này buộc phải thực hiện ngay nhiệm vụ vận tải đặc biệt, an sinh xã hội theo yêu cầu của người đứng đầu cơ quan quản lý nhà nước có thẩm quyền theo quy định;"}}, {_id:1900, properties:{index:"c", content:"thực hiện hành vi quy định tại điểm b, điểm c, điểm đ khoản 3 điều này buộc phải thực hiện các biện pháp bảo đảm an toàn theo quy định."}}, {_id:746, properties:{index:"a", content:"dựng rạp, lều quán, cổng ra vào, tường rào các loại, công trình khác trái phép trong phạm vi đất dành cho đường bộ, trừ các hành vi vi phạm quy định tại điểm đ khoản 6, điểm b khoản 8, điểm a khoản 9 điều này;"}}, {_id:1904, properties:{index:"a", content:"nhân viên bán vé của nhà ga, nhân viên bán vé của đại lý bán vé tàu, nhân viên bán vé trên tàu bán vé tàu trái quy định;"}}, {_id:1905, properties:{index:"b", content:"mua, bán vé tàu nhằm mục đích thu lợi bất chính."}}, {_id:1907, properties:{index:"a", content:"vận chuyển vé tàu giả;"}}, {_id:1908, properties:{index:"b", content:"bán vé tàu giả;"}}, {_id:1909, properties:{index:"c", content:"tàng trữ vé tàu giả."}}, {_id:747, properties:{index:"b", content:"sử dụng trái phép lòng đường đô thị, hè phố để: họp chợ; kinh doanh dịch vụ ăn uống; bày, bán hàng hóa; sửa chữa phương tiện, máy móc, thiết bị; rửa xe; đặt, treo biển hiệu, biển quảng cáo; xây, đặt bục bệ; làm mái che hoặc thực hiện các hoạt động khác gây cản trở giao thông, trừ các hành vi vi phạm quy định tại điểm d, điểm đ, điểm e, điểm g khoản 6; khoản 7; điểm a khoản 8 điều này;"}}, {_id:1910, properties:{index:"a", content:"thực hiện hành vi quy định tại khoản 1 điều này bị tịch thu vé tàu giả;"}}, {_id:1911, properties:{index:"b", content:"thực hiện hành vi quy định tại điểm b khoản 2 điều này bị tịch thu toàn bộ số vé tàu hiện có;"}}, {_id:748, properties:{index:"c", content:"chiếm dụng lòng đường đô thị hoặc hè phố dưới 05 m2 làm nơi trông, giữ xe;"}}] AS row
