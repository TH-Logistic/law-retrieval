UNWIND [{_id:833, properties:{index:6, content:"phạt tiền từ 8.000.000 đồng đến 10.000.000 đồng đối với tổ chức quản lý, vận hành trạm thu phí đường bộ thực hiện một trong các hành vi vi phạm sau đây:"}}, {_id:836, properties:{index:7, content:"phạt tiền từ 10.000.000 đồng đến 20.000.000 đồng đối với tổ chức quản lý, vận hành trạm thu phí đường bộ thực hiện một trong các hành vi vi phạm sau đây:"}}, {_id:839, properties:{index:8, content:"phạt tiền từ 30.000.000 đồng đến 40.000.000 đồng đối với tổ chức quản lý, vận hành trạm thu phí đường bộ thực hiện một trong các hành vi vi phạm sau đây:"}}, {_id:842, properties:{index:9, content:"phạt tiền từ 50.000.000 đồng đến 70.000.000 đồng đối với tổ chức quản lý, vận hành trạm thu phí đường bộ thực hiện một trong các hành vi vi phạm sau đây:"}}, {_id:1995, properties:{index:1, content:"quy trình cân sơ cấp"}}, {_id:847, properties:{index:11, content:"vi phạm quy định về phương tiện tham gia giao thông đường bộ"}}, {_id:850, properties:{index:1, content:"phạt tiền từ 100.000 đồng đến 200.000 đồng đối với hành vi điều khiển xe không có kính chắn gió hoặc có nhưng vỡ hoặc có nhưng không có tác dụng (đối với xe có thiết kế lắp kính chắn gió)."}}, {_id:1998, properties:{index:2, content:"nếu không có dấu hiệu vi phạm, xe hòa nhập vào dòng xe chung, tiếp tục hành trình"}}, {_id:851, properties:{index:2, content:"phạt tiền từ 300.000 đồng đến 400.000 đồng đối với một trong các hành vi vi phạm sau đây:"}}, {_id:1999, properties:{index:3, content:"nếu xe có dấu hiệu vi phạm thì thực hiện quy trình cân thứ cấp."}}, {_id:2000, properties:{index:4, content:"quy trình cân thứ cấp"}}, {_id:855, properties:{index:3, content:"phạt tiền từ 800.000 đồng đến 1.000.000 đồng đối với một trong các hành vi vi phạm sau đây:"}}, {_id:2003, properties:{index:5, content:"người sử dụng thiết bị cân in và ký vào phiếu cân kiểm tra tải trọng xe, chuyển cho lực lượng chức năng để lập biên bản vi phạm hành chính; việc giao, nhận phiếu cân kiểm tra tải trọng xe được ghi chép vào sổ theo dõi và ký nhận."}}, {_id:2004, properties:{index:6, content:"lực lượng chức năng tiếp nhận phiếu cân kiểm tra tải trọng xe, thực hiện xử phạt vi phạm hành chính theo quy định."}}, {_id:2005, properties:{index:7, content:"sau khi lập biên bản vi phạm hành chính theo quy định, lực lượng chức năng yêu cầu lái xe hoặc chủ xe tự hạ phần hàng quá tải, tự dỡ phần hàng quá khổ; trường hợp xe vận chuyển hàng siêu trường, siêu trọng mà phương tiện vận chuyển không đáp ứng về tải trọng và khổ giới hạn theo quy định, lực lượng chức năng yêu cầu lái xe, chủ xe bố trí phương tiện đáp ứng về tải trọng và khổ giới hạn theo quy định."}}, {_id:2007, properties:{index:1, content:"quy trình cân và sử dụng kết quả cân kiểm tra tải trọng xe của hệ thống cân động ở tốc độ cao hoặc cân động ở tốc độ thấp"}}, {_id:2010, properties:{index:2, content:"trường hợp không có dấu hiệu vi phạm, xe hòa nhập vào dòng xe chung, tiếp tục hành trình"}}, {_id:2011, properties:{index:3, content:"trường hợp vi phạm, hệ thống báo hiệu tự động hoặc người sử dụng thiết bị cân chỉ dẫn xe vào vị trí xử lý;"}}, {_id:2013, properties:{index:4, content:"quy trình cân và sử dụng kết quả cân kiểm tra tải trọng xe của cân xách tay và cân kiểm tra tải trọng xe lưu động"}}, {_id:862, properties:{index:4, content:"phạt tiền từ 2.000.000 đồng đến 3.000.000 đồng đối với một trong các hành vi vi phạm sau đây:"}}] AS row
