UNWIND [{_id:1167, properties:{index:"c", content:"đơn vị cung cấp phần mềm ứng dụng hỗ trợ kết nối vận tải không thực hiện đúng quy định về cung cấp phần mềm ứng dụng hỗ trợ kết nối vận tải; không công bố quy trình giải quyết khiếu nại của khách hàng, không có hệ thống lưu trữ các khiếu nại của khách hàng theo quy định."}}, {_id:1170, properties:{index:"a", content:"thực hiện hành vi quy định tại điểm h khoản 2; điểm a, điểm b, điểm d, điểm đ, điểm e, điểm g, điểm h, điểm l, điểm o, điểm p, điểm q, điểm r, điểm s, điểm t khoản 4; điểm d, điểm đ, điểm e, điểm g, điểm i, điểm k, điểm l, điểm m, điểm n, điểm o, điểm p, điểm q khoản 6; điểm e, điểm i khoản 7 điều này bị tước quyền sử dụng phù hiệu (biển hiệu) từ 01 tháng đến 03 tháng (nếu có hoặc đã được cấp) đối với xe vi phạm;"}}, {_id:3209, properties:{index:"b", content:"buộc tháo dỡ công trình, phần công trình xây dựng không có giấy phép hoặc xây dựng không đúng với giấy phép;"}}, {_id:176, properties:{index:"i", content:"quay đầu xe trái quy định trong khu dân cư;"}}, {_id:1171, properties:{index:"b", content:"thực hiện hành vi quy định tại điểm i, điểm k khoản 4; điểm h khoản 6; điểm b, điểm h khoản 7 điều này bị tước quyền sử dụng giấy phép kinh doanh vận tải từ 01 tháng đến 03 tháng;"}}, {_id:180, properties:{index:"k", content:"quay đầu xe ở phần đường dành cho người đi bộ qua đường, trên cầu, đầu cầu, ngầm, gầm cầu vượt, trừ trường hợp tổ chức giao thông tại những khu vực này có bố trí nơi quay đầu xe."}}, {_id:1172, properties:{index:"c", content:"thực hiện hành vi quy định tại điểm d, điểm đ, điểm i, điểm m khoản 6 điều này trong trường hợp cá nhân kinh doanh vận tải là người trực tiếp điều khiển phương tiện còn bị tước quyền sử dụng giấy phép lái xe từ 01 tháng đến 03 tháng;"}}, {_id:3214, properties:{index:"c", content:"buộc thực hiện biện pháp để khắc phục tình trạng ô nhiễm môi trường do vi phạm hành chính gây ra;"}}, {_id:1173, properties:{index:"d", content:"thực hiện hành vi quy định tại điểm i khoản 7 điều này trong trường hợp cá nhân kinh doanh vận tải là người trực tiếp điều khiển phương tiện còn bị tước quyền sử dụng giấy phép lái xe từ 02 tháng đến 04 tháng;"}}, {_id:3215, properties:{index:"d", content:"buộc tái xuất phương tiện khỏi việt nam;"}}, {_id:1174, properties:{index:"đ", content:"thực hiện hành vi quy định tại điểm i khoản 6 điều này còn bị tịch thu phương tiện (trừ trường hợp xe ô tô từ 10 chỗ ngồi trở lên kinh doanh vận tải hành khách có niên hạn sử dụng vượt quá quy định về điều kiện kinh doanh của hình thức kinh doanh đã đăng ký nhưng chưa quá 20 năm tính từ năm sản xuất, xe ô tô dưới 10 chỗ ngồi kinh doanh vận tải hành khách)."}}, {_id:1176, properties:{index:"a", content:"thực hiện hành vi quy định tại điểm a, điểm b, điểm c, điểm d, điểm đ khoản 2; điểm b khoản 4 điều này buộc phải đăng ký, niêm yết đầy đủ, chính xác các thông tin theo quy định;"}}, {_id:1177, properties:{index:"b", content:"thực hiện hành vi quy định tại điểm d, điểm đ khoản 4 điều này buộc phải gắn hộp đèn với chữ “taxi” hoặc buộc phải niêm yết cụm từ “xe taxi”, “xe hợp đồng”, “xe du lịch” theo đúng quy định;"}}, {_id:185, properties:{index:"a", content:"điều khiển xe chạy quá tốc độ quy định từ 05 km/h đến dưới 10 km/h;"}}, {_id:1178, properties:{index:"c", content:"thực hiện hành vi quy định tại điểm e khoản 4 điều này buộc phải cấp “thẻ nhận dạng lái xe” cho lái xe theo quy định;"}}, {_id:1179, properties:{index:"d", content:"thực hiện hành vi quy định tại điểm g khoản 4, điểm đ khoản 7 điều này buộc phải tổ chức tập huấn nghiệp vụ hoặc tổ chức khám sức khỏe định kỳ cho lái xe và nhân viên phục vụ trên xe theo quy định;"}}, {_id:3220, properties:{index:"đ", content:"buộc nộp lại số lợi bất hợp pháp có được do thực hiện vi phạm hành chính;"}}, {_id:1180, properties:{index:"đ", content:"thực hiện hành vi quy định tại điểm h khoản 4 điều này buộc phải ký hợp đồng với lái xe và nhân viên phục vụ trên xe theo quy định;"}}, {_id:1181, properties:{index:"e", content:"thực hiện hành vi quy định tại điểm i khoản 4 điều này buộc phải xây dựng và thực hiện quy trình bảo đảm an toàn giao thông theo quy định;"}}, {_id:3222, properties:{index:"e", content:"các biện pháp khắc phục hậu quả khác được quy định tại khoản 2 và khoản 3 điều này."}}] AS row
