UNWIND [{_id:1444, properties:{index:"a", content:"không cung cấp hoặc cung cấp không đúng quy định nội dung thông tin hỗ trợ cảnh giới để thực hiện nhiệm vụ tại vị trí cảnh giới;"}}, {_id:1445, properties:{index:"b", content:"không cung cấp hoặc cung cấp không đầy đủ trang thiết bị phục vụ cảnh giới theo quy định."}}, {_id:1447, properties:{index:"a", content:"không có phương thức chỉ huy, liên lạc giữa nhà gác hai đầu cầu để điều khiển giao thông trên cầu bảo đảm thông suốt, an toàn theo quy định;"}}, {_id:1448, properties:{index:"b", content:"không tổ chức kết nối tín hiệu đường sắt, đường bộ tại khu vực cầu chung do doanh nghiệp quản lý theo quy định."}}, {_id:1450, properties:{index:"a", content:"không lắp đặt, lắp đặt không đúng, không đủ, không duy trì hoạt động bình thường hệ thống báo hiệu, tín hiệu, thiết bị tại đường ngang, cầu chung, không tổ chức thực hiện phòng vệ theo quy định;"}}, {_id:1451, properties:{index:"b", content:"không thông báo kịp thời, không phối hợp với lực lượng chức năng điều hành giao thông để bảo đảm an toàn giao thông qua đường ngang khi xảy ra sự cố hư hỏng đèn báo hiệu trên đường bộ tại đường ngang thuộc phạm vi quản lý;"}}, {_id:1452, properties:{index:"c", content:"không ban hành chế độ kiểm tra, bảo trì hệ thống kết nối tín hiệu thuộc phạm vi quản lý theo quy định;"}}, {_id:1453, properties:{index:"d", content:"không lập kế hoạch xây dựng, bảo trì công trình, thiết bị hệ thống kết nối tín hiệu thuộc phạm vi quản lý theo quy định."}}, {_id:1457, properties:{index:"a", content:"không thực hiện việc khám, sửa chữa đoàn tàu đi, đến hoặc thực hiện khám, sửa chữa đoàn tàu đi, đến không đúng, không đủ nội dung theo quy định;"}}, {_id:1458, properties:{index:"b", content:"tiến hành sửa chữa toa xe trên đường sắt trong ga khi chưa thực hiện biện pháp phòng vệ theo quy định;"}}, {_id:1459, properties:{index:"c", content:"để toa xe không bảo đảm quy chuẩn kỹ thuật quốc gia về an toàn kỹ thuật và bảo vệ môi trường nối vào đoàn tàu;"}}, {_id:1460, properties:{index:"d", content:"không phát hiện hoặc phát hiện nhưng không sửa chữa kịp thời các hư hỏng của toa xe gây chậm tàu."}}, {_id:1463, properties:{index:"a", content:"lập tàu có ghép nối toa xe không bảo đảm quy chuẩn kỹ thuật quốc gia về an toàn kỹ thuật và bảo vệ môi trường, trừ trường hợp di chuyển phương tiện chạy thử nghiệm, đưa phương tiện bị hư hỏng về cơ sở sửa chữa;"}}, {_id:1464, properties:{index:"b", content:"lập tàu có ghép nối toa xe vận tải động vật, hàng hóa có mùi hôi thối, chất dễ cháy, dễ nổ, độc hại, hàng nguy hiểm khác vào tàu khách."}}, {_id:1466, properties:{index:"a", content:"cho tàu chạy từ ga lập tàu hoặc các ga có quy định về thử hãm đoàn tàu mà không đủ áp lực hãm theo quy định;"}}, {_id:1467, properties:{index:"b", content:"cho tàu chạy mà không thử hãm theo quy định."}}, {_id:1469, properties:{index:"a", content:"doanh nghiệp kinh doanh vận tải đường sắt không ban hành nội dung, chức danh đảm nhiệm việc khám kỹ thuật theo quy định;"}}, {_id:1470, properties:{index:"b", content:"doanh nghiệp kinh doanh kết cấu hạ tầng đường sắt, chủ sở hữu đường sắt chuyên dùng không quy định địa điểm, không giám sát việc khám kỹ thuật của đoàn tàu theo quy định."}}, {_id:1475, properties:{index:"a", content:"cho đầu máy dịch chuyển khi chưa nhận được kế hoạch dồn hoặc tín hiệu của người chỉ huy dồn cho phép;"}}, {_id:1476, properties:{index:"b", content:"vượt quá tốc độ dồn cho phép;"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Point;
UNWIND [{_id:1477, properties:{index:"c", content:"dồn phóng, thả trôi từ dốc gù toa xe có ghi “cấm phóng”, toa xe khác theo quy định không được dồn phóng hoặc tại ga có quy định cấm dồn phóng;"}}, {_id:1478, properties:{index:"d", content:"dồn phóng vào đường có toa xe đang tác nghiệp kỹ thuật, đang sửa chữa, đang xếp, dỡ hàng; dồn phóng vào đường nhánh trong khu gian, vào đường chưa được chiếu sáng đầy đủ hoặc khi có sương mù, mưa to, gió lớn;"}}, {_id:1479, properties:{index:"đ", content:"để toa xe ngoài mốc tránh va chạm sau mỗi cú dồn, trừ các trường hợp đặc biệt theo quy định;"}}, {_id:1480, properties:{index:"e", content:"để đầu máy, toa xe đỗ trên đường an toàn, đường lánh nạn khi không có lệnh của người có thẩm quyền;"}}, {_id:1481, properties:{index:"g", content:"đặt chèn trên đường sắt tại các vị trí cấm đặt chèn;"}}, {_id:1482, properties:{index:"h", content:"tiến hành dồn khi các toa xe trong đoàn dồn chưa treo hàm nối ống mềm vào chỗ quy định;"}}, {_id:1483, properties:{index:"i", content:"để toa xe chưa dồn trên đường ga, đường nhánh trong khu gian, đường dùng riêng mà không nối liền với nhau, không siết chặt hãm tay ở hai đầu đoàn xe, không chèn chắc chắn."}}, {_id:1486, properties:{index:"a", content:"sau khi dồn xong vẫn để toa xe chở hàng nguy hiểm (chất nổ, chất cháy) chưa nối vào tàu nhưng không nối liền với nhau, không chèn chắc chắn, không để riêng trên một đường, không phòng vệ bằng tín hiệu di động “ngừng”;"}}, {_id:404, properties:{index:"a", content:"thực hiện hành vi quy định tại khoản 9 điều này hoặc tái phạm hành vi quy định tại điểm b khoản 7 điều này, bị tước quyền sử dụng giấy phép lái xe từ 03 tháng đến 05 tháng;"}}, {_id:1487, properties:{index:"b", content:"sau khi dồn xong vẫn để ghi dẫn vào đường chứa toa xe chở hàng nguy hiểm (chất nổ, chất cháy) không thông sang đường khác."}}, {_id:406, properties:{index:"b", content:"thực hiện hành vi quy định tại điểm a, điểm b khoản 8 điều này bị tước quyền sử dụng giấy phép lái xe từ 05 tháng đến 07 tháng;"}}, {_id:1489, properties:{index:"a", content:"thực hiện hành vi quy định tại điểm a khoản 3 điều này buộc phải để toa xe chở hàng nguy hiểm (chất nổ, chất cháy) theo đúng quy định về dồn tàu;"}}, {_id:1490, properties:{index:"b", content:"thực hiện hành vi quy định tại điểm b khoản 3 điều này buộc phải để ghi dẫn sang đường khác theo đúng quy định về dồn tàu."}}, {_id:409, properties:{index:"c", content:"thực hiện hành vi quy định tại điểm c khoản 6 điều này bị tước quyền sử dụng giấy phép lái xe từ 10 tháng đến 12 tháng;"}}, {_id:411, properties:{index:"d", content:"thực hiện hành vi quy định tại điểm c khoản 8 điều này bị tước quyền sử dụng giấy phép lái xe từ 16 tháng đến 18 tháng;"}}, {_id:1495, properties:{index:"a", content:"điều khiển tàu chạy lùi khi sương mù, mưa to, gió lớn mà không xác nhận được tín hiệu;"}}, {_id:413, properties:{index:"đ", content:"thực hiện hành vi quy định tại khoản 10 điều này bị tước quyền sử dụng giấy phép lái xe từ 22 tháng đến 24 tháng."}}, {_id:1496, properties:{index:"b", content:"điều khiển tàu chạy lùi khi thông tin bị gián đoạn mà phía sau tàu đó có tàu chạy cùng chiều;"}}, {_id:1497, properties:{index:"c", content:"điều khiển tàu chạy lùi trong khu gian đóng đường tự động khi chưa có lệnh;"}}, {_id:1498, properties:{index:"d", content:"điều khiển tàu chạy tiến hoặc lùi trong trường hợp đã xin cứu viện mà chưa được phép bằng mệnh lệnh;"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Point;
UNWIND [{_id:1499, properties:{index:"đ", content:"điều khiển tàu chạy lùi trong trường hợp tàu có đầu máy đẩy vào khu gian rồi trở về."}}, {_id:1501, properties:{index:"a", content:"trực ban chạy tàu ga, lái tàu, trưởng tàu cho tàu chạy vào khu gian mà chưa có chứng vật chạy tàu;"}}, {_id:1502, properties:{index:"b", content:"trực ban chạy tàu ga, nhân viên gác ghi để người không có phận sự thực hiện nhiệm vụ của mình khi không được phép."}}, {_id:425, properties:{index:"a", content:"không chấp hành hiệu lệnh, chỉ dẫn của biển báo hiệu, vạch kẻ đường, trừ các hành vi vi phạm quy định tại điểm c, điểm đ, điểm e, điểm h khoản 2; điểm d, điểm g, điểm i, điểm m khoản 3; điểm a, điểm b, điểm c, điểm d, điểm e khoản 4; khoản 5; điểm b khoản 6; điểm a, điểm b khoản 7; điểm d khoản 8 điều này;"}}, {_id:1506, properties:{index:"a", content:"trực ban chạy tàu ga, gác ghi, gác đường ngang, gác cầu chung, gác hầm, tuần cầu, tuần đường, tuần hầm không đón, tiễn tàu hoặc tác nghiệp đón, tiễn tàu không đúng quy định trừ các hành vi vi phạm quy định tại khoản 2, khoản 3, khoản 4 điều này;"}}, {_id:1507, properties:{index:"b", content:"trực ban chạy tàu ga không ghi chép đầy đủ các mẫu điện tín;"}}, {_id:1508, properties:{index:"c", content:"trực ban chạy tàu ga, gác đường ngang, gác cầu chung không ghi chép đầy đủ thông tin về giờ tàu chạy qua ga, chắn, cầu chung theo quy định."}}, {_id:429, properties:{index:"b", content:"không có báo hiệu xin vượt trước khi vượt;"}}, {_id:1512, properties:{index:"a", content:"tổ chức đón tàu vào đường không thanh thoát mà không áp dụng các biện pháp an toàn theo quy định;"}}, {_id:1513, properties:{index:"b", content:"đón, gửi nhầm tàu;"}}, {_id:432, properties:{index:"c", content:"không giữ khoảng cách an toàn để xảy ra va chạm với xe chạy liền trước hoặc không giữ khoảng cách theo quy định của biển báo hiệu “cự ly tối thiểu giữa hai xe”;"}}, {_id:1514, properties:{index:"c", content:"đón, gửi tàu mà không thu chìa khóa ghi hoặc không áp dụng các biện pháp khống chế ghi;"}}, {_id:1515, properties:{index:"d", content:"cho tàu chạy vào khu gian mà không thông báo cho nhân viên gác đường ngang, cầu chung theo quy định;"}}, {_id:1516, properties:{index:"đ", content:"để phương tiện giao thông đường sắt khác chiếm dụng đường chính tuyến trong ga, trừ trường hợp bất khả kháng (tránh vượt tàu, dồn dịch, cứu hộ, cứu nạn);"}}, {_id:435, properties:{index:"d", content:"chuyển hướng không nhường quyền đi trước cho: người đi bộ, xe lăn của người khuyết tật qua đường tại nơi có vạch kẻ đường dành cho người đi bộ; xe thô sơ đang đi trên phần đường dành cho xe thô sơ;"}}, {_id:1517, properties:{index:"e", content:"không thông báo cho trực ban chạy tàu ga đến, ga đi, nhân viên điều độ chạy tàu tuyến về số hiệu tàu, giờ thực tế tàu đến, đi, thông qua sau khi tàu đến, đi, thông qua ga theo quy định;"}}, {_id:1518, properties:{index:"g", content:"không kiểm tra, không xác nhận việc dồn dịch gây ảnh hưởng, trở ngại đến đường đón, gửi tàu theo quy định."}}, {_id:1521, properties:{index:"a", content:"đã xác nhận được các tín hiệu dồn nhưng không kéo còi làm tín hiệu hô đáp;"}}, {_id:1522, properties:{index:"b", content:"khi dồn tàu không thực hiện đúng các tín hiệu dồn theo quy định."}}, {_id:441, properties:{index:"đ", content:"chuyển hướng không nhường đường cho: các xe đi ngược chiều; người đi bộ, xe lăn của người khuyết tật đang qua đường tại nơi không có vạch kẻ đường cho người đi bộ;"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Point;
UNWIND [{_id:1525, properties:{index:"a", content:"lái tàu, trưởng tàu cho tàu chạy khi chưa nhận được tín hiệu an toàn của trực ban chạy tàu ga hoặc người làm tín hiệu truyền;"}}, {_id:1526, properties:{index:"b", content:"lái tàu điều khiển tàu chạy vượt qua tín hiệu vào ga, ra ga đang ở trạng thái đóng khi chưa được phép của người chỉ huy chạy tàu ở ga;"}}, {_id:1527, properties:{index:"c", content:"lái tàu không dừng tàu khi tàu đã đè lên pháo phòng vệ và pháo phòng vệ đã nổ bình thường;"}}, {_id:444, properties:{index:"e", content:"lùi xe mô tô ba bánh không quan sát hoặc không có tín hiệu báo trước;"}}, {_id:1528, properties:{index:"d", content:"lái tàu tiếp tục cho tàu chạy khi đã nhận được tín hiệu ngừng tàu."}}, {_id:446, properties:{index:"g", content:"chở người ngồi trên xe sử dụng ô (dù);"}}, {_id:1533, properties:{index:"a", content:"không công bố hoặc công bố không đầy đủ nội dung biểu đồ chạy tàu trên phương tiện thông tin đại chúng, trên trang thông tin điện tử của doanh nghiệp theo quy định;"}}, {_id:1534, properties:{index:"b", content:"không công bố công khai công lệnh tải trọng, công lệnh tốc độ trên trang thông tin điện tử của doanh nghiệp theo quy định."}}, {_id:448, properties:{index:"h", content:"không tuân thủ các quy định về nhường đường tại nơi đường giao nhau, trừ các hành vi vi phạm quy định tại điểm b, điểm e khoản 2 điều này;"}}, {_id:1539, properties:{index:"a", content:"phát mệnh lệnh liên quan đến chạy tàu không đúng thẩm quyền;"}}, {_id:1540, properties:{index:"b", content:"không phát lệnh cho trực ban chạy tàu ga cấp cảnh báo kịp thời cho lái tàu;"}}, {_id:1541, properties:{index:"c", content:"không phát lệnh phong tỏa khu gian theo quy định để: tổ chức thi công, sửa chữa kết cấu hạ tầng đường sắt; tổ chức chạy tàu cứu viện, tàu công trình vào khu gian cần phải phong tỏa;"}}, {_id:1542, properties:{index:"d", content:"không kịp thời phát các mệnh lệnh thuộc thẩm quyền quy định gây chậm tàu, ách tắc giao thông."}}, {_id:1554, properties:{index:"a", content:"điều khiển phương tiện giao thông cơ giới đường bộ làm hỏng cần chắn, giàn chắn, các thiết bị khác tại đường ngang, cầu chung;"}}, {_id:1555, properties:{index:"b", content:"điều khiển xe bánh xích, xe lu bánh sắt, các phương tiện vận chuyển hàng siêu trường, siêu trọng, quá khổ giới hạn đi qua đường ngang mà không thông báo cho tổ chức quản lý đường ngang, không thực hiện đúng các biện pháp bảo đảm an toàn."}}, {_id:455, properties:{index:"i", content:"chuyển làn đường không đúng nơi được phép hoặc không có tín hiệu báo trước;"}}, {_id:1563, properties:{index:"a", content:"không cung cấp hoặc cung cấp không đủ các tài liệu, vật chứng liên quan đến tai nạn giao thông đường sắt; không chuyển giao hồ sơ tai nạn ban đầu theo quy định;"}}, {_id:1564, properties:{index:"b", content:"không thực hiện nghĩa vụ cứu nạn khi có điều kiện cứu nạn;"}}, {_id:1565, properties:{index:"c", content:"khi nhận được tin báo về tai nạn giao thông đường sắt hoặc khi được yêu cầu phối hợp, hỗ trợ không đến ngay hiện trường để giải quyết;"}}, {_id:1566, properties:{index:"d", content:"không kịp thời có biện pháp xử lý, biện pháp ngăn ngừa tai nạn giao thông đường sắt khi phát hiện hoặc nhận được tin báo công trình đường sắt bị hư hỏng;"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Point;
UNWIND [{_id:1567, properties:{index:"đ", content:"không kịp thời sửa chữa, khắc phục sự cố làm ảnh hưởng đến việc chạy tàu;"}}, {_id:1568, properties:{index:"e", content:"gây trở ngại cho việc khôi phục đường sắt sau khi xảy ra tai nạn giao thông đường sắt."}}, {_id:1570, properties:{index:"a", content:"thay đổi, xóa dấu vết hiện trường vụ tai nạn giao thông đường sắt;"}}, {_id:1571, properties:{index:"b", content:"lợi dụng tai nạn giao thông đường sắt để xâm phạm tài sản, phương tiện bị nạn; làm mất trật tự, cản trở việc xử lý tai nạn giao thông đường sắt;"}}, {_id:1572, properties:{index:"c", content:"gây tai nạn giao thông đường sắt mà không đến trình báo với cơ quan có thẩm quyền;"}}, {_id:1573, properties:{index:"d", content:"không phối hợp, không chấp hành mệnh lệnh của người, cơ quan có thẩm quyền trong việc khắc phục hậu quả, khôi phục giao thông đường sắt."}}, {_id:1575, properties:{index:"a", content:"không có hội đồng giải quyết tai nạn giao thông đường sắt hoặc hội đồng phân tích tai nạn giao thông đường sắt khi có tai nạn giao thông đường sắt xảy ra theo quy định;"}}, {_id:457, properties:{index:"k", content:"điều khiển xe chạy dàn hàng ngang từ 03 xe trở lên;"}}, {_id:1576, properties:{index:"b", content:"không lưu trữ hồ sơ các vụ sự cố, tai nạn giao thông đường sắt, không thường xuyên cập nhật số liệu về số vụ sự cố, tai nạn giao thông đường sắt theo quy định;"}}, {_id:1577, properties:{index:"c", content:"không quy định trách nhiệm của các đơn vị tham gia hoạt động trên đường sắt quốc gia trong việc giải quyết các vụ sự cố, tai nạn giao thông đường sắt, không công bố công khai địa chỉ, số điện thoại của các tổ chức, cá nhân có liên quan để phục vụ công tác xử lý sự cố, tai nạn giao thông đường sắt theo quy định."}}, {_id:1579, properties:{index:"a", content:"không xây dựng, thực hiện phương án phòng, chống và khắc phục kịp thời hậu quả do sự cố, thiên tai, tai nạn giao thông đường sắt để bảo đảm giao thông đường sắt an toàn, thông suốt;"}}, {_id:1580, properties:{index:"b", content:"không tuân thủ sự chỉ đạo, điều phối lực lượng của tổ chức phòng, chống sự cố, thiên tai, xử lý tai nạn giao thông đường sắt;"}}, {_id:461, properties:{index:"l", content:"không sử dụng đèn chiếu sáng trong thời gian từ 19 giờ ngày hôm trước đến 05 giờ ngày hôm sau hoặc khi sương mù, thời tiết xấu hạn chế tầm nhìn;"}}, {_id:1581, properties:{index:"c", content:"không thông báo kịp thời sự cố đe dọa an toàn chạy tàu và việc tạm đình chỉ chạy tàu cho trực ban chạy tàu ga ở hai đầu khu gian nơi xảy ra sự cố hoặc nhân viên điều hành giao thông vận tải đường sắt;"}}, {_id:1582, properties:{index:"d", content:"không đình chỉ chạy tàu khi thấy kết cấu hạ tầng đường sắt có nguy cơ gây mất an toàn chạy tàu;"}}, {_id:1583, properties:{index:"đ", content:"không thành lập tổ ứng phó cứu viện để giải quyết sự cố, tai nạn giao thông đường sắt."}}, {_id:1586, properties:{index:"a", content:"đi, đứng, nằm, ngồi hoặc hành vi khác trên đường sắt, trong cầu, hầm dành riêng cho đường sắt, trừ nhân viên đường sắt, lực lượng chức năng đang làm nhiệm vụ;"}}, {_id:1587, properties:{index:"b", content:"vượt tường rào, hàng rào ngăn cách giữa đường sắt với khu vực xung quanh;"}}, {_id:466, properties:{index:"m", content:"tránh xe không đúng quy định; sử dụng đèn chiếu xa khi tránh xe đi ngược chiều; không nhường đường cho xe đi ngược chiều theo quy định tại nơi đường hẹp, đường dốc, nơi có chướng ngại vật;"}}, {_id:1588, properties:{index:"c", content:"để súc vật đi qua đường sắt không đúng quy định hoặc để súc vật kéo xe qua đường sắt mà không có người điều khiển;"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Point;
UNWIND [{_id:1589, properties:{index:"d", content:"đi, đứng, nằm, ngồi hoặc hành vi khác trên nóc toa xe, đầu máy, bậc lên xuống toa xe; đu bám, đứng, ngồi hai bên thành toa xe, đầu máy, nơi nối giữa các toa xe, đầu máy; mở cửa lên, xuống tàu, đưa đầu, tay, chân và vật khác ra ngoài thành toa xe khi tàu đang chạy, trừ nhân viên đường sắt, lực lượng chức năng đang thi hành nhiệm vụ;"}}, {_id:1590, properties:{index:"đ", content:"phơi rơm, rạ, nông sản, để các vật phẩm khác trên đường sắt hoặc các công trình đường sắt khác;"}}, {_id:469, properties:{index:"n", content:"bấm còi trong thời gian từ 22 giờ ngày hôm trước đến 05 giờ ngày hôm sau, sử dụng đèn chiếu xa trong đô thị, khu đông dân cư, trừ các xe ưu tiên đang đi làm nhiệm vụ theo quy định;"}}, {_id:1591, properties:{index:"e", content:"để rơi vãi đất, cát, các loại vật tư, vật liệu khác lên đường sắt."}}, {_id:1593, properties:{index:"a", content:"neo đậu phương tiện vận tải thủy, bè, mảng hoặc các vật thể khác trong phạm vi bảo vệ cầu đường sắt;"}}, {_id:472, properties:{index:"o", content:"xe được quyền ưu tiên lắp đặt, sử dụng thiết bị phát tín hiệu ưu tiên không đúng quy định hoặc sử dụng thiết bị phát tín hiệu ưu tiên mà không có giấy phép của cơ quan có thẩm quyền cấp hoặc có giấy phép của cơ quan có thẩm quyền cấp nhưng không còn giá trị sử dụng theo quy định;"}}, {_id:1594, properties:{index:"b", content:"để phương tiện giao thông đường bộ, thiết bị, vật liệu, hàng hóa vi phạm khổ giới hạn tiếp giáp kiến trúc đường sắt, trừ hành vi vi phạm quy định tại điểm b khoản 3 điều này."}}, {_id:475, properties:{index:"p", content:"quay đầu xe tại nơi không được quay đầu xe, trừ hành vi vi phạm quy định tại điểm d khoản 4 điều này;"}}, {_id:1596, properties:{index:"a", content:"ngăn cản việc chạy tàu, tùy tiện báo hiệu hoặc sử dụng các thiết bị để dừng tàu, trừ trường hợp phát hiện có sự cố gây mất an toàn giao thông đường sắt;"}}, {_id:1597, properties:{index:"b", content:"để vật chướng ngại lên đường sắt làm cản trở giao thông đường sắt;"}}, {_id:1598, properties:{index:"c", content:"tự ý mở chắn đường ngang khi chắn đã đóng."}}, {_id:479, properties:{index:"q", content:"điều khiển xe chạy dưới tốc độ tối thiểu trên những đoạn đường bộ có quy định tốc độ tối thiểu cho phép."}}, {_id:1600, properties:{index:"a", content:"không thực hiện biện pháp đảm bảo an toàn công trình đường sắt, an toàn giao thông vận tải đường sắt khi công trình thiết bị điện, viễn thông nằm trong phạm vi bảo vệ công trình đường sắt, hành lang an toàn giao thông đường sắt;"}}, {_id:1601, properties:{index:"b", content:"không thực hiện biện pháp bảo đảm an toàn để công trình điện lực, đường dây tải điện gây nhiễu hệ thống thông tin, tín hiệu đường sắt;"}}, {_id:482, properties:{index:"a", content:"dừng xe, đỗ xe trên phần đường xe chạy ở đoạn đường ngoài đô thị nơi có lề đường;"}}, {_id:1602, properties:{index:"c", content:"không thực hiện biện pháp bảo đảm an toàn cho thiết bị, công trình đường sắt, giao thông vận tải đường sắt khi dây tải điện bị sự cố, đứt trong phạm vi bảo vệ công trình đường sắt, hành lang an toàn giao thông đường sắt."}}, {_id:1606, properties:{index:"a", content:"không có giải pháp kỹ thuật đảm bảo an toàn giao thông vận tải đường sắt trong quá trình khai thác, kinh doanh kết cấu hạ tầng đường sắt đối với tuyến đường được phép sử dụng độ dốc lớn hơn dốc hạn chế;"}}, {_id:487, properties:{index:"b", content:"không giảm tốc độ và nhường đường khi điều khiển xe chạy từ trong ngõ, đường nhánh ra đường chính;"}}, {_id:1607, properties:{index:"b", content:"không có biện pháp đảm bảo an toàn cho hành khách lên xuống tàu ở những ga có ke ga đang sử dụng chưa nâng cấp, cải tạo."}}, {_id:1609, properties:{index:"a", content:"thực hiện hành vi quy định tại điểm a khoản 1 điều này buộc phải ra khỏi đường sắt, cầu, hầm dành riêng cho đường sắt;"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Point;
UNWIND [{_id:1610, properties:{index:"b", content:"thực hiện hành vi quy định tại điểm đ khoản 1 điều này buộc phải đưa rơm, rạ, nông sản, các vật phẩm khác ra khỏi đường sắt hoặc các công trình đường sắt khác;"}}, {_id:490, properties:{index:"c", content:"điều khiển xe chạy quá tốc độ quy định từ 05 km/h đến dưới 10 km/h;"}}, {_id:1611, properties:{index:"c", content:"thực hiện hành vi quy định tại điểm e khoản 1, điểm b khoản 3 điều này buộc phải đưa đất, cát, vật chướng ngại, các loại vật tư, vật liệu khác ra khỏi đường sắt;"}}, {_id:1612, properties:{index:"d", content:"thực hiện hành vi quy định tại điểm a khoản 2 điều này buộc phải đưa bè, mảng, phương tiện vận tải thủy hoặc các vật thể khác ra khỏi phạm vi bảo vệ cầu đường sắt;"}}, {_id:1613, properties:{index:"đ", content:"thực hiện hành vi quy định tại điểm b khoản 2 điều này buộc phải đưa phương tiện giao thông đường bộ, thiết bị, vật liệu, hàng hóa ra khỏi khổ giới hạn tiếp giáp kiến trúc đường sắt;"}}, {_id:1614, properties:{index:"e", content:"thực hiện hành vi quy định tại khoản 4 điều này buộc phải thực hiện các biện pháp bảo đảm an toàn công trình đường sắt, an toàn giao thông đường sắt."}}, {_id:493, properties:{index:"d", content:"điều khiển xe chạy tốc độ thấp mà không đi bên phải phần đường xe chạy gây cản trở giao thông;"}}, {_id:1617, properties:{index:"a", content:"đưa vào vận hành, khai thác đường sắt đô thị không có giấy chứng nhận thẩm định hồ sơ an toàn hệ thống đường sắt đô thị theo quy định;"}}, {_id:1618, properties:{index:"b", content:"đưa vào vận hành, khai thác đường sắt đô thị không có giấy chứng nhận định kỳ hệ thống quản lý an toàn vận hành đường sắt đô thị do cơ quan có thẩm quyền cấp hoặc có nhưng hết hiệu lực."}}, {_id:1623, properties:{index:"a", content:"đổ, để chất độc hại, chất phế thải lên đường sắt hoặc xả chất thải không bảo đảm vệ sinh môi trường lên đường sắt;"}}, {_id:1624, properties:{index:"b", content:"đổ trái phép đất, đá, vật liệu khác lên đường sắt hoặc để rơi đất, đá, vật liệu khác từ trên tàu xuống đường sắt trong quá trình vận chuyển;"}}, {_id:1625, properties:{index:"c", content:"để chất dễ cháy, chất dễ nổ trong phạm vi bảo vệ công trình đường sắt, hành lang an toàn giao thông đường sắt;"}}, {_id:1626, properties:{index:"d", content:"làm che lấp tín hiệu giao thông đường sắt;"}}, {_id:1627, properties:{index:"đ", content:"làm hư hỏng hoặc làm mất tác dụng của hệ thống thoát nước công trình đường sắt;"}}, {_id:1628, properties:{index:"e", content:"đặt tấm đan bê tông, gỗ, sắt thép, các vật liệu khác trái phép trong lòng đường sắt hoặc trong phạm vi bảo vệ công trình đường sắt;"}}, {_id:1629, properties:{index:"g", content:"bơm, xả nước hoặc các chất lỏng khác làm ngập nền đường sắt, ảnh hưởng đến khả năng thoát nước của hệ thống thoát nước đường sắt hoặc ảnh hưởng đến chất lượng, an toàn công trình đường sắt."}}, {_id:1631, properties:{index:"a", content:"đào, lấy, san, lấp đất, đá hoặc các vật liệu khác trái phép trong phạm vi bảo vệ công trình đường sắt, hành lang an toàn giao thông đường sắt;"}}, {_id:1632, properties:{index:"b", content:"làm hỏng, tháo dỡ trái phép tường rào, hàng rào ngăn cách giữa đường sắt với khu vực xung quanh; làm sai lệch vị trí hoặc phá, dỡ trái phép mốc chỉ giới ga đường sắt, mốc giới đất dành cho đường sắt, hàng rào dùng để đóng lối đi tự mở, cọc dùng để thu hẹp lối đi tự mở;"}}, {_id:1633, properties:{index:"c", content:"làm hỏng, thay đổi, chuyển dịch biển hiệu, mốc hiệu, tín hiệu của công trình đường sắt."}}, {_id:498, properties:{index:"đ", content:"dừng xe, đỗ xe ở lòng đường đô thị gây cản trở giao thông; tụ tập từ 03 xe trở lên ở lòng đường, trong hầm đường bộ; đỗ, để xe ở lòng đường đô thị, hè phố trái quy định của pháp luật;"}}] AS row
