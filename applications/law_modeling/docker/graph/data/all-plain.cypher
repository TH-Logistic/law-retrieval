UNWIND [{_id:2384, properties:{index:"a", content:"chỉ huy, điều khiển giao thông trên đường; hướng dẫn, bắt buộc người tham gia giao thông chấp hành quy tắc giao thông;"}}, {_id:2385, properties:{index:"b", content:"khi có tình huống gây ách tắc giao thông hoặc có yêu cầu cần thiết khác về bảo đảm an ninh, trật tự được tạm thời đình chỉ đi lại ở một số đoạn đường nhất định, phân lại luồng, phân lại tuyến và nơi tạm dừng xe, đỗ xe."}}, {_id:2388, properties:{index:"a", content:"dừng ngay phương tiện; giữ nguyên hiện trường; cấp cứu người bị nạn và phải có mặt khi cơ quan có thẩm quyền yêu cầu;"}}, {_id:2389, properties:{index:"b", content:"ở lại nơi xảy ra tai nạn cho đến khi người của cơ quan công an đến, trừ trường hợp người điều khiển phương tiện cũng bị thương phải đưa đi cấp cứu hoặc phải đưa người bị nạn đi cấp cứu hoặc vì lý do bị đe dọa đến tính mạng, nhưng phải đến trình báo ngay với cơ quan công an nơi gần nhất;"}}, {_id:2390, properties:{index:"c", content:"cung cấp thông tin xác thực về vụ tai nạn cho cơ quan có thẩm quyền."}}, {_id:2392, properties:{index:"a", content:"bảo vệ hiện trường;"}}, {_id:2393, properties:{index:"b", content:"giúp đỡ, cứu chữa kịp thời người bị nạn;"}}, {_id:2394, properties:{index:"c", content:"báo tin ngay cho cơ quan công an, y tế hoặc ủy ban nhân dân nơi gần nhất;"}}, {_id:2395, properties:{index:"d", content:"bảo vệ tài sản của người bị nạn;"}}, {_id:2396, properties:{index:"đ", content:"cung cấp thông tin xác thực về vụ tai nạn theo yêu cầu của cơ quan có thẩm quyền."}}, {_id:2404, properties:{index:"a", content:"quốc lộ là đường nối liền thủ đô hà nội với trung tâm hành chính cấp tỉnh; đường nối liền trung tâm hành chính cấp tỉnh từ ba địa phương trở lên; đường nối liền từ cảng biển quốc tế, cảng hàng không quốc tế đến các cửa khẩu quốc tế, cửa khẩu chính trên đường bộ; đường có vị trí đặc biệt quan trọng đối với sự phát triển kinh tế - xã hội của vùng, khu vực;"}}, {_id:2405, properties:{index:"b", content:"đường tỉnh là đường nối trung tâm hành chính của tỉnh với trung tâm hành chính của huyện hoặc trung tâm hành chính của tỉnh lân cận; đường có vị trí quan trọng đối với sự phát triển kinh tế - xã hội của tỉnh;"}}, {_id:2406, properties:{index:"c", content:"đường huyện là đường nối trung tâm hành chính của huyện với trung tâm hành chính của xã, cụm xã hoặc trung tâm hành chính của huyện lân cận; đường có vị trí quan trọng đối với sự phát triển kinh tế - xã hội của huyện;"}}, {_id:2407, properties:{index:"d", content:"đường xã là đường nối trung tâm hành chính của xã với các thôn, làng, ấp, bản và đơn vị tương đương hoặc đường nối với các xã lân cận; đường có vị trí quan trọng đối với sự phát triển kinh tế - xã hội của xã;"}}, {_id:2408, properties:{index:"đ", content:"đường đô thị là đường trong phạm vi địa giới hành chính nội thành, nội thị;"}}, {_id:2409, properties:{index:"e", content:"đường chuyên dùng là đường chuyên phục vụ cho việc vận chuyển, đi lại của một hoặc một số cơ quan, tổ chức, cá nhân."}}, {_id:2411, properties:{index:"a", content:"hệ thống quốc lộ do bộ trưởng bộ giao thông vận tải quyết định;"}}, {_id:2412, properties:{index:"b", content:"hệ thống đường tỉnh, đường đô thị do chủ tịch ủy ban nhân dân cấp tỉnh quyết định sau khi thỏa thuận với bộ giao thông vận tải (đối với đường tỉnh) và thỏa thuận với bộ giao thông vận tải và bộ xây dựng (đối với đường đô thị);"}}, {_id:2413, properties:{index:"c", content:"hệ thống đường huyện, đường xã do chủ tịch ủy ban nhân dân cấp huyện quyết định sau khi được chủ tịch ủy ban nhân dân cấp tỉnh đồng ý;"}}, {_id:2414, properties:{index:"d", content:"hệ thống đường chuyên dùng do cơ quan, tổ chức, cá nhân có đường chuyên dùng quyết định sau khi có ý kiến chấp thuận bằng văn bản của bộ trưởng bộ giao thông vận tải đối với đường chuyên dùng đấu nối vào quốc lộ; ý kiến chấp thuận bằng văn bản của chủ tịch ủy ban nhân dân cấp tỉnh đối với đường chuyên dùng đấu nối vào đường tỉnh, đường đô thị, đường huyện; ý kiến chấp thuận bằng văn bản của chủ tịch ủy ban nhân dân cấp huyện đối với đường chuyên dùng đấu nối vào đường xã."}}] AS row
