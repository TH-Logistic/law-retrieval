}, {_id:2234, properties:{index:"b", content:"trên các đoạn đường cong và gần đầu dốc tầm nhìn bị che khuất;"}}, {_id:2235, properties:{index:"c", content:"trên cầu, gầm cầu vượt;"}}, {_id:2236, properties:{index:"d", content:"song song với một xe khác đang dừng, đỗ;"}}, {_id:2237, properties:{index:"đ", content:"trên phần đường dành cho người đi bộ qua đường;"}}, {_id:2238, properties:{index:"e", content:"nơi đường giao nhau và trong phạm vi 5 mét tính từ mép đường giao nhau;"}}, {_id:2239, properties:{index:"g", content:"nơi dừng của xe buýt;"}}] AS row
