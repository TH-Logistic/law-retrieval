UNWIND [{_id:3077, properties:{no:50, content:"Việc người điều khiển xe máy, xe đạp sử dụng hoặc chờ người sử dụng ô (dù) khi tham gia giao thông tiềm ẩn nhiều nguy cơ gây tai nạn giao thông nhất là trong thời tiết mưa gió. Vậy Nghị định số 100/2019/NĐ-CP quy định xử phạt đối với người điều khiển xe máy, xe đạp sử dụng ô dù khi tham gia giao thông như thế nào?"}}, {_id:3080, properties:{no:51, content:"Hành vi đi xe gắn máy, xe đạp dàn hàng ngang 2 xe có vi phạm luật giao thông đường bộ không? Nếu bị phát hiện xử phạt như thế nào?"}}, {_id:3083, properties:{no:52, content:"Việc không giúp đỡ người bị tai nạn giao thông có bị xử phạt hay không?"}}, {_id:5, properties:{no:85, content:"Người đang điều khiển xe hoặc chở người ngồi trên xe bám, kéo, đẩy xe khác, vật khác, dẫn dắt súc vật, mang vác vật cồng kềnh; chở người đứng trên yên, giá đèo hàng hoặc ngồi trên tay lái; xếp hàng hóa trên xe vượt quá giới hạn quy định; điều khiển xe kéo theo xe khác, vật khác bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:3086, properties:{no:53, content:"Hành vi đi, đứng, nằm, ngồi trên đường sắt có bị xử lý không?"}}, {_id:3094, properties:{no:54, content:"Tuyến đường sắt đi qua tỉnh Nam Định có nhiều đoạn chạy qua các khu có đông dân cư sinh sống. Theo thống kê cho thấy, ngoài đường ngang hợp pháp, có người gác chắn hoặc đường ngang phòng vệ bằng biển báo thì đã hình thành hàng trăm lối đi dân tự mở. Vậy hành vi tự mở lối đi qua đường sắt bị xử lý như thế nào?"}}, {_id:3104, properties:{no:55, content:"Các hành vi vi phạm quy định về quy tắc giao thông tại đường ngang, cầu chung, hầm đường sắt bị xử lý như thế nào?"}}, {_id:22, properties:{no:86, content:"Hành vi không chấp hành hiệu lệnh của đèn tín hiệu giao thông và không chấp hành hiệu lệnh, hướng dẫn của người điều khiển giao thông hoặc người kiểm soát giao thông bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:3110, properties:{no:56, content:"Hành vi ném đất, đá hoặc vật khác lên tàu hoặc từ trên tàu xuống bị xử lý như thế nào?"}}, {_id:25, properties:{no:87, content:"Người đang điều khiển xe sử dụng ô (dù), điện thoại di động, thiết bị âm thanh, trừ thiết bị trợ thính bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:3118, properties:{no:57, content:"Hành vi sử dụng vé tàu giả và bán vé tàu trái quy định bị xử lý như thế nào?"}}, {_id:32, properties:{no:88, content:"Hành vi điều khiển xe trên đường mà trong máu hoặc hơi thở có nồng độ cồn nhưng chưa vượt quá 50 miligam/100 mililít máu hoặc chưa vượt quá 0,25 miligam/1 lít khí thở bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:35, properties:{no:89, content:"Hành vi điều khiển xe lạng lách hoặc đánh võng trên đường bộ trong, ngoài đô thị bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:3123, properties:{no:58, content:"Hành vi điều khiển tàu chạy quá tốc độ quy định bị xử lý như thế nào?"}}, {_id:41, properties:{no:90, content:"Hành vi gây tai nạn giao thông không dừng lại, không giữ nguyên hiện trường, bỏ trốn không đến trình báo với cơ quan có thẩm quyền, không tham gia cấp cứu người bị nạn bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:3128, properties:{no:59, content:"Luật Giao thông đường bộ quy định quy tắc chung giao thông đường bộ là gì?"}}, {_id:3131, properties:{no:60, content:"Luật Giao thông đường bộ quy định các hành vi nào bị nghiêm cấm?"}}, {_id:3133, properties:{no:61, content:"Luật Giao thông đường bộ quy định chấp hành báo hiệu đường bộ như thế nào?"}}, {_id:3136, properties:{no:62, content:"Luật Giao thông đường bộ quy định sử dụng làn đường như thế nào?"}}, {_id:3139, properties:{no:63, content:"Luật Giao thông đường bộ quy định người điều khiển phương tiện khi dừng xe, đỗ xe trên đường bộ phải thực hiện quy định nào?"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Question;
UNWIND [{_id:3144, properties:{no:64, content:"Luật Giao thông đường bộ quy định người điều khiển phương tiện không được dừng xe, đỗ xe tại các vị trí nào?"}}, {_id:52, properties:{no:91, content:"Hành vi điều khiển xe trên đường mà trong máu hoặc hơi thở có nồng độ cồn vượt quá 80 miligam/100 mililít máu hoặc vượt quá 0,4 miligam/1 lít khí thở bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:55, properties:{no:92, content:"Hành vi không đi bên phải theo chiều đi của mình, đi không đúng phần đường quy định; Dừng xe đột ngột; chuyển hướng không báo hiệu trước bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:62, properties:{no:93, content:"Hành vi dùng xe đẩy làm quầy hàng lưu động trên đường, gây cản trở giao thông bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:3149, properties:{no:65, content:"Theo Luật Giao thông đường bộ, người điều khiển xe mô tô hai bánh, xe gắn máy được chở mấy người?"}}, {_id:69, properties:{no:94, content:"Hành vi không chấp hành hiệu lệnh, chỉ dẫn của người điều khiển giao thông hoặc người kiểm soát giao thông; Không chấp hành hiệu lệnh của đèn tín hiệu giao thông bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:74, properties:{no:95, content:"Hành vi điều khiển xe trên đường mà trong máu hoặc hơi thở có nồng độ cồn vượt quá 50 miligam đến 80 miligam/100 mililít máu hoặc vượt quá 0,25 miligam đến 0,4 miligam/1 lít khí thở bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:76, properties:{no:96, content:"Hành vi điều khiển xe trên đường mà trong máu hoặc hơi thở có nồng độ cồn vượt quá 80 miligam/100 mililít máu hoặc vượt quá 0,4 miligam/1 lít khí thở và không chấp hành yêu cầu kiểm tra về nồng độ cồn của người thi hành công vụ bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:79, properties:{no:97, content:"Việc vượt đèn đỏ với từng loại phương tiện sẽ bị phạt như thế nào? "}}, {_id:84, properties:{no:98, content:"Người điểu khiển phương tiện khi lùi xe cần phải tuân thủ những quy định nào?"}}, {_id:87, properties:{no:99, content:"Luật giao thông đường bộ năm 2008 quy định như thế nào về tín hiệu đèn giao thông? Mức xử phạt vi phạm hành chính đối với người điều khiển phương tiện không chấp hành tín hiệu đèn giao thông được quy định như thế nào?"}}, {_id:93, properties:{no:100, content:"Chủ xe mô tô, ô tô tự ý đổi màu sơn xe bị xử phạt như thế nào?"}}, {_id:98, properties:{no:101, content:"Tại nơi đường giao nhau không có báo hiệu đi theo đường vòng xuyến, người điều khiển phương tiện phải nhường đường như thế nào là đúng quy tắc giao thông?"}}, {_id:104, properties:{no:102, content:"Trên đoạn đường bộ giao nhau cùng mức với đường sắt thì loại phương tiện nào được quyền ưu tiên đi trước?"}}, {_id:108, properties:{no:103, content:"Tại nơi đường bộ giao nhau cùng mức với đường sắt chỉ có đèn tín hiệu hoặc chuông báo hiệu, khi đèn tín hiệu màu đỏ đã bật sáng hoăc có tiếng chuông báo hiệu, người tham gia giao thông phải dừng lại ngay và giữ khoảng cách tối thiểu bao nhiêu mét tính từ ray gần nhất?"}}, {_id:115, properties:{no:104, content:"Người lái xe phải làm gì khi điều khiển xe vào đường cao tốc?"}}, {_id:119, properties:{no:105, content:"Người lái xe phải làm gì khi điều khiển xe ra khỏi đường cao tốc?"}}, {_id:3158, properties:{no:66, content:"Theo Luật Giao thông đường bộ, người điều khiển, người ngồi trên xe mô tô hai bánh, xe mô tô ba bánh, xe gắn máy có phải đội mũ bảo hiểm không?"}}, {_id:124, properties:{no:106, content:"Người điều khiển phương tiện tham gia giao thông trong hầm đường bộ ngoài việc phải tuân thủ các quy tắc giao thông còn phải thực hiện những quy định nào?"}}, {_id:3164, properties:{no:67, content:"Theo Luật Giao thông đường bộ, người điều khiển xe mô tô hai bánh, xe mô tô ba bánh, xe gắn máy không được thực hiện hành vi nào?"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Question;
UNWIND [{_id:126, properties:{no:107, content:"Người ngồi trên xe mô tô hai bánh, xe mô tô 3 bánh, xe gắn máy phải đội mũ bảo hiểm có cài quai đúng quy cách khi nào?"}}, {_id:130, properties:{no:108, content:"Người điều khiển xe mô tô hai bánh, xe mô tô ba bánh, xe gắn máy không được thực hiện những hành vi nào?"}}, {_id:134, properties:{no:109, content:"Trên đường có nhiều làn đường, người điều kiển phương tiện tham gia giao thông sử dụng làn đường như thế nào là đúng?"}}, {_id:136, properties:{no:110, content:"Tại nơi đường giao nhau, người lái xe đang đi trên đường không ưu tiên phải nhường đường như thế nào?"}}, {_id:141, properties:{no:111, content:"Khi điều khiển xe chạy trên đường người lái xe phải mang theo các loại giấy tờ gì?"}}, {_id:144, properties:{no:112, content:"Người điều khiển xe mô tô, xe gắn máy trên đường mà máu trong có nồng độ còn vượt quá bao nhiêu thì bị cấm?"}}, {_id:146, properties:{no:113, content:"Tại nơi đường giao nhau, khi đèn điều khiển giao thông có tín hiệu vàng, người điều khiển phương tiện phải thực hiện như thế nào?"}}, {_id:3168, properties:{no:68, content:"Theo Luật Giao thông đường bộ, người ngồi trên xe mô tô hai bánh, xe mô tô ba bánh, xe gắn máy khi tham gia giao thông không được thực hiện hành vi nào?"}}, {_id:150, properties:{no:114, content:"Hành vi điều khiển xe cơ giới chạy quá tốc độ quy định, giành đường, vượt ẩu bị xử lý như thế nào?"}}, {_id:3174, properties:{no:69, content:"Luật Giao thông đường bộ quy định người điều khiển, người ngồi trên xe đạp, người điều khiển xe thô sơ khác phải tuân thủ những quy tắc nào?"}}, {_id:154, properties:{no:115, content:"Hành vi bỏ trốn sau khi gây tai nạn để trốn tránh trách nhiệm hoặc khi có điều kiện mà cố ý không cứu giúp người bị tai nạn giao thông bị xử lý như thế nào?"}}, {_id:3178, properties:{no:70, content:"Luật Giao thông đường bộ quy định quy tắc giao thông đường bộ đối với người đi bộ như thế nào?\n"}}, {_id:3181, properties:{no:71, content:"Luật Giao thông đường bộ quy định đối với người khuyết tật, người già yếu tham gia giao thông như thế nào?"}}, {_id:3185, properties:{no:72, content:"Luật Giao thông đường bộ quy định đối với người dẫn dắt súc vật đi trên đường bộ như thế nào?"}}, {_id:3190, properties:{no:73, content:"Luật Giao thông đường bộ quy định độ tuổi của người lái xe như thế nào?"}}, {_id:3195, properties:{no:74, content:"Hành vi chuyển hướng không nhường đường cho: Các xe đi ngược chiều; người đi bộ, xe lăn của người khuyết tật đang qua đường tại nơi không có vạch kẻ đường cho người đi bộ bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:158, properties:{no:116, content:"Người điều khiển xe mô tô hai bánh, ba bánh, xe gắn máy sử dụng xe để kéo, đẩy các xe khác, vật khác khi tham gia giao thông bị xử lý như thế nào?"}}, {_id:162, properties:{no:117, content:"Người ngồi trên xe mô tô hai bánh, xe mô tô ba bánh, xe gắn máy khi tham gia giao thông mang, vác vật cồng kềnh bị xử lý như thế nào?"}}, {_id:166, properties:{no:118, content:"Bảo đảm trật tự, an toàn giao thông đường bộ là trách nhiệm của ai?"}}, {_id:3204, properties:{no:75, content:"Hành vi điều khiển xe chạy tốc độ thấp hơn các xe khác đi cùng chiều mà không đi về bên phải phần đường xe chạy, trừ trường hợp các xe khác đi cùng chiều chạy quá tốc độ quy định bị xử phạt vi phạm hành chính như thế nào?"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Question;
UNWIND [{_id:171, properties:{no:119, content:"Biển báo hiệu đường bộ gồm những nhóm nào, ý nghĩa của từng nhóm?"}}, {_id:173, properties:{no:120, content:"Tại nơi đường giao nhau, khi người điều khiển giao thông ra hiệu lệnh bằng hai tay hoặc một tay giang ngang để báo hiệu thì người tham gia giao thông phải đi như thế nào là đúng quy tắc giao thông?"}}, {_id:177, properties:{no:121, content:"Tại nơi đường giao nhau, khi người điều khiển giao thông ra hiệu lệnh bằng tay phải giơ về phía trước để báo hiệu thì người tham gia giao thông phải đi như thế nào?"}}, {_id:179, properties:{no:122, content:"Tại nơi đường giao nhau, khi người điều khiển giao thông ra hiệu lệnh tay giơ thẳng đứng để báo hiệu thì người tham gia giao thông phải đi như thế nào?"}}, {_id:3216, properties:{no:76, content:"Hành vi không thắt dây an toàn khi điều khiển xe chạy trên đường; chở người trên xe ô tô không thắt dây an toàn (tại vị trí có trang bị dây an toàn) khi xe đang chạy bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:183, properties:{no:123, content:"Trên đường giao thông, khi hiệu lệnh của người điều khiển giao thông trái với hiệu lệnh của đèn hoặc biển báo hiệu thì người tham gia giao thông phải chấp hành theo hiệu lệnh nào?"}}, {_id:186, properties:{no:124, content:"Tại nơi có biển báo hiệu cố định lại có báo hiệu tạm thời thì người tham gia giao thông phải chấp hành hiệu lệnh của báo hiệu nào?"}}, {_id:3221, properties:{no:77, content:"Hành vi dùng tay sử dụng điện thoại di động khi đang điều khiển xe chạy trên đường bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:190, properties:{no:125, content:"Trên đường có nhiều làn đường cho xe đi cùng chiều được phân biệt bằng vạch kẻ phân làn đường, người điều khiển phương tiện phải cho xe đi như thế nào?"}}, {_id:3225, properties:{no:78, content:"Hành vi không chấp hành hiệu lệnh của đèn tín hiệu giao thông; không chấp hành hiệu lệnh, hướng dẫn của người điều khiển giao thông hoặc người kiểm soát giao thông bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:196, properties:{no:126, content:"\"Phương tiện giao thông đường bộ\" gồm \n những loại nào?"}}, {_id:3233, properties:{no:79, content:"Hành vi điều khiển xe chạy quá tốc độ quy định từ 10 km/h đến 20 km/h bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:3238, properties:{no:80, content:"Hành vi không chấp hành yêu cầu kiểm tra về nồng độ cồn của người thi hành công vụ bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:3244, properties:{no:81, content:"Hành vi không có báo hiệu xin vượt trước khi vượt bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:198, properties:{no:127, content:"Người điều khiển phương tiện tham gia giao thông trong hầm đường bộ ngoài việc phải tuân thủ các quy tắc giao thông còn phải thực hiện những quy định nào?"}}, {_id:3250, properties:{no:82, content:"Hành vi chở người ngồi trên xe sử dụng ô (dù) bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:202, properties:{no:128, content:"Những trường hợp nào không được đi vào đường cao tốc trừ người, phương tiện, thiết bị phục vụ cho việc quản lý, bảo trì đường cao tốc?"}}, {_id:205, properties:{no:129, content:"Trên đường bộ trong khu vực đông dân cư xe mô tô hai bánh, xe gắn máy tham gia giao thông với tốc độ tối đa cho phép là bao nhiêu?"}}, {_id:3253, properties:{no:83, content:"Hành vi chở theo 03 người trên xe trở lên bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:3257, properties:{no:84, content:"Hành vi bấm còi, rú ga (nẹt pô) liên tục trong đô thị, khu đông dân cư, trừ các xe ưu tiên đang đi làm nhiệm vụ theo quy định bị xử phạt vi phạm hành chính như thế nào?"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Question;
UNWIND [{_id:211, properties:{no:130, content:"Trên đường bộ trong khu vực đông dân cư, loại xe nào tham gia giao thông với tốc độ tối đa cho phép là 40km/h?"}}, {_id:214, properties:{no:131, content:"Khi có tín hiệu của xe ưu tiên người tham gia giao thông phải làm gì?"}}, {_id:217, properties:{no:132, content:"Người lái xe phải làm gì khi quay đầu xe trên cầu, gầm cầu vượt, đường ngầm hay khu vực đường bộ giao nhau cùng mức với đường sắt?"}}, {_id:224, properties:{no:133, content:"Người lái xe phải giảm tốc độ thấp hơn tốc độ tối đa cho phép đến mức không nguy hiểm?"}}, {_id:230, properties:{no:134, content:"Trong trường hợp đặc biệt, xe quá tải trọng, quá khổ giới hạn của đường bộ, xe bánh xích có thể lưu hành trên đường nhưng phải xin phép cơ quan nào?"}}, {_id:238, properties:{no:135, content:"Thời gian làm việc trong một ngày của người lái xe được quy định là bao nhiêu?"}}, {_id:242, properties:{no:136, content:"Khi sơ cứu ban đầu cho người bị tai nạn giao thông đường bộ không còn hô hấp, người lái xe và người có mặt tại hiện trường vụ tai nạn phải thực hiện các công việc gì?"}}, {_id:247, properties:{no:137, content:"Khi điều khiển ôtô qua đoạn đường ngập nước, người lái xe cần thực hiện các thao tác nào?"}}, {_id:252, properties:{no:138, content:"Khi điều khiển ôtô tới gần xe chạy ngược chiều vào ban đêm, người lái xe cần thực hiện các thao tác nào?"}}, {_id:255, properties:{no:139, content:"Khi điều khiển xe ôtô trên đường trơn cần chú ý những điểm gì?"}}, {_id:259, properties:{no:140, content:"Hành vi đưa xe cơ giới, xe máy chuyên \n dụng không bảo đảm tiêu chuẩn an toàn \n kỹ thuật và bảo vệ môi trường tham gia \n giao thông đường bộ có bị nghiêm cấm \n hay không?"}}, {_id:265, properties:{no:141, content:"Tại ngã ba hoặc ngã tư không có biển báo an toàn, người lái xe phải nhường đường như thế nào?"}}, {_id:268, properties:{no:142, content:"Khi xe ô tô đi ngược chiều đến rất gần, \n xe ô tô phía sau cùng chiều cố tình vượt, \n người lái xe xử lý như thế nào?"}}, {_id:273, properties:{no:143, content:"Trên đường cao tốc, trừ khi nhập làn và tách làn, với điều kiện mặt đường khô ráo, xe cơ giới đang chạy với tốc độ trên 100 km/h đến 200 km/h, người lái xe phải duy trì khoảng cách an toàn với xe đang chạy phía trước tối thiểu bao nhiêu?"}}, {_id:281, properties:{no:144, content:"Hành khách có các nghĩa vụ gì khi đi trên \n xe vận tải hành khách?"}}, {_id:285, properties:{no:145, content:"Người lái xe cố tình vi phạm Luật giao thông đường bộ, không phân biệt làn đường, vạch phân làn, phóng nhanh, vượt ẩu, vượt đèn đỏ, đi vào đường cấm, đường một chiều được coi là vi hành vi nào?"}}, {_id:294, properties:{no:146, content:"Khi khởi hành ô tô trên đường bằng, người lái xe cần thực hiện các thao tác nào theo trình tự nào?"}}, {_id:299, properties:{no:147, content:"Để đạt được hiệu quả phanh cao nhất, người lái xe mô tô phải sử dụng các kỹ năng như thế nào?"}}, {_id:303, properties:{no:148, content:"Niên hạn sử dụng của ô tô tải (tính bắt đầu từ năm sản xuất) là bao nhiêu năm?"}}, {_id:308, properties:{no:149, content:"Trong khu dân cư, nơi nào cho phép người lái xe, người điều khiển xe máy chuyên dụng được quay đầu xe?"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Question;
UNWIND [{_id:313, properties:{no:150, content:"Người đủ bao nhiêu tuổi trở lên thì được điều khiển xe ô tô tải, máy kéo có trọng tải từ 3,5 tấn trở lên; xe hạng B2 kéo rơ mooc (FB2)?"}}, {_id:322, properties:{no:151, content:"Người kinh doanh vận tải hành khách có những quyền hạn nào theo quy định?"}}, {_id:328, properties:{no:152, content:"Khi vào số để tiến hoặc lùi xe ô tô có số tự động, người lái xe phải thực hiện các thao tác nào để đảm bảo an toàn?"}}, {_id:335, properties:{no:153, content:"Thế nào là động cơ 4 kỳ?"}}, {_id:339, properties:{no:154, content:"Phần của đường bộ đươc sử dụng cho các phương tiện giao thông qua lại là gì?"}}, {_id:341, properties:{no:155, content:"Người tham gia giao thông đường bộ gồm những đối tượng nào?"}}, {_id:343, properties:{no:156, content:"Khi lái xe trên đường, người lái xe \n cần quan sát và bảo đảm tốc độ \n phương tiện như thé nào?"}}, {_id:347, properties:{no:157, content:"Phương tiện giao thông đường bộ di chuyển với tốc độ thấp hơn phải đi như thế nào?"}}, {_id:350, properties:{no:158, content:"Khi lái xe trong khu đô thị và đông dân cư trừ các khu vực có biển cấm sử dụng còi, người lái xe được sử dụng còi như thế nào?"}}, {_id:355, properties:{no:159, content:"Việc sản xuất, mua bán, sử dụng biển số xe cơ giới, xe máy chuyên dùng được quy định như thế nào trong Luật Giao thông đường bộ?"}}, {_id:359, properties:{no:160, content:"Bạn đang lái xe phía trước có một xe cảnh sát giao thông không phát tín hiệu ưu tiên bạn có được phép vượt hay không?"}}, {_id:364, properties:{no:161, content:"Bạn đang lái xe phía trước có một xe cứu thương không phát tín hiệu ưu tiên có được phép vượt hay không?"}}, {_id:368, properties:{no:162, content:"Bạn đang lái xe phía trước có một xe chữa cháy không phát tín hiệu ưu tiên có được phép vượt hay không?"}}, {_id:371, properties:{no:163, content:"Người lái xe không được lùi xe ở những khu vực nào?"}}, {_id:373, properties:{no:164, content:"Khi xe đã kéo 1 xe hoặc xe đã kéo 1 rơ moóc, bạn có được phép kéo thêm xe (kể cả xe thô sơ) hoặc rơ moóc thứ hai hay không?"}}, {_id:379, properties:{no:165, content:"Người ngồi trên xe mô tô hai bánh, ba bánh, xe gắn máy khi tham gia giao thông có được mang, vác vật cồng kềnh hay không?"}}, {_id:383, properties:{no:166, content:"Người ngồi trên xe mô tô hai bánh, xe mô tô ba bánh, xe gắn máy khi tham gia giao thông có được sử dụng ô khi trời mưa hay không?"}}, {_id:386, properties:{no:167, content:"Hành vi sử dụng xe mô tô để kéo, đẩy xe mô tô khác bị hết xăng đến trạm mua xăng có được phép hay không?"}}, {_id:392, properties:{no:168, content:"Tuổi tối đa của người lái xe ô tô chở người trên 30 chỗ ngồi (hạng E) là bao nhiêu tuổi?"}}, {_id:397, properties:{no:169, content:"Người có giấy phép lái xe hạng B2 được điều khiển loại xe nào?"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Question;
UNWIND [{_id:402, properties:{no:170, content:"Khi dạy thực hành lái xe, giáo viên phải mang theo các loại giấy tờ gì?"}}, {_id:405, properties:{no:171, content:"Khi điều khển xe chạy trên đường biết có xe sau xin vượt nếu đủ điều kiện an toàn người lái xe phải làm gì?"}}, {_id:410, properties:{no:172, content:"Khi muốn chuyển hướng, người lái xe phải thực hiện như thế nào để đảm bảo an toàn giao thông?"}}, {_id:414, properties:{no:173, content:"Khi tránh xe đi ngược chiều, các xe phải nhường đường như thế nào là đúng quy tắc giao thông?"}}, {_id:419, properties:{no:174, content:"Trên đường cao tốc, người lái xe phải dừng xe, đỗ xe như thế nào để đảm bảo an toàn giao thông?"}}, {_id:422, properties:{no:175, content:"Xe quá tải trọng, quá khổ giới hạn tham gia giao thông cần tuân thủ quy định nào?"}}, {_id:424, properties:{no:176, content:"Người lái xe phải giảm tốc độ, đi sát về bên phải của phần đường xe chạy trong các trường hợp nào?"}}, {_id:428, properties:{no:177, content:"Tác dụng của mũ bảo hiểm đối với người ngồi trên xe mô tô hai bánh trong trường hợp xảy ra tai nạn giao thông là gì?"}}, {_id:431, properties:{no:178, content:"Khi điều khiển xe trên đường vòng, khuất tầm nhìn người lái xe cần phải làm gì để đảm bảo an toàn?"}}, {_id:436, properties:{no:179, content:"Trên đường bộ, người lái xe ô tô có được phép dừng xe, đỗ xe song song với một xe khác đang dừng, đỗ hay không?"}}, {_id:440, properties:{no:180, content:"Khi điều khiển ô tô xuống dốc cao, người lái xe cần thực hiện các thao tác nào để đảm bảo an toàn?"}}, {_id:443, properties:{no:181, content:"Khi điều khiển xe ô tô rẽ trái ở chỗ đường giao nhau, người lái xe cần thực hiện các thao tác nào để đảm bảo an toàn?"}}, {_id:445, properties:{no:182, content:"Khi điều khiển xe qua đường sắt, người lái xe cần phải thực hiện các thao tác nào để đảm bảo an toàn?"}}, {_id:449, properties:{no:183, content:"Khi động cơ ô tô đã khởi động, muốn điều chỉnh ghế của người lái, người lái xe phải điều chỉnh cần số ở vị trí nào?"}}, {_id:456, properties:{no:184, content:"Khi điều khiển ô tô gặp mưa to hoặc sương mù, người lái xe phải làm gì để đảm bảo an toàn?"}}, {_id:460, properties:{no:185, content:"Khi động cơ xe số tự động có trang bị chìa khóa thông minh có cần đạp hết hành trình bàn đạp của phanh?"}}, {_id:467, properties:{no:186, content:"Khi đã đỗ xe ô tô sát lề đường bên phải, người lái xe phải thực hiện các thao tác nào khi mở cửa xuống xe để đảm bảo an toàn?"}}, {_id:473, properties:{no:187, content:"Khi điều khiển xe giảm số, người lái xe cần chú ý những điểm gì để đảm bảo an toàn?"}}, {_id:476, properties:{no:188, content:"Khi điều khiển xe sử dụng hộp số cơ khí vượt qua rãnh lớn cắt ngang mặt đường, người lái xe cần thực hiện các thao tác nào để đảm bảo an toàn?"}}, {_id:480, properties:{no:189, content:"Khi điều khiển xe trên đường vòng người lái xe cần phải làm gì để đảm bảo an toàn?"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Question;
UNWIND [{_id:484, properties:{no:190, content:"Khi nhả hệ thống phanh dừng cơ khí điều khiển bằng tay (phanh tay), người lái xe cần phải thực hiện các thao tác nào?"}}, {_id:488, properties:{no:191, content:"Khi xảy ra tai nạn giao thông, có người bị thương nghiêm trọng, người lái xe và người có mặt tại hiện trường vụ tai nạn phải thực hiện các công việc gì?"}}, {_id:494, properties:{no:192, content:"Khi sơ cứu người tai nạn giao thông đường bộ, có vết thương chảy máu ngoài, màu đỏ tươi phun thành tia và phun mạnh khi mạch đập bạn phải làm gì?"}}, {_id:500, properties:{no:193, content:"Khi dừng, đỗ xe trên đường phố hẹp, người lái xe ô tô phải dừng, đỗ xe ở vị trí cách xe ô tô đang đỗ bên kia đường khoảng cách tối thiểu là bao nhiêu mét trong các trường hợp nào để đảm bảo an toàn giao thông?"}}, {_id:506, properties:{no:194, content:"Khi điêu khiển xe chạy với tốc độ 60km/h người lái xe phải chủ động điều khiển xe như thế nào để đảo bảo an toàn giao thông?"}}, {_id:510, properties:{no:195, content:"Trên đường bộ ngoài khu vực đông dân cư, đường đôi có dải phân cách giữa (trừ đường cao tốc), loại xe nào tham gia giao thông với tốc độ tối đa cho phép là 70 km/h"}}, {_id:517, properties:{no:196, content:"Tốc độ tối đa cho phép đối với xe máy chuyên dùng, xe gắn máy (kể cả xe máy điện) và các loại xe tương tự trên đường bộ (trừ đường cao tốc) không được vượt quá bao nhiêu km/h?"}}, {_id:522, properties:{no:197, content:"Khi lái xe trên đường vắng mà cảm thấy buồn ngủ, người lái xe nên chọn cách xử lý như thế nào cho phù hợp?"}}, {_id:525, properties:{no:198, content:"Biển báo hiệu có dạng hình tròn, viền đỏ, nền trắng, trên nền có hình vẽ được chữ số, chữ viết màu đen là loại biển gì?"}}, {_id:534, properties:{no:199, content:"Người có giấy phép lái xe hạng B1 được điều khiển loại xe nào?"}}, {_id:537, properties:{no:200, content:"Người có giấy phép lái xe hạng B2 được điều khiển loại xe nào?"}}, {_id:539, properties:{no:201, content:"Người có giấy phép lái xe hạng C được điều khiển loại xe nào?"}}, {_id:542, properties:{no:202, content:"Người có giấy phép lái xe hạng D được điều khiển loại xe nào?"}}, {_id:545, properties:{no:203, content:"Người có giấy phép lái xe hạng E được điều khiển loại xe nào?"}}, {_id:548, properties:{no:204, content:"Người có giấy phép lái xe hạng F được điều khiển loại xe nào?"}}, {_id:2812, properties:{no:1, content:"Nguyên tắc hoạt động giao thông đường bộ được quy định như thế nào?"}}, {_id:2814, properties:{no:2, content:"Các hành vi bị nghiêm cấm thực hiện theo Luật giao thông đường bộ năm 2008?"}}, {_id:2826, properties:{no:3, content:"Quy tắc chung khi tham gia giao thông đường bộ được Luật giao thông đường bộ quy định như thế nào?"}}, {_id:2834, properties:{no:4, content:"Theo quy định của Luật Giao thông đường bộ thì hệ thống báo hiệu đường bộ gồm những gì? Khi tham gia giao thông phải chấp hành báo hiệu đường bộ như thế nào?"}}, {_id:2843, properties:{no:5, content:"Tốc độ và khoảng cách giữa các xe cơ giới khi tham gia giao thông được quy định như thế nào? mức phạt nếu vi phạm của từng loại xe?"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Question;
UNWIND [{_id:2845, properties:{no:6, content:"Những nguyên tắc cần phải tuân thủ khi muốn vượt xe? Những trường hợp nào không được vượt xe? Vượt xe không đúng quy định của pháp luật sẽ bị xử phạt như thế nào?"}}, {_id:2851, properties:{no:7, content:"Chuyển hướng không đúng quy định bị xử phạt như thế nào?"}}, {_id:2853, properties:{no:8, content:"Trong thực tế có không ít các vụ tai nạn giao thông nghiêm trọng liên quan đến người điều khiển phương tiện trong quá trình lùi xe gây ra. Vậy người điểu khiển phương tiện khi lùi xe cần phải tuân thủ những quy định nào?"}}, {_id:2858, properties:{no:9, content:"Khi điều khiển xe đi trên đường, để tránh xe đi ngược chiều tôi phải làm gì để không vi phạm quy định của pháp luật? Nếu vi phạm sẽ bị xử phạt như thế nào?"}}, {_id:2861, properties:{no:10, content:"Khi tham gia giao thông muốn dừng đỗ xe cần làm gì để đúng quy định? Nếu vi phạm sẽ bị xử phạt như thế nào?"}}, {_id:2868, properties:{no:11, content:"Hiện nay một số xe ô tô, mô tô và các loại xe tương tự khác sử dụng còi hơi, còi không đúng thiết kế, bấm còi không đúng nơi quy định gây khiến người đi đường dễ giật mình bởi tiếng còi to chói tai dẫn đến tai nạn giao thông. Vậy chế tài xử phạt cho những hành vi này như thế nào?"}}, {_id:2876, properties:{no:12, content:"Hành vi sử dụng điện thoại khi tham gia giao thông bị xử lý như thế nào?"}}, {_id:2879, properties:{no:13, content:"Hiện nay, tại một số cổng trường tiểu học, vẫn bắt gặp hình ảnh phụ huynh đưa đón con đi học bằng xe máy, xe đạp điện không đội mũ bảo hiểm cho các cháu. Vậy hành vi này có vi phạm pháp luật và bị xử phạt vi phạm hành chính trong lĩnh vực giao thông đường bộ hay không?"}}, {_id:2883, properties:{no:14, content:"Luật giao thông đường bộ năm 2008 quy định như thế nào về tín hiệu đèn giao thông? Mức xử phạt vi phạm hành chính đối với người điều khiển phương tiện không chấp hành tín hiệu đèn giao thông được quy định như thế nào?"}}, {_id:2888, properties:{no:15, content:"Người đi bộ khi tham giao thông đường bộ phải tuân thủ những quy tắc giao thông nào? Bị xử phạt vi phạm hành chính trong những trường hợp nào và mức phạt là bao nhiêu?"}}, {_id:2891, properties:{no:16, content:"Anh Nguyễn Văn A có một chiếc xe ô tô, do không may biển số bị rơi mất. Anh A có đến cơ quan Công an để xin làm lại nhưng trong thời gian chờ cấp lại biển số xe Anh A có đi làm biển có số giống như trong Giấy đăng ký để đeo tạm để đi đường và bị CSGT lập biên bản và tạm giữ xe. Vậy mức xử phạt vi phạm hành chính đối với hành vi vi phạm của anh A là bao nhiêu?"}}, {_id:2900, properties:{no:17, content:"Hành vi phóng nhanh lạng lách, đánh võng luôn là nỗi bức xúc và lo lắng cho người đi đường, uy hiếp đến trật tự an toàn giao thông. Vậy mức xử phạt vi phạm hành chính đối với hành vi này được quy định như thế nào?"}}, {_id:2904, properties:{no:18, content:"Tại khoản 6 Điều 8 Luật giao thông đường bộ năm 2008 quy định nghiêm cấm hành vi đua xe, cổ vụ đua xe, tổ chức đua xe trái phép, lạng lách, đánh võng. Vậy mức xử phạt vi phạm hành chính đối với người đua xe trái phép, người cổ vũ đua xe trái phép được quy định như thế nào?"}}, {_id:2913, properties:{no:19, content:"Hành vi uống rượu, bia khi điều khiển ô tô tham gia giao thông sẽ bị xử phạt vi phạm hành chính như thế nào?"}}, {_id:2916, properties:{no:20, content:"Trường hợp người điều khiển xe mô tô, xe máy kéo uốngrượu, bia khi lái xe tham gia giao thông sẽ bị xử phạt như thế nào?"}}, {_id:2921, properties:{no:21, content:"Người đi xe đạp đã uống rượu, bia khi tham gia giao thông có bị xử phạt vi phạm hành chính không?"}}, {_id:2922, properties:{no:22, content:"Pháp luật quy định xử phạt đối với người điều khiển xe ô tô chở quá trọng tải như thế nào?"}}, {_id:2926, properties:{no:23, content:"Theo Nghị định số 100/2019/NĐ-CP khi xe vượt quá trọng tải cho phép thì không chỉ người điều khiển xe, mà cả chủ xe cũng sẽ bị xử phạt. Vậy mức xử phạt đối với chủ xe được quy định như thế nào?"}}, {_id:2933, properties:{no:24, content:"Ông Trần Văn B lái xe khách 24 chỗ ngồi bị lập biên bản lỗi chở quá số người quy định 30/24 người. Xin hỏi ông B sẽ bị xử phạt như thế nào? Chủ xe có bị phạt không?"}}, {_id:2938, properties:{no:25, content:"Hiện nay, trên các tuyến đường giao thông vẫn xảy ra tình trạng các xe thường đón, trả khách tại những địa điểm không đúng nơi quy định, dễ gây tai nạn và gây nguy hiểm cho những người tham gia giao thông khác. Vậy trường hợp lái xe đón trả khách không đúng nơi quy định này bị xử lý như thế nào?"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Question;
UNWIND [{_id:2947, properties:{no:26, content:"Pháp luật quy định người lái xe phải mang theo Bảo hiểm trách nhiệm dân sự của chủ xe cơ giới còn hiệu lực khi tham gia giao thông. Vậy hành vi không mang theo Bảo hiểm hoặc có mang theo Bảo hiểm nhưng đã hết hạn của lái xe khi tham gia giao thông thì bị xử lý như thế nào?"}}, {_id:2954, properties:{no:27, content:"Thời gian gần đây, trên các trang diễn đàn mạng có nhiều ý kiến bức xúc về tình trạng ô-tô “độ” đèn pha với cường độ ánh sáng mạnh, gây lóa mắt, nguy hiểm cho các phương tiện đi ngược chiều. Trong nhiều tình huống, có những xe lắp đèn ở phía sau, mỗi lần đạp phanh hoặc bật nguồn, dàn đèn led chiếu thẳng vào xe đi phía sau khiến lái xe phía sau không thể quan sát tình hình để phản xạ kịp thời nên sẽ tiềm ẩn nguy cơdẫn đến tai nạn. Vậy hành vi này bị xử lý như thế nào?"}}, {_id:2964, properties:{no:28, content:"Việc tự ý chiếm dụng lòng đường, vỉa hè làm nơi trông giữ xe gây cản trở giao thông và mất mỹ quan đô thị có xem là vi phạm pháp luật giao thông đường bộ không? Nếu vi phạm thì bị xử phạt tại nghị định nào?"}}, {_id:2971, properties:{no:29, content:"Nghị định số 100/2019/NĐ-CP quy định như thế nào về xử phạt người điều khiển xe ô tô, máy kéo và các loại xe tương tự xe ô tô vi phạm quy định về bảo vệ môi trường khi tham gia giao thông?"}}, {_id:2976, properties:{no:30, content:"Xe máy chuyên dùng là xe gì? Người điều khiển xe máy chuyên dùng khi tham gia giao thông phải đáp ứng điều kiện nào? Người điều khiển xe máy chuyên dùng sẽ bị xử phạt như thế nào khi có hành vi vi phạm quy định về điều kiện của người điều khiển xe máy chuyên dùng?"}}, {_id:2984, properties:{no:31, content:"Lái xe khi tham gia giao thông phải đáp ứng điều kiện gì? Nghị định số 100/2019/NĐ-CP quy định như thế nào về xử phạt các hành vi vi phạm quy định về điều kiện của người điều khiển xe cơ giới."}}, {_id:2987, properties:{no:32, content:"Luật giao thông đường bộ 2008 quy định độ tuổi của người lái xe gắn máy, xe mô tô là bao nhiêu? Người điều khiển phương tiện giao thông vi phạm quy định về độ tuổi bị xử phạt như thế nào?"}}, {_id:2994, properties:{no:33, content:"Theo Nghị định số 100/2020/NĐ-CP ngày 17/01/2020 của Chính phủ quy định về điều kiện kinh doanh bằng xe ô tô thì Xe ô tô kinh doanh vận tải hành khách, xe ô tô kinh doanh vận tải hàng hóa và xe trung chuyển phải lắp thiết bị giám sát hành trình. Vậy người điều khiển xe ô tô kinh doanh vận tải không lắp thiết bị giám sát hành trình bị xử phạt như thế nào?"}}, {_id:3000, properties:{no:34, content:"Theo Nghị định số 100/2020/NĐ-CP ngày 17/01/2020 của Chính phủ quy định về điều kiện kinh doanh bằng xe ô tô thì Xe ô tô kinh doanh vận tải hành khách, xe ô tô kinh doanh vận tải hàng hóa và xe trung chuyển phải lắp thiết bị giám sát hành trình. Vậy cá nhân, tổ chức kinh doanh vận tải không thực hiện việc lắp thiết bị giám sát hành trình bị xử phạt như thế nào?"}}, {_id:3003, properties:{no:35, content:"Theo quy định của Nghị định số 100/2019/NĐ-CP, Nhân viên phục vụ trên xe buýt, xe vận chuyển hành khách theo tuyến cố định sẽ bị xử phạt như thế nào khi có hành vi vi phạm quy định về trật tự an toàn giao thông?"}}, {_id:3009, properties:{no:36, content:"Theo quy định của Nghị định số 100/2019/NĐ-CP, hành khách đi xe vi phạm quy định về trật tự an toàn giao thông sẽ bị xử phạt như thế nào?"}}, {_id:3013, properties:{no:37, content:"Nghị định số 100/2019/NĐ-CP quy định về xử phạt đối với Tổ chức, cá nhân có hành vi vi phạm quy định về xây dựng bến xe, bãi đỗ xe, tạm dừng nghỉ như thế nào?"}}, {_id:3020, properties:{no:38, content:"Những hành vi vi phạm quy định về bảo vệ hạ tầng giao thông đường bộ thường hay xảy ra?"}}, {_id:3024, properties:{no:39, content:"Từ ngày 01/01/2020, các hành vi vi phạm nào của tổ chức quản lý, vận hành trạm thu phí đường bộ sẽ bị xử lý theo Nghị định số 100/2019/NĐ-CP? Mức phạt là bao nhiêu?"}}, {_id:3028, properties:{no:40, content:"Nghị định số 100/2019/NĐ-CP quy định vể xử phạt đối với Người điều khiển xe ô tô thực hiện hảnh vi vi phạm quy định về vận chuyển hàng siêu trường, siêu trọng?"}}, {_id:3033, properties:{no:41, content:"Người điều khiển xe vệ sinh môi trường, xe ô tô chở phế thải thực hiện hành vi điều khiển xe chạy không đúng tuyến, không đúng thời gian quy định thì bị xử phạt như thế nào?"}}, {_id:3040, properties:{no:42, content:"Nghị định số 100/2019/NĐ-CP quy định mức xử phạt đối với người điều khiển phương tiện giao thông cơ giới đường bộ gắn biển nước ngoài như thế nào?"}}, {_id:3044, properties:{no:43, content:"Hành vi không làm thủ tục đăng ký sang tên xe có bị xử lý vi phạm hành chính hay không?"}}, {_id:3050, properties:{no:44, content:"Đăng kiểm viên, nhân viên nghiệp vụ của Trung tâm đăng kiểm bị xử phạt như thế nào khi thực hiện hành vi vi phạm quy định về hoạt động kiểm định an toàn kỹ thuật, bảo vệ môi trường xe cơ giới?"}}, {_id:3056, properties:{no:45, content:"Trung tâm đăng kiểm thực hiện hành vi vi phạm quy định về hoạt động kiểm định an toàn kỹ thuật, bảo vệ môi trường xe cơ giới thì bị xử phạt như thế nào?"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Question;
