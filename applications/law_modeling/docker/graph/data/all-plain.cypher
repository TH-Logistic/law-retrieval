UNWIND [{_id:1951, properties:{index:"a", content:"trạm kiểm tra tải trọng xe;"}}, {_id:1952, properties:{index:"b", content:"đội nghiệp vụ thuộc thanh tra sở giao thông vận tải, đội thanh tra - an toàn thuộc cục quản lý đường bộ;"}}, {_id:1953, properties:{index:"c", content:"bộ phận cân thuộc tổ chức được giao khai thác, bảo trì kết cấu hạ tầng giao thông đường bộ."}}, {_id:1955, properties:{index:"a", content:"cơ quan quản lý phương tiện, thiết bị kỹ thuật nghiệp vụ quy định tại điều 4 thông tư này có văn bản giao cho đơn vị trực tiếp quản lý, sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ quy định tại khoản 1 điều này, trong đó phải nêu rõ đơn vị trực tiếp quản lý, sử dụng; loại phương tiện, thiết bị kỹ thuật nghiệp vụ; thời gian sử dụng; phạm vi và trách nhiệm quản lý, sử dụng;"}}, {_id:794, properties:{index:"a", content:"thực hiện hành vi quy định tại khoản 1 điều này buộc phải treo biển báo thông tin công trình có đầy đủ nội dung theo quy định;"}}, {_id:1956, properties:{index:"b", content:"đơn vị trực tiếp quản lý, sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ có trách nhiệm quản lý, sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ đúng nhiệm vụ được cơ quan quản lý phương tiện, thiết bị kỹ thuật nghiệp vụ giao và quy định của pháp luật."}}, {_id:795, properties:{index:"b", content:"thực hiện hành vi quy định tại điểm a, điểm b khoản 2; khoản 3; điểm a khoản 4; khoản 5 điều này buộc phải thực hiện ngay các biện pháp bảo đảm an toàn giao thông theo quy định;"}}, {_id:796, properties:{index:"c", content:"thực hiện hành vi quy định tại điểm c, điểm d khoản 2; điểm b khoản 4 điều này buộc phải thực hiện ngay các biện pháp bảo đảm an toàn giao thông theo quy định và khôi phục lại tình trạng ban đầu đã bị thay đổi do vi phạm hành chính gây ra."}}, {_id:1959, properties:{index:"a", content:"là thanh tra viên, công chức thanh tra, công chức thuộc các cơ quan thực hiện chức năng thanh tra đường bộ; nhân viên thuộc các cơ quan quản lý đường bộ và các tổ chức được giao khai thác, bảo trì kết cấu hạ tầng giao thông đường bộ"}}, {_id:1961, properties:{index:"a", content:"hiểu biết các tiêu chuẩn, quy chuẩn kỹ thuật, quy định, quy trình, nghiệp vụ liên quan đến kiểm tra tải trọng xe, các quy định của pháp luật về bảo vệ kết cấu hạ tầng giao thông đường bộ, trật tự an toàn giao thông, xử phạt vi phạm hành chính, quy trình vận hành và bảo trì thiết bị; sử dụng thành thạo thiết bị cân kiểm tra tải trọng xe;"}}, {_id:801, properties:{index:"a", content:"xây dựng hoặc thành lập bến xe, bãi đỗ xe, trạm dừng nghỉ, trạm thu phí đường bộ khi chưa được cơ quan quản lý nhà nước về giao thông vận tải đồng ý theo quy định;"}}, {_id:802, properties:{index:"b", content:"xây dựng bến xe, bãi đỗ xe, trạm dừng nghỉ, trạm thu phí đường bộ không bảo đảm tiêu chuẩn kỹ thuật theo quy định."}}, {_id:1962, properties:{index:"b", content:"có trình độ b về tin học trở lên đối với người sử dụng hệ thống máy tính điều khiển hệ thống thiết bị cân kiểm tra tải trọng xe."}}, {_id:804, properties:{index:"a", content:"thực hiện hành vi quy định tại điểm a khoản 1 điều này còn buộc phải tháo dỡ công trình xây dựng trái phép và khôi phục lại tình trạng ban đầu đã bị thay đổi do vi phạm hành chính gây ra;"}}, {_id:805, properties:{index:"b", content:"thực hiện hành vi quy định tại điểm b khoản 1 điều này còn buộc phải xây dựng lại bến xe, bãi đỗ xe, trạm dừng nghỉ, trạm thu phí đường bộ theo đúng quy định, bảo đảm tiêu chuẩn kỹ thuật."}}, {_id:1964, properties:{index:"a", content:"đáp ứng yêu cầu quy định tại điểm a khoản 1 điều này;"}}, {_id:1965, properties:{index:"b", content:"hiểu biết các quy định của pháp luật về bảo vệ kết cấu hạ tầng giao thông đường bộ, trật tự an toàn giao thông, xử phạt vi phạm hành chính trong lĩnh vực giao thông đường bộ."}}, {_id:1969, properties:{index:"a", content:"quản lý và sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ đúng quy trình, quy định của pháp luật;"}}, {_id:1970, properties:{index:"b", content:"sử dụng kết quả thu được từ phương tiện, thiết bị kỹ thuật nghiệp vụ đúng mục đích theo quy định của pháp luật và chịu trách nhiệm trước pháp luật về kết quả thu được từ phương tiện, thiết bị kỹ thuật nghiệp vụ."}}, {_id:1974, properties:{index:"a", content:"phải được kiểm định theo quy định của pháp luật; bảo đảm tuân thủ tiêu chuẩn, quy chuẩn kỹ thuật trong suốt quá trình sử dụng và giữa hai kỳ kiểm định, hiệu chuẩn;"}}] AS row
