UNWIND [{_id:1993, properties:{index:"c", content:"tổng cục đường bộ việt nam quy định cụ thể mẫu phiếu cân kiểm tra tải trọng xe, mẫu phiếu xác nhận kết quả thiết bị ghi hình và mẫu sổ theo dõi việc sử dụng phiếu cân kiểm tra tải trọng xe và phiếu xác nhận kết quả thiết bị ghi hình."}}, {_id:824, properties:{index:"b", content:"làm hư hỏng hoặc làm mất tác dụng của hệ thống thoát nước công trình đường bộ, trừ các hành vi vi phạm quy định tại khoản 5 điều này;"}}, {_id:825, properties:{index:"c", content:"tự ý tháo dỡ, di chuyển, treo, đặt, làm hư hỏng, làm sai mục đích sử dụng hoặc làm sai lệch biển báo hiệu, đèn tín hiệu giao thông, rào chắn, cọc tiêu, cột cây số, vạch kẻ đường, tường bảo vệ, lan can phòng hộ, mốc chỉ giới;"}}, {_id:826, properties:{index:"d", content:"tự ý đập phá, tháo dỡ bó vỉa hè hoặc sửa chữa, cải tạo vỉa hè trái phép, trừ các hành vi vi phạm quy định tại điểm a, điểm c khoản 5 điều này."}}, {_id:828, properties:{index:"a", content:"khoan, đào, xẻ đường, hè phố trái phép;"}}, {_id:829, properties:{index:"b", content:"phá dỡ trái phép dải phân cách, gương cầu, các công trình, thiết bị an toàn giao thông trên đường bộ, cấu kiện, phụ kiện của công trình đường bộ, trừ các hành vi vi phạm quy định tại điểm c khoản 4 điều này;"}}, {_id:830, properties:{index:"c", content:"tự ý tháo, mở làm hư hỏng nắp cống, nắp ga các công trình ngầm, hệ thống tuy nen trên đường giao thông;"}}, {_id:831, properties:{index:"d", content:"nổ mìn hoặc khai thác đất, cát, đá, sỏi, khoáng sản khác trái phép làm ảnh hưởng đến công trình đường bộ;"}}, {_id:832, properties:{index:"đ", content:"rải, đổ hóa chất gây hư hỏng công trình đường bộ."}}, {_id:834, properties:{index:"a", content:"không thực hiện đúng quy trình quản lý, vận hành theo quy định để số lượng xe ô tô xếp hàng chờ trước trạm thu phí trên một làn xe chờ dài nhất lớn hơn 100 xe đến 150 xe hoặc để chiều dài dòng xe xếp hàng chờ trước trạm thu phí (tính từ cabin thu phí đến xe cuối cùng của hàng xe chờ) từ 750 m đến 1.000 m;"}}, {_id:835, properties:{index:"b", content:"không thực hiện đúng quy trình quản lý, vận hành theo quy định để thời gian đi qua trạm thu phí của một xe ô tô bất kỳ kể từ lúc dừng xe chờ thu phí đến lúc ra khỏi trạm thu phí lớn hơn 10 phút đến 20 phút."}}, {_id:837, properties:{index:"a", content:"không thực hiện đúng quy trình quản lý, vận hành theo quy định để số lượng xe ô tô xếp hàng chờ trước trạm thu phí trên một làn xe chờ dài nhất lớn hơn 150 xe đến 200 xe hoặc để chiều dài dòng xe xếp hàng chờ trước trạm thu phí (tính từ cabin thu phí đến xe cuối cùng của hàng xe chờ) lớn hơn 1.000 m đến 2.000 m;"}}, {_id:838, properties:{index:"b", content:"không thực hiện đúng quy trình quản lý, vận hành theo quy định để thời gian đi qua trạm thu phí của một xe ô tô bất kỳ kể từ lúc dừng xe chờ thu phí đến lúc ra khỏi trạm thu phí lớn hơn 20 phút đến 30 phút."}}, {_id:840, properties:{index:"a", content:"không thực hiện đúng quy trình quản lý, vận hành theo quy định để số lượng xe ô tô xếp hàng chờ trước trạm thu phí trên một làn xe chờ dài nhất lớn hơn 200 xe hoặc để chiều dài dòng xe xếp hàng chờ trước trạm thu phí (tính từ cabin thu phí đến xe cuối cùng của hàng xe chờ) lớn hơn 2.000 m;"}}, {_id:841, properties:{index:"b", content:"không thực hiện đúng quy trình quản lý, vận hành theo quy định để thời gian đi qua trạm thu phí của một xe ô tô bất kỳ kể từ lúc dừng xe chờ thu phí đến lúc ra khỏi trạm thu phí lớn hơn 30 phút."}}, {_id:843, properties:{index:"a", content:"vi phạm khoản 6, khoản 7, khoản 8 điều này mà không chấp hành yêu cầu của cơ quan quản lý nhà nước có thẩm quyền về việc triển khai các giải pháp để khắc phục ùn tắc giao thông tại khu vực trạm thu phí;"}}, {_id:844, properties:{index:"b", content:"không thực hiện việc kết nối, chia sẻ dữ liệu từ trạm thu phí cho cơ quan quản lý nhà nước có thẩm quyền theo quy định."}}, {_id:845, properties:{index:"a", content:"thực hiện hành vi quy định tại điểm a khoản 3 điều này buộc phải bổ sung hoặc sửa chữa các biển báo hiệu bị mất, bị hư hỏng và khắc phục các hư hỏng của công trình đường bộ;"}}, {_id:846, properties:{index:"b", content:"thực hiện hành vi quy định tại điểm b khoản 2; điểm b, điểm c, điểm d khoản 4; khoản 5 điều này buộc phải khôi phục lại tình trạng ban đầu đã bị thay đổi do vi phạm hành chính gây ra.\n\nmục"}}, {_id:1996, properties:{index:"a", content:"các xe thuộc đối tượng bị cân kiểm tra khi đến trạm kiểm tra tải trọng xe phải tuân thủ hướng dẫn của hệ thống báo hiệu đường bộ hoặc nhân viên hướng dẫn, điều tiết giao thông, đi đúng làn đường hoặc phần đường dành riêng để thiết bị cân sơ cấp kiểm tra tải trọng và kích thước; trong vùng đặt thiết bị cân chỉ cho từng xe vào để tránh làm sai lệch kết quả kiểm tra;"}}] AS row
