UNWIND [{_id:1051, properties:{index:"c", content:"chở công-ten-nơ trên xe (kể cả sơ mi rơ moóc) mà không sử dụng thiết bị để định vị chắc chắn công-ten-nơ với xe hoặc có sử dụng thiết bị nhưng công-ten-nơ vẫn bị xê dịch trong quá trình vận chuyển."}}, {_id:1053, properties:{index:"a", content:"điều khiển xe (kể cả rơ moóc và sơ mi rơ moóc) chở hàng vượt trọng tải (khối lượng hàng chuyên chở) cho phép tham gia giao thông được ghi trong giấy chứng nhận kiểm định an toàn kỹ thuật và bảo vệ môi trường của xe trên 30% đến 50%;"}}, {_id:1054, properties:{index:"b", content:"điều khiển xe quá thời gian quy định tại khoản 1 điều 65 của luật giao thông đường bộ;"}}, {_id:1055, properties:{index:"c", content:"điều khiển xe tham gia kinh doanh vận tải hàng hóa có gắn thiết bị giám sát hành trình của xe nhưng thiết bị không hoạt động theo quy định hoặc sử dụng biện pháp kỹ thuật, trang thiết bị ngoại vi, các biện pháp khác làm sai lệch dữ liệu của thiết bị giám sát hành trình của xe ô tô;"}}, {_id:1056, properties:{index:"d", content:"điều khiển xe kéo theo rơ moóc, sơ mi rơ moóc mà khối lượng toàn bộ (bao gồm khối lượng bản thân rơ moóc, sơ mi rơ moóc và khối lượng hàng chuyên chở) của rơ moóc, sơ mi rơ moóc vượt khối lượng cho phép kéo theo được ghi trong giấy chứng nhận kiểm định an toàn kỹ thuật và bảo vệ môi trường của xe trên 30% đến 50%."}}, {_id:1058, properties:{index:"a", content:"điều khiển xe (kể cả rơ moóc và sơ mi rơ moóc) chở hàng vượt trọng tải (khối lượng hàng chuyên chở) cho phép tham gia giao thông được ghi trong giấy chứng nhận kiểm định an toàn kỹ thuật và bảo vệ môi trường của xe trên 50% đến 100%;"}}, {_id:1059, properties:{index:"b", content:"điều khiển xe kéo theo rơ moóc, sơ mi rơ moóc mà khối lượng toàn bộ (bao gồm khối lượng bản thân rơ moóc, sơ mi rơ moóc và khối lượng hàng chuyên chở) của rơ moóc, sơ mi rơ moóc vượt khối lượng cho phép kéo theo được ghi trong giấy chứng nhận kiểm định an toàn kỹ thuật và bảo vệ môi trường của xe trên 50% đến 100%;"}}, {_id:1060, properties:{index:"c", content:"nhận, trả hàng trên đường cao tốc;"}}, {_id:1061, properties:{index:"d", content:"điều khiển xe không có hoặc không gắn phù hiệu theo quy định (đối với loại xe có quy định phải gắn phù hiệu) hoặc có phù hiệu nhưng đã hết giá trị sử dụng hoặc sử dụng phù hiệu không do cơ quan có thẩm quyền cấp."}}, {_id:1063, properties:{index:"a", content:"điều khiển xe (kể cả rơ moóc và sơ mi rơ moóc) chở hàng vượt trọng tải (khối lượng hàng chuyên chở) cho phép tham gia giao thông được ghi trong giấy chứng nhận kiểm định an toàn kỹ thuật và bảo vệ môi trường của xe trên 100% đến 150%;"}}, {_id:1064, properties:{index:"b", content:"điều khiển xe kéo theo rơ moóc, sơ mi rơ moóc mà khối lượng toàn bộ (bao gồm khối lượng bản thân rơ moóc, sơ mi rơ moóc và khối lượng hàng chuyên chở) của rơ moóc, sơ mi rơ moóc vượt khối lượng cho phép kéo theo được ghi trong giấy chứng nhận kiểm định an toàn kỹ thuật và bảo vệ môi trường của xe trên 100% đến 150%."}}, {_id:1066, properties:{index:"a", content:"điều khiển xe (kể cả rơ moóc và sơ mi rơ moóc) chở hàng vượt trọng tải (khối lượng hàng chuyên chở) cho phép tham gia giao thông được ghi trong giấy chứng nhận kiểm định an toàn kỹ thuật và bảo vệ môi trường của xe trên 150%;"}}, {_id:1067, properties:{index:"b", content:"điều khiển xe kéo theo rơ moóc, sơ mi rơ moóc mà khối lượng toàn bộ (bao gồm khối lượng bản thân rơ moóc, sơ mi rơ moóc và khối lượng hàng chuyên chở) của rơ moóc, sơ mi rơ moóc vượt khối lượng cho phép kéo theo được ghi trong giấy chứng nhận kiểm định an toàn kỹ thuật và bảo vệ môi trường của xe trên 150%;"}}, {_id:3138, properties:{index:"a", content:"máy kéo là loại xe gồm phần đầu máy tự di chuyển, được lái bằng càng hoặc vô lăng và rơ moóc được kéo theo (có thể tháo rời với phần đầu kéo);"}}, {_id:1068, properties:{index:"c", content:"vi phạm quy định tại khoản 1, điểm c khoản 4 điều này mà gây tai nạn giao thông."}}, {_id:1069, properties:{index:"a", content:"thực hiện hành v