UNWIND [{_id:3049, properties:{content:"đăng kiểm viên"}}, {_id:3051, properties:{content:"nhân viên nghiệp vụ của trung tâm đăng kiểm"}}, {_id:3052, properties:{content:"hoạt động kiểm định an toàn kỹ thuật"}}, {_id:3053, properties:{content:"bảo vệ môi trường xe cơ giới"}}, {_id:3055, properties:{content:"trung tâm đăng kiểm"}}, {_id:3058, properties:{content:"sản xuất"}}, {_id:3060, properties:{content:"lắp ráp trái phép"}}, {_id:3061, properties:{content:"sản xuất biển số"}}, {_id:3062, properties:{content:"trái phép"}}, {_id:3066, properties:{content:"đổi màu sơn xe"}}, {_id:3068, properties:{content:"dẫn dắt súc vật"}}, {_id:3069, properties:{content:"điều khiển xe súc vật"}}, {_id:3070, properties:{content:"uống rượu"}}] AS row
