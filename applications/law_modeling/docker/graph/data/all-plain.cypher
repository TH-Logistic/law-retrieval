UNWIND [{_id:1025, properties:{index:"a", content:"thực hiện hành vi quy định tại khoản 2, khoản 4 (trường hợp vượt trên 50% đến 100% số người quy định được phép chở của phương tiện); điểm c, điểm d, điểm e khoản 3; điểm a, điểm b, điểm c, điểm d, điểm đ, điểm e, điểm h, điểm i, điểm k, điểm l, điểm m, điểm o, điểm q khoản 5; khoản 6; điểm b khoản 7 điều này bị tước quyền sử dụng giấy phép lái xe từ 01 tháng đến 03 tháng;"}}, {_id:1026, properties:{index:"b", content:"thực hiện hành vi quy định tại điểm a khoản 7 điều này bị tước quyền sử dụng giấy phép lái xe từ 02 tháng đến 04 tháng;"}}, {_id:1027, properties:{index:"c", content:"thực hiện hành vi quy định tại khoản 2, khoản 4 điều này (trường hợp vượt trên 100% số người quy định được phép chở của phương tiện) bị tước quyền sử dụng giấy phép lái xe từ 03 tháng đến 05 tháng;"}}, {_id:1028, properties:{index:"d", content:"thực hiện hành vi quy định tại điểm e khoản 6, điểm b khoản 7 điều này bị tịch thu phù hiệu (biển hiệu) đã hết giá trị sử dụng hoặc không do cơ quan có thẩm quyền cấp."}}, {_id:4, properties:{index:"m", content:"buộc phải ký hợp đồng với lái xe và nhân viên phục vụ trên xe;"}}, {_id:6, properties:{index:"n", content:"buộc phải xây dựng và thực hiện quy trình bảo đảm an toàn giao thông theo quy định;"}}, {_id:1030, properties:{index:"a", content:"thực hiện hành vi quy định tại khoản 2, khoản 4 điều này (trường hợp chở hành khách) buộc phải bố trí phương tiện khác để chở số hành khách vượt quá quy định được phép chở của phương tiện;"}}, {_id:3089, properties:{index:"a", content:"cơ quan nhà nước có hành vi vi phạm mà hành vi đó không thuộc nhiệm vụ quản lý nhà nước được giao;"}}, {_id:3092, properties:{index:"b", content:"đơn vị lực lượng vũ trang nhân dân;"}}, {_id:1031, properties:{index:"b", content:"thực hiện hành vi quy định tại điểm l khoản 3 điều này (trường hợp thu tiền vé cao hơn quy định) buộc phải nộp lại số lợi bất hợp pháp có được do thực hiện vi phạm hành chính."}}, {_id:3102, properties:{index:"c", content:"đơn vị sự nghiệp công lập;"}}, {_id:15, properties:{index:"o", content:"buộc phải bố trí người trực tiếp điều hành hoạt động vận tải đủ điều kiện theo quy định;"}}, {_id:3103, properties:{index:"d", content:"cơ quan đảng cộng sản việt nam;"}}, {_id:3108, properties:{index:"đ", content:"tổ chức chính trị - xã hội, tổ chức chính trị xã hội - nghề nghiệp, tổ chức xã hội, tổ chức xã hội - nghề nghiệp;"}}, {_id:21, properties:{index:"p", content:"buộc phải lắp đặt camera, dây an toàn, đồng hồ tính tiền cước, thiết bị in hóa đơn, thiết bị giám sát hành trình trên xe theo đúng quy định;"}}, {_id:3109, properties:{index:"e", content:"tổ chức kinh tế được thành lập theo quy định của luật doanh nghiệp gồm: doanh nghiệp tư nhân, công ty cổ phần, công ty trách nhiệm hữu hạn, công ty hợp danh và các đơn vị phụ thuộc doanh nghiệp (chi nhánh, văn phòng đại diện);"}}, {_id:23, properties:{index:"q", content:"buộc phải cung cấp, cập nhật, truyền, lưu trữ, quản lý các thông tin từ thiết bị giám sát hành trình, camera lắp trên xe ô tô theo quy định;"}}, {_id:24, properties:{index:"r", content:"buộc phải cung cấp tên đăng nhập, mật khẩu truy cập vào phần mềm xử lý dữ liệu từ thiết bị giám sát hành trình của xe ô tô hoặc máy chủ của đơn vị cho cơ quan có thẩm quyền theo quy định;"}}, {_id:1036, properties:{index:"a", content:"vận chuyển hàng trên xe phải chằng buộc mà không chằng buộc hoặc có chằng buộc nhưng không chắc chắn, trừ hành vi vi phạm quy định tại điểm c khoản 4 điều này;"}}, {_id:1037, properties:{index:"b", content:"điều khiển xe xếp hàng trên nóc buồng lái, xếp hàng làm lệch xe;"}}] AS row
