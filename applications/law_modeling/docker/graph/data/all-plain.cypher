6, properties:{index:"c", content:"sử dụng giấy phép lái xe không hợp lệ (giấy phép lái xe có số phôi ghi ở mặt sau không trùng với số phôi được cấp mới nhất trong hệ thống thông tin quản lý giấy phép lái xe)."}}, {_id:959, properties:{index:"a", content:"có giấy phép lái xe nhưng không phù hợp với loại xe đang điều khiển;"}}, {_id:960, properties:{index:"b", content:"không có giấy phép lái xe hoặc sử dụng giấy phép lái xe không do cơ quan có thẩm quyền cấp, giấy phép lái xe bị tẩy xóa;"}}, {_id:961, properties:{index:"c", content:"có giấy phép lái xe quốc tế do các nước tham gia công ước về giao thông đường bộ năm 1968 cấp (trừ giấy phép lái xe quốc tế do việt nam cấp) nhưng không mang theo giấy phép lái xe quốc gia;"}}, {_id:962, properties:{index:"d", content:"sử dụng giấy phép lái xe không hợp lệ (giấy phép lái xe có số phôi ghi ở mặt sau không trùng với số phôi được cấp mới nhất trong hệ thống thông tin quản lý giấy phép lái xe)."}}, {_id:964, properties:{index:"a", content:"có giấy phép lái xe nhưng không phù hợp với loại xe đang điều khiển hoặc đã hết hạn sử dụng từ 06 tháng trở lên;"}}, {_id:966, properties:{index:"a", content:"thực hiện hành vi quy định tại điểm a, điểm c khoản 5; điểm b, điểm d khoản 7; điểm b, điểm d khoản 8 điều này còn bị áp dụng hình thức xử phạt bổ sung tịch thu giấy phép lái xe không do cơ quan có thẩm quyền cấp, giấy phép lái xe bị tẩy xóa, giấy phép lái xe không hợp lệ;"}}, {_id:967, properties:{index:"b", content:"thực hiện hành vi quy định tại điểm c khoản 5, điểm d khoản 7, điểm d khoản 8 bị tước quyền sử dụng giấy phép lái xe được cấp mới nhất trong hệ thống thông tin quản lý giấy phép lái xe từ 01 tháng đến 03 tháng."}}, {_id:969, properties:{index:"a", content:"người điều khiển xe máy chuyên dùng không đúng độ tuổi hoặc tuổi không phù hợp với ngành nghề theo quy định;"}}, {_id:970, properties:{index:"b", content:"người điều khiển xe máy chuyên dùng không mang theo bằng (hoặc chứng chỉ) điều khiển, chứng chỉ bồi dưỡng kiến thứ