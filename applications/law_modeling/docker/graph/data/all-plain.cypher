UNWIND [{_id:2039, properties:{index:1, content:"các hành vi vi phạm về trật tự an toàn giao thông và bảo vệ công trình giao thông được ghi hình ảnh từ thiết bị ghi hình của tổ chức quản lý, sử dụng thiết bị ghi hình sẽ được chuyển cho lực lượng chức năng có thẩm quyền để xác định vi phạm hành chính và xử phạt vi phạm hành chính theo quy định của pháp luật."}}, {_id:2040, properties:{index:2, content:"người sử dụng thiết bị ghi hình có trách nhiệm trích in hình ảnh vi phạm kèm theo phiếu xác nhận kết quả vi phạm, chuyển cho lực lượng chức năng để làm căn cứ xác định vi phạm hành chính và xử phạt vi phạm hành chính theo quy định."}}, {_id:2041, properties:{index:3, content:"trình tự giao, nhận hình ảnh vi phạm kèm phiếu xác nhận kết quả"}}, {_id:885, properties:{index:1, content:"phạt tiền từ 100.000 đồng đến 200.000 đồng đối với một trong các hành vi vi phạm sau đây:"}}, {_id:2045, properties:{index:4, content:"sau khi nhận được hình ảnh vi phạm và phiếu xác nhận kết quả, lực lượng chức năng thông báo cho tổ chức, cá nhân vi phạm về hành vi vi phạm, yêu cầu đến cơ quan có thẩm quyền để xử phạt vi phạm hành chính theo quy định;"}}, {_id:2047, properties:{index:5, content:"người tiếp nhận phiếu xác nhận kết quả thiết bị ghi hình là người có thẩm quyền lập biên bản vi phạm hành chính theo quy định tại nghị định của chính phủ quy định về xử phạt vi phạm hành chính trong lĩnh vực giao thông đường bộ."}}, {_id:2048, properties:{index:6, content:"đối với những hành vi vi phạm không thuộc thẩm quyền xử phạt của lực lượng thanh tra ngành giao thông vận tải thì lực lượng thanh tra ngành giao thông vận tải có trách nhiệm tiếp nhận và chuyển hồ sơ vụ việc vi phạm cho lực lượng chức năng có thẩm quyền xử phạt theo quy định pháp luật."}}, {_id:2050, properties:{index:1, content:"cá nhân, tổ chức bị xử phạt vi phạm hành chính có quyền được biết thông tin vi phạm thu được từ phương tiện, thiết bị kỹ thuật nghiệp vụ; có quyền khiếu nại theo quy định của pháp luật về khiếu nại."}}, {_id:2051, properties:{index:2, content:"việc khiếu nại và giải quyết khiếu nại thực hiện theo quy định của pháp luật về khiếu nại."}}, {_id:2053, properties:{index:1, content:"triển khai thực hiện thông tư này."}}, {_id:2054, properties:{index:2, content:"xây dựng chương trình, tổ chức tập huấn về quy trình, thao tác sử dụng, bảo quản phương tiện, thiết bị kỹ thuật nghiệp vụ và các quy định của pháp luật về xử phạt vi phạm hành chính trong lĩnh vực đường bộ theo quy định của pháp luật và thông tư này."}}, {_id:2055, properties:{index:3, content:"kiểm tra, giám sát việc sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ và sử dụng kết quả thu được từ phương tiện, thiết bị kỹ thuật nghiệp vụ."}}, {_id:2056, properties:{index:4, content:"tổng hợp, báo cáo bộ giao thông vận tải định kỳ hàng quý, đột xuất việc quản lý, sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ."}}, {_id:899, properties:{index:4, content:"ngoài việc bị phạt tiền, người điều khiển xe thực hiện hành vi vi phạm còn bị áp dụng các hình thức xử phạt bổ sung sau đây:"}}, {_id:2057, properties:{index:5, content:"hướng dẫn sở giao thông vận tải, cục quản lý đường bộ và đơn vị được giao quản lý, khai thác, bảo trì kết cấu hạ tầng giao thông đường bộ biểu mẫu và nội dung báo cáo việc quản lý, sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ; tổ chức ứng dụng công nghệ thông tin để thực hiện công tác thông tin, báo cáo theo quy định tại thông tư này."}}, {_id:906, properties:{index:1, content:"phạt tiền từ 100.000 đồng đến 200.000 đồng đối với hành vi điều khiển xe không có đăng ký, không gắn biển số (đối với loại xe có quy định phải đăng ký và gắn biển số)."}}, {_id:2059, properties:{index:1, content:"bố trí người sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ đáp ứng yêu cầu theo quy định."}}, {_id:2060, properties:{index:2, content:"quản lý, sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ được trang bị để phát hiện vi phạm về tải trọng xe, trật tự an toàn giao thông và bảo vệ kết cấu hạ tầng giao thông đường bộ, bảo đảm hiệu quả, đúng pháp luật."}}, {_id:907, properties:{index:2, content:"phạt tiền từ 200.000 đồng đến 300.000 đồng đối với một trong các hành vi vi phạm sau đây:"}}, {_id:2061, properties:{index:3, content:"phối hợp với tổng cục đường bộ việt nam tổ chức tập huấn nghiệp vụ cho người sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ thuộc thẩm quyền quản lý."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:911, properties:{index:1, content:"phạt tiền từ 300.000 đồng đến 400.000 đồng đối với một trong các hành vi vi phạm sau đây:"}}, {_id:2062, properties:{index:4, content:"tổ chức việc tiếp nhận, sử dụng kết quả thu được từ phương tiện, thiết bị kỹ thuật nghiệp vụ để xác định vi phạm hành chính và xử phạt vi phạm hành chính theo quy định tại thông tư này và pháp luật về xử lý vi phạm hành chính."}}, {_id:917, properties:{index:2, content:"phạt tiền từ 1.000.000 đồng đến 2.000.000 đồng đối với một trong các hành vi vi phạm sau đây:"}}, {_id:2063, properties:{index:5, content:"báo cáo tổng cục đường bộ việt nam định kỳ hàng quý, đột xuất về việc quản lý, sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ theo quy định."}}, {_id:2065, properties:{index:1, content:"phân công rõ nhiệm vụ, bố trí người sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ bảo đảm tiêu chuẩn; phối hợp với tổng cục đường bộ việt nam tổ chức tập huấn cho người sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ thuộc thẩm quyền quản lý."}}, {_id:2066, properties:{index:2, content:"kết nối với phần mềm giám sát, quản lý tập trung dữ liệu tải trọng xe của tổng cục đường bộ việt nam; chuyển thông tin thu được từ phương tiện, thiết bị kỹ thuật nghiệp vụ đến lực lượng chức năng theo quy định."}}, {_id:924, properties:{index:3, content:"ngoài việc bị phạt tiền, người điều khiển phương tiện thực hiện hành vi vi phạm còn bị áp dụng các hình thức xử phạt bổ sung sau đây:"}}, {_id:2067, properties:{index:3, content:"chịu trách nhiệm về tính chính xác của kết quả cân kiểm tra tải trọng xe và hình ảnh thu được từ thiết bị ghi hình cung cấp cho lực lượng chức năng sử dụng làm căn cứ để xác định vi phạm hành chính và xử phạt vi phạm hành chính; có trách nhiệm bảo mật các thông tin liên quan đến người vi phạm theo quy định."}}, {_id:2068, properties:{index:4, content:"phối hợp với người có thẩm quyền tiếp nhận, xử phạt vi phạm chính trong lĩnh vực giao thông đường bộ trong việc giải quyết khiếu nại trong quá trình xử phạt vi phạm hành chính theo quy định."}}, {_id:2071, properties:{index:1, content:"đường bộ gồm đường, cầu đường bộ, hầm đường bộ, bến phà đường bộ."}}, {_id:2072, properties:{index:2, content:"công trình đường bộ gồm đường bộ, nơi dừng xe, đỗ xe trên đường bộ, đèn tín hiệu, biển báo hiệu, vạch kẻ đường, cọc tiêu, rào chắn, đảo giao thông, dải phân cách, cột cây số, tường, kè, hệ thống thoát nước, trạm kiểm tra tải trọng xe, trạm thu phí và các công trình, thiết bị phụ trợ đường bộ khác."}}, {_id:929, properties:{index:4, content:"ngoài việc bị áp dụng hình thức xử phạt, người điều khiển phương tiện thực hiện hành vi vi phạm quy định tại điểm b, điểm c, điểm d khoản 1 điều này còn bị áp dụng biện pháp khắc phục hậu quả: buộc phải lắp đầy đủ thiết bị hoặc thay thế thiết bị đủ tiêu chuẩn an toàn kỹ thuật hoặc khôi phục lại tính năng kỹ thuật của thiết bị theo quy định."}}, {_id:2073, properties:{index:3, content:"kết cấu hạ tầng giao thông đường bộ gồm công trình đường bộ, bến xe, bãi đỗ xe, trạm dừng nghỉ và các công trình phụ trợ khác trên đường bộ phục vụ giao thông và hành lang an toàn đường bộ."}}, {_id:2074, properties:{index:4, content:"đất của đường bộ là phần đất trên đó công trình đường bộ được xây dựng và phần đất dọc hai bên đường bộ để quản lý, bảo trì, bảo vệ công trình đường bộ."}}, {_id:931, properties:{index:1, content:"phạt tiền từ 100.000 đồng đến 200.000 đồng đối với hành vi điều khiển xe không đáp ứng yêu cầu về vệ sinh lưu thông trong đô thị."}}, {_id:932, properties:{index:2, content:"phạt tiền từ 2.000.000 đồng đến 4.000.000 đồng đối với một trong các hành vi vi phạm sau đây:"}}, {_id:2075, properties:{index:5, content:"hành lang an toàn đường bộ là dải đất dọc hai bên đất của đường bộ, tính từ mép ngoài đất của đường bộ ra hai bên để bảo đảm an toàn giao thông đường bộ."}}, {_id:936, properties:{index:3, content:"phạt tiền từ 4.000.000 đồng đến 6.000.000 đồng đối với người điều khiển xe đổ trái phép rác, đất, cát, đá, vật liệu, chất phế thải trong phạm vi đất dành cho đường bộ ở đoạn đường ngoài đô thị."}}, {_id:2076, properties:{index:6, content:"phần đường xe chạy là phần của đường bộ được sử dụng cho phương tiện giao thông qua lại."}}, {_id:937, properties:{index:4, content:"phạt tiền từ 10.000.000 đồng đến 15.000.000 đồng đối với người điều khiển xe thực hiện hành vi đổ trái phép rác, đất, cát, đá, vật liệu, chất phế thải ra đường phố."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2077, properties:{index:7, content:"làn đường là một phần của phần đường xe chạy được chia theo chiều dọc của đường, có bề rộng đủ cho xe chạy an toàn."}}, {_id:2078, properties:{index:8, content:"khổ giới hạn của đường bộ là khoảng trống có kích thước giới hạn về chiều cao, chiều rộng của đường, cầu, bến phà, hầm đường bộ để các xe kể cả hàng hóa xếp trên xe đi qua được an toàn."}}, {_id:2079, properties:{index:9, content:"đường phố là đường đô thị, gồm lòng đường và hè phố."}}, {_id:2080, properties:{index:11, content:"dải phân cách gồm loại cố định và loại di động."}}, {_id:2081, properties:{index:12, content:"nơi đường giao nhau cùng mức (sau đây gọi là nơi đường giao nhau) là nơi hai hay nhiều đường bộ gặp nhau trên cùng một mặt phẳng, gồm cả mặt bằng hình thành vị trí giao nhau đó."}}, {_id:2082, properties:{index:13, content:"đường cao tốc là đường dành cho xe cơ giới, có dải phân cách chia đường cho xe chạy hai chiều riêng biệt; không giao nhau cùng mức với một hoặc các đường khác; được bố trí đầy đủ trang thiết bị phục vụ, bảo đảm giao thông liên tục, an toàn, rút ngắn thời gian hành trình và chỉ cho xe ra, vào ở những điểm nhất định."}}, {_id:938, properties:{index:5, content:"ngoài việc bị phạt tiền, người điều khiển phương tiện thực hiện hành vi vi phạm quy định tại khoản 3, khoản 4 điều này còn bị áp dụng hình thức xử phạt bổ sung tước quyền sử dụng giấy phép lái xe từ 01 tháng đến 03 tháng."}}, {_id:939, properties:{index:6, content:"ngoài việc bị áp dụng hình thức xử phạt, người điều khiển phương tiện thực hiện hành vi vi phạm quy định tại khoản 2, khoản 3, khoản 4 điều này còn bị áp dụng các biện pháp khắc phục hậu quả: buộc phải thu dọn rác, chất phế thải, vật liệu, hàng hóa và khôi phục lại tình trạng ban đầu đã bị thay đổi do vi phạm hành chính gây ra; nếu gây ô nhiễm môi trường phải thực hiện các biện pháp khắc phục tình trạng ô nhiễm môi trường do vi phạm hành chính gây ra."}}, {_id:941, properties:{index:1, content:"phạt cảnh cáo người từ đủ 14 tuổi đến dưới 16 tuổi điều khiển xe mô tô, xe gắn máy (kể cả xe máy điện) và các loại xe tương tự xe mô tô hoặc điều khiển xe ô tô, máy kéo và các loại xe tương tự xe ô tô."}}, {_id:2083, properties:{index:14, content:"đường chính là đường bảo đảm giao thông chủ yếu trong khu vực."}}, {_id:945, properties:{index:3, content:"phạt tiền từ 200.000 đồng đến 400.000 đồng đối với một trong các hành vi vi phạm sau đây:"}}, {_id:2084, properties:{index:15, content:"đường nhánh là đường nối vào đường chính."}}, {_id:2085, properties:{index:16, content:"đường ưu tiên là đường mà trên đó phương tiện tham gia giao thông đường bộ được các phương tiện giao thông đến từ hướng khác nhường đường khi qua nơi đường giao nhau, được cắm biển báo hiệu đường ưu tiên."}}, {_id:2086, properties:{index:17, content:"đường gom là đường để gom hệ thống đường giao thông nội bộ của các khu đô thị, công nghiệp, kinh tế, dân cư, thương mại - dịch vụ và các đường khác vào đường chính hoặc vào đường nhánh trước khi đấu nối vào đường chính."}}, {_id:2087, properties:{index:18, content:"phương tiện giao thông đường bộ gồm phương tiện giao thông cơ giới đường bộ, phương tiện giao thông thô sơ đường bộ."}}, {_id:2088, properties:{index:19, content:"phương tiện giao thông cơ giới đường bộ (sau đây gọi là xe cơ giới) gồm xe ô tô; máy kéo; rơ moóc hoặc sơ mi rơ moóc được kéo bởi xe ô tô, máy kéo; xe mô tô hai bánh; xe mô tô ba bánh; xe gắn máy (kể cả xe máy điện) và các loại xe tương tự."}}, {_id:2089, properties:{index:20, content:"phương tiện giao thông thô sơ đường bộ (sau đây gọi là xe thô sơ) gồm xe đạp (kể cả xe đạp máy), xe xích lô, xe lăn dùng cho người khuyết tật, xe súc vật kéo và các loại xe tương tự."}}, {_id:2090, properties:{index:21, content:"xe máy chuyên dùng gồm xe máy thi công, xe máy nông nghiệp, lâm nghiệp và các loại xe đặc chủng khác sử dụng vào mục đích quốc phòng, an ninh có tham gia giao thông đường bộ."}}, {_id:2091, properties:{index:22, content:"phương tiện tham gia giao thông đường bộ gồm phương tiện giao thông đường bộ và xe máy chuyên dùng."}}, {_id:2092, properties:{index:23, content:"người tham gia giao thông gồm người điều khiển, người sử dụng phương tiện tham gia giao thông đường bộ; người điều khiển, dẫn dắt súc vật; người đi bộ trên đường bộ."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2093, properties:{index:24, content:"người điều khiển phương tiện gồm người điều khiển xe cơ giới, xe thô sơ, xe máy chuyên dùng tham gia giao thông đường bộ."}}, {_id:2094, properties:{index:25, content:"người lái xe là người điều khiển xe cơ giới."}}, {_id:2095, properties:{index:26, content:"người điều khiển giao thông là cảnh sát giao thông; người được giao nhiệm vụ hướng dẫn giao thông tại nơi thi công, nơi ùn tắc giao thông, ở bến phà, tại cầu đường bộ đi chung với đường sắt."}}, {_id:949, properties:{index:4, content:"phạt tiền từ 400.000 đồng đến 600.000 đồng đối với một trong các hành vi vi phạm sau đây:"}}, {_id:2096, properties:{index:27, content:"hành khách là người được chở trên phương tiện vận tải hành khách đường bộ, có trả tiền."}}, {_id:2097, properties:{index:28, content:"hành lý là vật phẩm mà hành khách mang theo trên cùng phương tiện hoặc gửi theo phương tiện khác."}}, {_id:2098, properties:{index:29, content:"hàng hóa là máy móc, thiết bị, nguyên vật liệu, nhiên liệu, hàng tiêu dùng, động vật sống và các động sản khác được vận chuyển bằng phương tiện giao thông đường bộ."}}, {_id:2099, properties:{index:30, content:"hàng nguy hiểm là hàng hóa có chứa các chất nguy hiểm khi chở trên đường có khả năng gây nguy hại tới tính mạng, sức khỏe con người, môi trường, an toàn và an ninh quốc gia."}}, {_id:2100, properties:{index:31, content:"vận tải đường bộ là hoạt động sử dụng phương tiện giao thông đường bộ để vận chuyển người, hàng hóa trên đường bộ."}}, {_id:2101, properties:{index:32, content:"người vận tải là tổ chức, cá nhân sử dụng phương tiện giao thông đường bộ để thực hiện hoạt động vận tải đường bộ."}}, {_id:2102, properties:{index:33, content:"cơ quan quản lý đường bộ là cơ quan thực hiện chức năng quản lý nhà nước chuyên ngành thuộc bộ giao thông vận tải; cơ quan chuyên môn thuộc ủy ban nhân dân tỉnh, thành phố trực thuộc trung ương (sau đây gọi chung là cấp tỉnh), ủy ban nhân dân huyện, quận, thị xã, thành phố thuộc tỉnh (sau đây gọi chung là cấp huyện); ủy ban nhân dân xã, phường, thị trấn (sau đây gọi chung là cấp xã)."}}, {_id:2104, properties:{index:1, content:"hoạt động giao thông đường bộ phải bảo đảm thông suốt, trật tự, an toàn, hiệu quả; góp phần phát triển kinh tế - xã hội, bảo đảm quốc phòng, an ninh và bảo vệ môi trường."}}, {_id:2105, properties:{index:2, content:"phát triển giao thông đường bộ theo quy hoạch, từng bước hiện đại và đồng bộ; gắn kết phương thức vận tải đường bộ với các phương thức vận tải khác."}}, {_id:2106, properties:{index:3, content:"quản lý hoạt động giao thông đường bộ được thực hiện thống nhất trên cơ sở phân công, phân cấp trách nhiệm, quyền hạn cụ thể, đồng thời có sự phối hợp chặt chẽ giữa các bộ, ngành và chính quyền địa phương các cấp."}}, {_id:2107, properties:{index:4, content:"bảo đảm trật tự, an toàn giao thông đường bộ là trách nhiệm của cơ quan, tổ chức, cá nhân."}}, {_id:2108, properties:{index:5, content:"người tham gia giao thông phải có ý thức tự giác, nghiêm chỉnh chấp hành quy tắc giao thông, giữ gìn an toàn cho mình và cho người khác"}}, {_id:953, properties:{index:5, content:"phạt tiền từ 800.000 đồng đến 1.200.000 đồng đối với người điều khiển xe mô tô hai bánh có dung tích xi lanh dưới 175 cm3 và các loại xe tương tự xe mô tô thực hiện một trong các hành vi vi phạm sau đây:"}}, {_id:2109, properties:{index:6, content:"chủ phương tiện và người điều khiển phương tiện phải chịu trách nhiệm trước pháp luật về việc bảo đảm an toàn của phương tiện tham gia giao thông đường bộ."}}, {_id:2110, properties:{index:7, content:"mọi hành vi vi phạm pháp luật giao thông đường bộ phải được phát hiện, ngăn chặn kịp thời, xử lý nghiêm minh, đúng pháp luật."}}, {_id:2112, properties:{index:1, content:"nhà nước tập trung các nguồn lực phát triển giao thông đường bộ, ưu tiên đầu tư phát triển kết cấu hạ tầng giao thông đường bộ ở vùng kinh tế trọng điểm, các thành phố, miền núi, vùng sâu, vùng xa, biên giới, hải đảo, vùng dân tộc thiểu số; có chính sách huy động các nguồn lực để quản lý, bảo trì đường bộ."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2113, properties:{index:2, content:"nhà nước có chính sách ưu tiên phát triển vận tải hành khách công cộng; hạn chế sử dụng phương tiện giao thông cá nhân ở các thành phố."}}, {_id:2114, properties:{index:3, content:"nhà nước khuyến khích, tạo điều kiện cho tổ chức, cá nhân việt nam và nước ngoài đầu tư, kinh doanh khai thác kết cấu hạ tầng giao thông đường bộ và hoạt động vận tải đường bộ; nghiên cứu, ứng dụng khoa học, công nghệ tiên tiến và đào tạo nguồn nhân lực trong lĩnh vực giao thông đường bộ."}}, {_id:957, properties:{index:6, content:"phạt tiền từ 1.200.000 đồng đến 3.000.000 đồng đối với người từ đủ 16 tuổi đến dưới 18 tuổi điều khiển xe ô tô, máy kéo, các loại xe tương tự xe ô tô."}}, {_id:2116, properties:{index:1, content:"quy hoạch giao thông vận tải đường bộ là quy hoạch lĩnh vực chuyên ngành, gồm quy hoạch kết cấu hạ tầng, quy hoạch phương tiện giao thông và vận tải đường bộ."}}, {_id:2117, properties:{index:2, content:"quy hoạch giao thông vận tải đường bộ được lập trên cơ sở chiến lược phát triển kinh tế - xã hội, bảo đảm quốc phòng, an ninh và hội nhập quốc tế, đồng bộ với quy hoạch ngành, lĩnh vực; gắn kết chặt chẽ với quy hoạch các chuyên ngành giao thông vận tải khác."}}, {_id:2118, properties:{index:3, content:"quy hoạch giao thông vận tải đường bộ được lập cho ít nhất 10 năm và định hướng phát triển cho ít nhất 10 năm tiếp theo; được điều chỉnh phù hợp với tình hình phát triển kinh tế - xã hội trong từng giai đoạn"}}, {_id:958, properties:{index:7, content:"phạt tiền từ 3.000.000 đồng đến 4.000.000 đồng đối với người điều khiển xe mô tô hai bánh có dung tích xi lanh từ 175 cm3 trở lên, xe mô tô ba bánh thực hiện một trong các hành vi vi phạm sau đây:"}}, {_id:2119, properties:{index:4, content:"việc điều chỉnh quy hoạch phải bảo đảm tính kế thừa của các quy hoạch đã được phê duyệt.\n\nquy hoạch giao thông vận tải đường bộ sau khi được phê duyệt phải được công bố để cơ quan, tổ chức, cá nhân có liên quan biết, thực hiện và tham gia giám sát."}}, {_id:963, properties:{index:8, content:"phạt tiền từ 4.000.000 đồng đến 6.000.000 đồng đối với người điều khiển xe ô tô, máy kéo và các loại xe tương tự xe ô tô vi phạm một trong các hành vi sau đây:"}}, {_id:2120, properties:{index:5, content:"quy hoạch giao thông vận tải đường bộ phải xác định rõ mục tiêu, quan điểm, tính chất và quy mô phát triển; nhu cầu sử dụng đất, nhu cầu vốn, nguồn vốn, nguồn nhân lực; xác định danh mục các dự án, dự án ưu tiên; đánh giá tác động của quy hoạch; xác định cơ chế, chính sách và giải pháp thực hiện quy hoạch."}}, {_id:965, properties:{index:9, content:"ngoài việc bị phạt tiền, người điều khiển phương tiện thực hiện hành vi vi phạm còn bị áp dụng các hình thức xử phạt bổ sung sau đây:"}}, {_id:2121, properties:{index:6, content:"bộ giao thông vận tải lập quy hoạch giao thông vận tải đường bộ trong phạm vi cả nước, liên vùng, vùng; quy hoạch quốc lộ, đường cao tốc trình thủ tướng chính phủ phê duyệt sau khi có ý kiến của các bộ, cơ quan ngang bộ và uỷ ban nhân dân cấp tỉnh có liên quan."}}, {_id:2122, properties:{index:7, content:"uỷ ban nhân dân cấp tỉnh tổ chức lập, trình hội đồng nhân dân cùng cấp quyết định quy hoạch giao thông vận tải đường bộ do địa phương quản lý, trước khi trình hội đồng nhân dân cùng cấp quyết định phải có ý kiến của bộ giao thông vận tải.\n\nđối với quy hoạch giao thông vận tải đường bộ của thành phố trực thuộc trung ương loại đô thị đặc biệt thì uỷ ban nhân dân thành phố lập, trình hội đồng nhân dân cùng cấp thông qua và phải có ý kiến của bộ giao thông vận tải, bộ xây dựng trước khi trình thủ tướng chính phủ phê duyệt."}}, {_id:2123, properties:{index:8, content:"quy hoạch các công trình kỹ thuật hạ tầng khác phải phù hợp, đồng bộ với quy hoạch kết cấu hạ tầng giao thông đường bộ."}}, {_id:2124, properties:{index:9, content:"nhà nước bảo đảm vốn ngân sách nhà nước và có chính sách huy động các nguồn vốn khác cho công tác lập quy hoạch giao thông vận tải đường bộ."}}, {_id:973, properties:{index:2, content:"phạt tiền từ 600.000 đồng đến 1.000.000 đồng đối với người điều khiển xe máy chuyên dùng không có bằng (hoặc chứng chỉ) điều khiển, chứng chỉ bồi dưỡng kiến thức pháp luật về giao thông đường bộ."}}, {_id:2126, properties:{index:1, content:"cơ quan thông tin, tuyên truyền có trách nhiệm tổ chức tuyên truyền, phổ biến pháp luật về giao thông đường bộ thường xuyên, rộng rãi đến toàn dân."}}, {_id:979, properties:{index:2, content:"phạt tiền từ 400.000 đồng đến 600.000 đồng trên mỗi người vượt quá quy định được phép chở của phương tiện nhưng tổng mức phạt tiền tối đa không vượt quá 40.000.000 đồng đối với người điều khiển xe ô tô chở hành khách, ô tô chở người (trừ xe buýt) thực hiện hành vi vi phạm: chở quá từ 02 người trở lên trên xe đến 9 chỗ, chở quá từ 03 người trở lên trên xe 10 chỗ đến xe 15 chỗ, chở quá từ 04 người trở lên trên xe 16 chỗ đến xe 30 chỗ, chở quá từ 05 người trở lên trên xe trên 30 chỗ, trừ các hành vi vi phạm quy định tại khoản 4 điều này."}}, {_id:980, properties:{index:3, content:"phạt tiền từ 600.000 đồng đến 800.000 đồng đối với một trong các hành vi vi phạm sau đây:"}}, {_id:2127, properties:{index:2, content:"uỷ ban nhân dân các cấp trong phạm vi nhiệm vụ, quyền hạn của mình có trách nhiệm tổ chức tuyên truyền, phổ biến, giáo dục pháp luật về giao thông đường bộ tại địa phương, có hình thức tuyên truyền, phổ biến phù hợp đến đồng bào các dân tộc thiểu số."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2128, properties:{index:3, content:"cơ quan quản lý nhà nước về giáo dục và đào tạo có trách nhiệm đưa pháp luật về giao thông đường bộ vào chương trình giảng dạy trong nhà trường và các cơ sở giáo dục khác phù hợp với từng ngành học, cấp học."}}, {_id:2129, properties:{index:4, content:"mặt trận tổ quốc việt nam và các tổ chức thành viên của mặt trận có trách nhiệm phối hợp với cơ quan hữu quan và chính quyền địa phương tuyên truyền, vận động nhân dân thực hiện pháp luật về giao thông đường bộ."}}, {_id:2130, properties:{index:5, content:"cơ quan, tổ chức có trách nhiệm tổ chức tuyên truyền, phổ biến, giáo dục pháp luật về giao thông đường bộ cho cán bộ, chiến sĩ, công chức, viên chức, người lao động khác thuộc thẩm quyền quản lý.\n\nthành viên trong gia đình có trách nhiệm tuyên truyền, giáo dục, nhắc nhở thành viên khác chấp hành pháp luật về giao thông đường bộ."}}, {_id:2132, properties:{index:1, content:"phá hoại đường, cầu, hầm, bến phà đường bộ, đèn tín hiệu, cọc tiêu, biển báo hiệu, gương cầu, dải phân cách, hệ thống thoát nước và các công trình, thiết bị khác thuộc kết cấu hạ tầng giao thông đường bộ."}}, {_id:2133, properties:{index:2, content:"đào, khoan, xẻ đường trái phép; đặt, để chướng ngại vật trái phép trên đường; đặt, rải vật nhọn, đổ chất gây trơn trên đường; để trái phép vật liệu, phế thải, thải rác ra đường; mở đường, đấu nối trái phép vào đường chính; lấn, chiếm hoặc sử dụng trái phép đất của đường bộ, hành lang an toàn đường bộ; tự ý tháo mở nắp cống, tháo dỡ, di chuyển trái phép hoặc làm sai lệch công trình đường bộ."}}, {_id:2134, properties:{index:3, content:"sử dụng lòng đường, lề đường, hè phố trái phép."}}, {_id:2135, properties:{index:4, content:"đưa xe cơ giới, xe máy chuyên dùng không bảo đảm tiêu chuẩn an toàn kỹ thuật và bảo vệ môi trường tham gia giao thông đường bộ."}}, {_id:2136, properties:{index:5, content:"thay đổi tổng thành, linh kiện, phụ kiện xe cơ giới để tạm thời đạt tiêu chuẩn kỹ thuật của xe khi đi kiểm định."}}, {_id:2137, properties:{index:6, content:"đua xe, cổ vũ đua xe, tổ chức đua xe trái phép, lạng lách, đánh võng."}}, {_id:2138, properties:{index:7, content:"điều khiển phương tiện giao thông đường bộ mà trong cơ thể có chất ma túy."}}, {_id:2139, properties:{index:8, content:"điều khiển xe ô tô, máy kéo, xe máy chuyên dùng trên đường mà trong máu hoặc hơi thở có nồng độ cồn.\n\nđiều khiển xe mô tô, xe gắn máy mà trong máu có nồng độ cồn vượt quá 50 miligam/100 mililít máu hoặc 0,25 miligam/1 lít khí thở."}}, {_id:2140, properties:{index:9, content:"điều khiển xe cơ giới không có giấy phép lái xe theo quy định.\n\nđiều khiển xe máy chuyên dùng tham gia giao thông đường bộ không có chứng chỉ bồi dưỡng kiến thức pháp luật về giao thông đường bộ, bằng hoặc chứng chỉ điều khiển xe máy chuyên dùng."}}, {_id:2141, properties:{index:10, content:"giao xe cơ giới, xe máy chuyên dùng cho người không đủ điều kiện để điều khiển xe tham gia giao thông đường bộ."}}, {_id:2142, properties:{index:11, content:"điều khiển xe cơ giới chạy quá tốc độ quy định, giành đường, vượt ẩu."}}, {_id:2143, properties:{index:12, content:"bấm còi, rú ga liên tục; bấm còi trong thời gian từ 22 giờ đến 5 giờ, bấm còi hơi, sử dụng đèn chiếu xa trong đô thị và khu đông dân cư, trừ các xe được quyền ưu tiên đang đi làm nhiệm vụ theo quy định của luật này."}}, {_id:2144, properties:{index:13, content:"lắp đặt, sử dụng còi, đèn không đúng thiết kế của nhà sản xuất đối với từng loại xe cơ giới; sử dụng thiết bị âm thanh gây mất trật tự an toàn giao thông, trật tự công cộng."}}, {_id:2145, properties:{index:14, content:"vận chuyển hàng cấm lưu thông, vận chuyển trái phép hoặc không thực hiện đầy đủ các quy định về vận chuyển hàng nguy hiểm, động vật hoang dã."}}, {_id:2146, properties:{index:15, content:"đe dọa, xúc phạm, tranh giành, lôi kéo hành khách; bắt ép hành khách sử dụng dịch vụ ngoài ý muốn; chuyển tải, xuống khách hoặc các hành vi khác nhằm trốn tránh phát hiện xe chở quá tải, quá số người quy định."}}, {_id:2147, properties:{index:16, content:"kinh doanh vận tải bằng xe ô tô khi không đáp ứng đủ điều kiện kinh doanh theo quy định."}}, {_id:2148, properties:{index:17, content:"bỏ trốn sau khi gây tai nạn để trốn tránh trách nhiệm."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:996, properties:{index:4, content:"phạt tiền từ 1.000.000 đồng đến 2.000.000 đồng trên mỗi người vượt quá quy định được phép chở của phương tiện nhưng tổng mức phạt tiền tối đa không vượt quá 40.000.000 đồng đối với người điều khiển xe ô tô chở hành khách chạy tuyến có cự ly lớn hơn 300 km thực hiện hành vi vi phạm: chở quá từ 02 người trở lên trên xe đến 9 chỗ, chở quá từ 03 người trở lên trên xe 10 chỗ đến xe 15 chỗ, chở quá từ 04 người trở lên trên xe 16 chỗ đến xe 30 chỗ, chở quá từ 05 người trở lên trên xe trên 30 chỗ."}}, {_id:2149, properties:{index:18, content:"khi có điều kiện mà cố ý không cứu giúp người bị tai nạn giao thông."}}, {_id:997, properties:{index:5, content:"phạt tiền từ 1.000.000 đồng đến 2.000.000 đồng đối với một trong các hành vi vi phạm sau đây:"}}, {_id:2150, properties:{index:19, content:"xâm phạm tính mạng, sức khỏe, tài sản của người bị nạn và người gây tai nạn."}}, {_id:2151, properties:{index:20, content:"lợi dụng việc xảy ra tai nạn giao thông để hành hung, đe dọa, xúi giục, gây sức ép, làm mất trật tự, cản trở việc xử lý tai nạn giao thông."}}, {_id:2152, properties:{index:21, content:"lợi dụng chức vụ, quyền hạn, nghề nghiệp của bản thân hoặc người khác để vi phạm pháp luật về giao thông đường bộ."}}, {_id:2153, properties:{index:22, content:"sản xuất, sử dụng trái phép hoặc mua, bán biển số xe cơ giới, xe máy chuyên dùng."}}, {_id:2154, properties:{index:23, content:"hành vi vi phạm quy tắc giao thông đường bộ, hành vi khác gây nguy hiểm cho người và phương tiện tham gia giao thông đường bộ."}}, {_id:2156, properties:{index:1, content:"người tham gia giao thông phải đi bên phải theo chiều đi của mình, đi đúng làn đường, phần đường quy định và phải chấp hành hệ thống báo hiệu đường bộ."}}, {_id:2157, properties:{index:2, content:"xe ô tô có trang bị dây an toàn thì người lái xe và người ngồi hàng ghế phía trước trong xe ô tô phải thắt dây an toàn."}}, {_id:2159, properties:{index:1, content:"hệ thống báo hiệu đường bộ gồm hiệu lệnh của người điều khiển giao thông; tín hiệu đèn giao thông, biển báo hiệu, vạch kẻ đường, cọc tiêu hoặc tường bảo vệ, rào chắn."}}, {_id:2160, properties:{index:2, content:"hiệu lệnh của người điều khiển giao thông quy định như sau:"}}, {_id:2164, properties:{index:3, content:"tín hiệu đèn giao thông có ba mầu, quy định như sau:"}}, {_id:2168, properties:{index:4, content:"biển báo hiệu đường bộ gồm năm nhóm, quy định như sau:"}}, {_id:1014, properties:{index:6, content:"phạt tiền từ 3.000.000 đồng đến 5.000.000 đồng đối với một trong các hành vi vi phạm sau đây:"}}, {_id:2174, properties:{index:5, content:"vạch kẻ đường là vạch chỉ sự phân chia làn đường, vị trí hoặc hướng đi, vị trí dừng lại."}}, {_id:2175, properties:{index:6, content:"cọc tiêu hoặc tường bảo vệ được đặt ở mép các đoạn đường nguy hiểm để hướng dẫn cho người tham gia giao thông biết phạm vi an toàn của nền đường và hướng đi của đường."}}, {_id:2176, properties:{index:7, content:"rào chắn được đặt ở nơi đường bị thắt hẹp, đầu cầu, đầu cống, đầu đoạn đường cấm, đường cụt không cho xe, người qua lại hoặc đặt ở những nơi cần điều khiển, kiểm soát sự đi lại."}}, {_id:2177, properties:{index:8, content:"bộ trưởng bộ giao thông vận tải quy định cụ thể về báo hiệu đường bộ."}}, {_id:2179, properties:{index:1, content:"người tham gia giao thông phải chấp hành hiệu lệnh và chỉ dẫn của hệ thống báo hiệu đường bộ."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2180, properties:{index:2, content:"khi có người điều khiển giao thông thì người tham gia giao thông phải chấp hành hiệu lệnh của người điều khiển giao thông."}}, {_id:1021, properties:{index:7, content:"phạt tiền từ 5.000.000 đồng đến 7.000.000 đồng đối với một trong các hành vi vi phạm sau đây:"}}, {_id:2181, properties:{index:3, content:"tại nơi có biển báo hiệu cố định lại có báo hiệu tạm thời thì người tham gia giao thông phải chấp hành hiệu lệnh của báo hiệu tạm thời."}}, {_id:2182, properties:{index:4, content:"tại nơi có vạch kẻ đường dành cho người đi bộ, người điều khiển phương tiện phải quan sát, giảm tốc độ và nhường đường cho người đi bộ, xe lăn của người khuyết tật qua đường.\n\nnhững nơi không có vạch kẻ đường cho người đi bộ, người điều khiển phương tiện phải quan sát, nếu thấy người đi bộ, xe lăn của người khuyết tật đang qua đường thì phải giảm tốc độ, nhường đường cho người đi bộ, xe lăn của người khuyết tật qua đường bảo đảm an toàn."}}, {_id:2184, properties:{index:1, content:"người lái xe, người điều khiển xe máy chuyên dùng phải tuân thủ quy định về tốc độ xe chạy trên đường và phải giữ một khoảng cách an toàn đối với xe chạy liền trước xe của mình; ở nơi có biển báo \"cự ly tối thiểu giữa hai xe\" phải giữ khoảng cách không nhỏ hơn số ghi trên biển báo."}}, {_id:2185, properties:{index:2, content:"bộ trưởng bộ giao thông vận tải quy định về tốc độ xe và việc đặt biển báo tốc độ; tổ chức thực hiện đặt biển báo tốc độ trên các tuyến quốc lộ."}}, {_id:2186, properties:{index:3, content:"chủ tịch uỷ ban nhân dân cấp tỉnh tổ chức thực hiện việc đặt biển báo tốc độ trên các tuyến đường do địa phương quản lý."}}, {_id:2188, properties:{index:1, content:"trên đường có nhiều làn đường cho xe đi cùng chiều được phân biệt bằng vạch kẻ phân làn đường, người điều khiển phương tiện phải cho xe đi trong một làn đường và chỉ được chuyển làn đường ở những nơi cho phép; khi chuyển làn đường phải có tín hiệu báo trước và phải bảo đảm an toàn."}}, {_id:2189, properties:{index:2, content:"trên đường một chiều có vạch kẻ phân làn đường, xe thô sơ phải đi trên làn đường bên phải trong cùng, xe cơ giới, xe máy chuyên dùng đi trên làn đường bên trái."}}, {_id:2190, properties:{index:3, content:"phương tiện tham gia giao thông đường bộ di chuyển với tốc độ thấp hơn phải đi về bên phải."}}, {_id:2192, properties:{index:1, content:"xe xin vượt phải có báo hiệu bằng đèn hoặc còi; trong đô thị và khu đông dân cư từ 22 giờ đến 5 giờ chỉ được báo hiệu xin vượt bằng đèn."}}, {_id:2193, properties:{index:2, content:"xe xin vượt chỉ được vượt khi không có chướng ngại vật phía trước, không có xe chạy ngược chiều trong đoạn đường định vượt, xe chạy trước không có tín hiệu vượt xe khác và đã tránh về bên phải."}}, {_id:2194, properties:{index:3, content:"khi có xe xin vượt, nếu đủ điều kiện an toàn, người điều khiển phương tiện phía trước phải giảm tốc độ, đi sát về bên phải của phần đường xe chạy cho đến khi xe sau đã vượt qua, không được gây trở ngại đối với xe xin vượt."}}, {_id:2195, properties:{index:4, content:"khi vượt, các xe phải vượt về bên trái, trừ các trường hợp sau đây thì được phép vượt bên phải:"}}, {_id:2199, properties:{index:5, content:"không được vượt xe khi có một trong các trường hợp sau đây:"}}, {_id:2207, properties:{index:1, content:"khi muốn chuyển hướng, người điều khiển phương tiện phải giảm tốc độ và có tín hiệu báo hướng rẽ."}}, {_id:2208, properties:{index:2, content:"trong khi chuyển hướng, người lái xe, người điều khiển xe máy chuyên dùng phải nhường quyền đi trước cho người đi bộ, người đi xe đạp đang đi trên phần đường dành riêng cho họ, nhường đường cho các xe đi ngược chiều và chỉ cho xe chuyển hướng khi quan sát thấy không gây trở ngại hoặc nguy hiểm cho người và phương tiện khác."}}, {_id:2209, properties:{index:3, content:"trong khu dân cư, người lái xe, người điều khiển xe máy chuyên dùng chỉ được quay đầu xe ở nơi đường giao nhau và nơi có biển báo cho phép quay đầu xe."}}, {_id:2210, properties:{index:4, content:"không được quay đầu xe ở phần đường dành cho người đi bộ qua đường, trên cầu, đầu cầu, gầm cầu vượt, ngầm, trong hầm đường bộ, đường cao tốc, tại nơi đường bộ giao nhau cùng mức với đường sắt, đường hẹp, đường dốc, đoạn đường cong tầm nhìn bị che khuất."}}, {_id:2212, properties:{index:1, content:"khi lùi xe, người điều khiển phải quan sát phía sau, có tín hiệu cần thiết và chỉ khi nào thấy không nguy hiểm mới được lùi."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2213, properties:{index:2, content:"không được lùi xe ở khu vực cấm dừng, trên phần đường dành cho người đi bộ qua đường, nơi đường bộ giao nhau, đường bộ giao nhau cùng mức với đường sắt, nơi tầm nhìn bị che khuất, trong hầm đường bộ, đường cao tốc."}}, {_id:2215, properties:{index:1, content:"trên đường không phân chia thành hai chiều xe chạy riêng biệt, hai xe đi ngược chiều tránh nhau, người điều khiển phải giảm tốc độ và cho xe đi về bên phải theo chiều xe chạy của mình."}}, {_id:2216, properties:{index:2, content:"các trường hợp nhường đường khi tránh nhau quy định như sau:"}}, {_id:2220, properties:{index:3, content:"xe cơ giới đi ngược chiều gặp nhau không được dùng đèn chiếu xa."}}, {_id:2222, properties:{index:1, content:"dừng xe là trạng thái đứng yên tạm thời của phương tiện giao thông trong một khoảng thời gian cần thiết đủ để cho người lên, xuống phương tiện, xếp dỡ hàng hóa hoặc thực hiện công việc khác."}}, {_id:2223, properties:{index:2, content:"đỗ xe là trạng thái đứng yên của phương tiện giao thông không giới hạn thời gian."}}, {_id:2224, properties:{index:3, content:"người điều khiển phương tiện khi dừng xe, đỗ xe trên đường bộ phải thực hiện quy định sau đây:"}}, {_id:2232, properties:{index:4, content:"người điều khiển phương tiện không được dừng xe, đỗ xe tại các vị trí sau đây:"}}, {_id:2245, properties:{index:1, content:"phải cho xe dừng, đỗ sát theo lề đường, hè phố phía bên phải theo chiều đi của mình; bánh xe gần nhất không được cách xa lề đường, hè phố quá 0,25 mét và không gây cản trở, nguy hiểm cho giao thông"}}, {_id:2246, properties:{index:2, content:"trường hợp đường phố hẹp, phải dừng xe, đỗ xe ở vị trí cách xe ô tô đang đỗ bên kia đường tối thiểu 20 mét."}}, {_id:2247, properties:{index:3, content:"không được dừng xe, đỗ xe trên đường xe điện, trên miệng cống thoát nước, miệng hầm của đường điện thoại, điện cao thế, chỗ dành riêng cho xe chữa cháy lấy nước"}}, {_id:2248, properties:{index:4, content:"không được để phương tiện giao thông ở lòng đường, hè phố trái quy định."}}, {_id:2250, properties:{index:1, content:"hàng hóa xếp trên xe phải gọn gàng, chằng buộc chắc chắn, không để rơi vãi dọc đường, không kéo lê hàng hóa trên mặt đường và không cản trở việc điều khiển xe."}}, {_id:2251, properties:{index:2, content:"khi xếp hàng hóa vượt phía trước và phía sau xe thì ban ngày phải có cờ báo hiệu màu đỏ, ban đêm hoặc khi trời tối phải có đèn đỏ báo hiệu."}}, {_id:2252, properties:{index:3, content:"bộ trưởng bộ giao thông vận tải quy định cụ thể việc xếp hàng hóa trên phương tiện giao thông đường bộ."}}, {_id:2254, properties:{index:1, content:"chỉ được chở người trên xe ô tô chở hàng trong các trường hợp sau đây:"}}, {_id:2258, properties:{index:2, content:"xe ô tô chở người trong các trường hợp quy định tại khoản 1 điều này phải có thùng cố định, bảo đảm an toàn khi tham gia giao thông."}}, {_id:2260, properties:{index:1, content:"những xe sau đây được quyền ưu tiên đi trước xe khác khi qua đường giao nhau từ bất kỳ hướng nào tới theo thứ tự:"}}, {_id:2266, properties:{index:2, content:"xe quy định tại các điểm a, b, c và d khoản 1 điều này khi đi làm nhiệm vụ phải có tín hiệu còi, cờ, đèn theo quy định; không bị hạn chế tốc độ; được phép đi vào đường ngược chiều, các đường khác có thể đi được, kể cả khi có tín hiệu đèn đỏ và chỉ phải tuân theo chỉ dẫn của người điều khiển giao thông.\n\nchính phủ quy định cụ thể tín hiệu của xe được quyền ưu tiên."}}, {_id:2267, properties:{index:3, content:"khi có tín hiệu của xe được quyền ưu tiên, người tham gia giao thông phải nhanh chóng giảm tốc độ, tránh hoặc dừng lại sát lề đường bên phải để nhường đường"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2268, properties:{index:4, content:"không được gây cản trở xe được quyền ưu tiên."}}, {_id:2270, properties:{index:1, content:"khi đến bến phà, cầu phao, các xe phải xếp hàng trật tự, đúng nơi quy định, không làm cản trở giao thông."}}, {_id:2271, properties:{index:2, content:"khi xuống phà, đang ở trên phà và khi lên bến, mọi người phải xuống xe, trừ người điều khiển xe cơ giới, xe máy chuyên dùng, người bệnh, người già yếu và người khuyết tật."}}, {_id:2272, properties:{index:3, content:"xe cơ giới, xe máy chuyên dùng phải xuống phà trước, xe thô sơ, người đi bộ xuống phà sau; khi lên bến, người đi bộ lên trước, các phương tiện giao thông lên sau theo hướng dẫn của người điều khiển giao thông."}}, {_id:2273, properties:{index:4, content:"thứ tự ưu tiên qua phà, qua cầu phao quy định như sau:"}}, {_id:2279, properties:{index:1, content:"tại nơi đường giao nhau không có báo hiệu đi theo vòng xuyến, phải nhường đường cho xe đi đến từ bên phải;"}}, {_id:2280, properties:{index:2, content:"tại nơi đường giao nhau có báo hiệu đi theo vòng xuyến, phải nhường đường cho xe đi bên trái;"}}, {_id:2281, properties:{index:3, content:"tại nơi đường giao nhau giữa đường không ưu tiên và đường ưu tiên hoặc giữa đường nhánh và đường chính thì xe đi từ đường không ưu tiên hoặc đường nhánh phải nhường đường cho xe đi trên đường ưu tiên hoặc đường chính từ bất kỳ hướng nào tới."}}, {_id:2283, properties:{index:1, content:"trên đoạn đường bộ giao nhau cùng mức với đường sắt, cầu đường bộ đi chung với đường sắt, phương tiện giao thông đường sắt được quyền ưu tiên đi trước."}}, {_id:2284, properties:{index:2, content:"tại nơi đường bộ giao nhau cùng mức với đường sắt có đèn tín hiệu, rào chắn và chuông báo hiệu, khi đèn tín hiệu mầu đỏ đã bật sáng, có tiếng chuông báo hiệu, rào chắn đang dịch chuyển hoặc đã đóng, người tham gia giao thông đường bộ phải dừng lại phía phần đường của mình và cách rào chắn một khoảng cách an toàn; khi đèn tín hiệu đã tắt, rào chắn mở hết, tiếng chuông báo hiệu ngừng mới được đi qua."}}, {_id:2285, properties:{index:3, content:"tại nơi đường bộ giao nhau cùng mức với đường sắt chỉ có đèn tín hiệu hoặc chuông báo hiệu, khi đèn tín hiệu mầu đỏ đã bật sáng hoặc có tiếng chuông báo hiệu, người tham gia giao thông đường bộ phải dừng ngay lại và giữ khoảng cách tối thiểu 5 mét tính từ ray gần nhất; khi đèn tín hiệu đã tắt hoặc tiếng chuông báo hiệu ngừng mới được đi qua."}}, {_id:2286, properties:{index:4, content:"tại nơi đường bộ giao nhau cùng mức với đường sắt không có đèn tín hiệu, rào chắn và chuông báo hiệu, người tham gia giao thông đường bộ phải quan sát cả hai phía, khi thấy chắc chắn không có phương tiện đường sắt đang đi tới mới được đi qua, nếu thấy có phương tiện đường sắt đang đi tới thì phải dừng lại và giữ khoảng cách tối thiểu 5 mét tính từ ray gần nhất và chỉ khi phương tiện đường sắt đã đi qua mới được đi."}}, {_id:2287, properties:{index:5, content:"khi phương tiện tham gia giao thông đường bộ bị hư hỏng tại nơi đường bộ giao nhau cùng mức với đường sắt hoặc trong phạm vi an toàn đường sắt thì người điều khiển phương tiện phải bằng mọi cách nhanh nhất đặt báo hiệu trên đường sắt cách tối thiểu 500 mét về hai phía để báo cho người điều khiển phương tiện đường sắt và tìm cách báo cho người quản lý đường sắt, nhà ga nơi gần nhất, đồng thời phải bằng mọi biện pháp nhanh chóng đưa phương tiện ra khỏi phạm vi an toàn đường sắt."}}, {_id:2288, properties:{index:6, content:"những người có mặt tại nơi phương tiện tham gia giao thông đường bộ bị hư hỏng trên đoạn đường bộ giao nhau cùng mức với đường sắt có trách nhiệm giúp đỡ người điều khiển phương tiện đưa phương tiện ra khỏi phạm vi an toàn đường sắt."}}, {_id:2290, properties:{index:1, content:"người lái xe, người điều khiển xe máy chuyên dùng trên đường cao tốc ngoài việc tuân thủ các quy tắc giao thông quy định tại luật này còn phải thực hiện các quy định sau đây:"}}, {_id:2295, properties:{index:2, content:"người lái xe, người điều khiển xe máy chuyên dùng phải cho xe chạy cách nhau một khoảng cách an toàn ghi trên biển báo hiệu."}}, {_id:2296, properties:{index:3, content:"chỉ được dừng xe, đỗ xe ở nơi quy định; trường hợp buộc phải dừng xe, đỗ xe không đúng nơi quy định thì người lái xe phải đưa xe ra khỏi phần đường xe chạy, nếu không thể được thì phải báo hiệu để người lái xe khác biết."}}, {_id:2297, properties:{index:4, content:"người đi bộ, xe thô sơ, xe gắn máy, xe mô tô và máy kéo; xe máy chuyên dùng có tốc độ thiết kế nhỏ hơn 70 km/h không được đi vào đường cao tốc, trừ người, phương tiện, thiết bị phục vụ việc quản lý, bảo trì đường cao tốc."}}, {_id:2299, properties:{index:1, content:"xe cơ giới, xe máy chuyên dùng phải bật đèn; xe thô sơ phải bật đèn hoặc có vật phát sáng báo hiệu;"}}, {_id:2300, properties:{index:2, content:"chỉ được dừng xe, đỗ xe ở nơi quy định."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2302, properties:{index:1, content:"người điều khiển phương tiện phải tuân thủ các quy định về tải trọng, khổ giới hạn của đường bộ và chịu sự kiểm tra của cơ quan có thẩm quyền."}}, {_id:2303, properties:{index:2, content:"trường hợp đặc biệt, xe quá tải trọng, quá khổ giới hạn của đường bộ, xe bánh xích gây hư hại mặt đường có thể được lưu hành trên đường nhưng phải được cơ quan quản lý đường bộ có thẩm quyền cấp giấy phép và phải thực hiện các biện pháp bắt buộc để bảo vệ đường bộ, bảo đảm an toàn giao thông."}}, {_id:2304, properties:{index:3, content:"bộ trưởng bộ giao thông vận tải quy định về tải trọng, khổ giới hạn của đường bộ và công bố về tải trọng, khổ giới hạn của quốc lộ; quy định việc cấp giấy phép lưu hành cho xe quá tải trọng, quá khổ giới hạn của đường bộ, xe bánh xích gây hư hại mặt đường."}}, {_id:2305, properties:{index:4, content:"chủ tịch uỷ ban nhân dân cấp tỉnh công bố về tải trọng, khổ giới hạn của đường bộ do địa phương quản lý."}}, {_id:2307, properties:{index:1, content:"một xe ô tô chỉ được kéo theo một xe ô tô hoặc xe máy chuyên dùng khác khi xe này không tự chạy được và phải bảo đảm các quy định sau đây:"}}, {_id:2311, properties:{index:2, content:"xe kéo rơ moóc phải có tổng trọng lượng lớn hơn tổng trọng lượng của rơ moóc hoặc phải có hệ thống hãm có hiệu lực cho rơ moóc."}}, {_id:2312, properties:{index:3, content:"không được thực hiện các hành vi sau đây:"}}, {_id:2317, properties:{index:1, content:"người điều khiển xe mô tô hai bánh, xe gắn máy chỉ được chở một người, trừ những trường hợp sau thì được chở tối đa hai người:"}}, {_id:2321, properties:{index:2, content:"người điều khiển, người ngồi trên xe mô tô hai bánh, xe mô tô ba bánh, xe gắn máy phải đội mũ bảo hiểm có cài quai đúng quy cách."}}, {_id:2322, properties:{index:3, content:"người điều khiển xe mô tô hai bánh, xe mô tô ba bánh, xe gắn máy không được thực hiện các hành vi sau đây:"}}, {_id:2329, properties:{index:4, content:"người ngồi trên xe mô tô hai bánh, xe mô tô ba bánh, xe gắn máy khi tham gia giao thông không được thực hiện các hành vi sau đây:"}}, {_id:2336, properties:{index:1, content:"người điều khiển xe đạp chỉ được chở một người, trừ trường hợp chở thêm một trẻ em dưới 7 tuổi thì được chở tối đa hai người.\n\nngười điều khiển xe đạp phải thực hiện quy định tại khoản 3 điều 30 của luật này; người ngồi trên xe đạp khi tham gia giao thông phải thực hiện quy định tại khoản 4 điều 30 của luật này."}}, {_id:2337, properties:{index:2, content:"người điều khiển, người ngồi trên xe đạp máy phải đội mũ bảo hiểm có cài quai đúng quy cách."}}, {_id:2338, properties:{index:3, content:"người điều khiển xe thô sơ khác phải cho xe đi hàng một, nơi có phần đường dành cho xe thô sơ thì phải đi đúng phần đường quy định; khi đi ban đêm phải có báo hiệu ở phía trước và phía sau xe"}}, {_id:2339, properties:{index:4, content:"người điều khiển xe súc vật kéo phải có biện pháp bảo đảm vệ sinh trên đường."}}, {_id:2340, properties:{index:5, content:"hàng hóa xếp trên xe thô sơ phải bảo đảm an toàn, không gây cản trở giao thông và che khuất tầm nhìn của người điều khiển."}}, {_id:2342, properties:{index:1, content:"người đi bộ phải đi trên hè phố, lề đường; trường hợp đường không có hè phố, lề đường thì người đi bộ phải đi sát mép đường."}}, {_id:2343, properties:{index:2, content:"người đi bộ chỉ được qua đường ở những nơi có đèn tín hiệu, có vạch kẻ đường hoặc có cầu vượt, hầm dành cho người đi bộ và phải tuân thủ tín hiệu chỉ dẫn."}}, {_id:2344, properties:{index:3, content:"trường hợp không có đèn tín hiệu, không có vạch kẻ đường, cầu vượt, hầm dành cho người đi bộ thì người đi bộ phải quan sát các xe đang đi tới, chỉ qua đường khi bảo đảm an toàn và chịu trách nhiệm bảo đảm an toàn khi qua đường."}}, {_id:2345, properties:{index:4, content:"người đi bộ không được vượt qua dải phân cách, không đu bám vào phương tiện giao thông đang chạy; khi mang vác vật cồng kềnh phải bảo đảm an toàn và không gây trở ngại cho người và phương tiện tham gia giao thông đường bộ."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2346, properties:{index:5, content:"trẻ em dưới 7 tuổi khi đi qua đường đô thị, đường thường xuyên có xe cơ giới qua lại phải có người lớn dắt; mọi người có trách nhiệm giúp đỡ trẻ em dưới 7 tuổi khi đi qua đường."}}, {_id:2348, properties:{index:1, content:"người khuyết tật sử dụng xe lăn không có động cơ được đi trên hè phố và nơi có vạch kẻ đường dành cho người đi bộ."}}, {_id:2349, properties:{index:2, content:"người khiếm thị khi đi trên đường bộ phải có người dắt hoặc có công cụ để báo hiệu cho người khác nhận biết đó là người khiếm thị."}}, {_id:2350, properties:{index:3, content:"mọi người có trách nhiệm giúp đỡ người khuyết tật, người già yếu khi đi qua đường."}}, {_id:2352, properties:{index:1, content:"người dẫn dắt súc vật đi trên đường bộ phải cho súc vật đi sát mép đường và bảo đảm vệ sinh trên đường; trường hợp cần cho súc vật đi ngang qua đường thì phải quan sát và chỉ được đi qua đường khi có đủ điều kiện an toàn."}}, {_id:2353, properties:{index:2, content:"không được dẫn dắt súc vật đi vào phần đường dành cho xe cơ giới."}}, {_id:2355, properties:{index:1, content:"tổ chức hoạt động văn hóa, thể thao, diễu hành, lễ hội trên đường bộ thực hiện theo quy định sau đây:"}}, {_id:2359, properties:{index:2, content:"không được thực hiện các hành vi sau đây:"}}, {_id:2370, properties:{index:1, content:"lòng đường và hè phố chỉ được sử dụng cho mục đích giao thông."}}, {_id:2371, properties:{index:2, content:"các hoạt động khác trên đường phố phải thực hiện theo quy định tại khoản 1 điều 35 của luật này, trường hợp đặc biệt, việc sử dụng tạm thời một phần lòng đường, hè phố vào mục đích khác do ủy ban nhân dân cấp tỉnh quy định nhưng không được làm ảnh hưởng đến trật tự, an toàn giao thông."}}, {_id:2376, properties:{index:1, content:"tổ chức giao thông gồm các nội dung sau đây:"}}, {_id:2380, properties:{index:2, content:"trách nhiệm tổ chức giao thông quy định như sau:"}}, {_id:2383, properties:{index:3, content:"trách nhiệm điều khiển giao thông của cảnh sát giao thông như sau:"}}, {_id:2387, properties:{index:1, content:"người điều khiển phương tiện và những người liên quan trực tiếp đến vụ tai nạn có trách nhiệm sau đây:"}}, {_id:2391, properties:{index:2, content:"những người có mặt tại nơi xảy ra vụ tai nạn có trách nhiệm sau đây:"}}, {_id:2397, properties:{index:3, content:"người điều khiển phương tiện khác khi đi qua nơi xảy ra vụ tai nạn có trách nhiệm chở người bị nạn đi cấp cứu"}}, {_id:2398, properties:{index:4, content:"các xe được quyền ưu tiên, xe chở người được hưởng quyền ưu đãi, miễn trừ ngoại giao, lãnh sự không bắt buộc thực hiện quy định tại khoản này."}}, {_id:2399, properties:{index:5, content:"cơ quan công an khi nhận được tin về vụ tai nạn có trách nhiệm cử người tới ngay hiện trường để điều tra vụ tai nạn, phối hợp với cơ quan quản lý đường bộ và ủy ban nhân dân địa phương bảo đảm giao thông thông suốt, an toàn."}}, {_id:2400, properties:{index:6, content:"ủy ban nhân dân cấp xã nơi xảy ra tai nạn có trách nhiệm kịp thời thông báo cho cơ quan công an, y tế đến để xử lý, giải quyết vụ tai nạn; tổ chức cứu chữa, giúp đỡ người bị nạn, bảo vệ hiện trường, bảo vệ tài sản của người bị nạn; trường hợp có người chết mà không rõ tung tích, không có thân nhân hoặc thân nhân không có khả năng chôn cất thì sau khi cơ quan nhà nước có thẩm quyền đã hoàn tất các công việc theo quy định của pháp luật và đồng ý cho chôn cất, ủy ban nhân dân cấp xã có trách nhiệm tổ chức chôn cất.\n\ntrường hợp vụ tai nạn vượt quá khả năng giải quyết, uỷ ban nhân dân cấp xã phải kịp thời báo cáo uỷ ban nhân dân cấp trên."}}, {_id:2401, properties:{index:7, content:"bộ công an có trách nhiệm thống kê, tổng hợp, xây dựng cơ sở dữ liệu thông tin về tai nạn giao thông đường bộ, cung cấp cho cơ quan, tổ chức, cá nhân theo quy định của pháp luật."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2403, properties:{index:1, content:"mạng lưới đường bộ được chia thành sáu hệ thống, gồm quốc lộ, đường tỉnh, đường huyện, đường xã, đường đô thị và đường chuyên dùng, quy định như sau:"}}, {_id:2410, properties:{index:2, content:"thẩm quyền phân loại và điều chỉnh các hệ thống đường bộ quy định như sau:"}}, {_id:2416, properties:{index:1, content:"đường bộ được đặt tên hoặc số hiệu như sau:"}}, {_id:2419, properties:{index:2, content:"đường bộ kết nối vào mạng lưới đường bộ trong khu vực, đường bộ quốc tế thì sử dụng cả tên, số hiệu đường trong nước và tên, số hiệu đường trong khu vực, đường bộ quốc tế."}}, {_id:2420, properties:{index:3, content:"việc đặt tên, số hiệu đường bộ do cơ quan có thẩm quyền phân loại đường bộ quyết định; riêng đường đô thị, đường tỉnh, việc đặt tên do hội đồng nhân dân cấp tỉnh quyết định trên cơ sở đề nghị của uỷ ban nhân dân cùng cấp."}}, {_id:2421, properties:{index:4, content:"chính phủ quy định cụ thể việc đặt tên, số hiệu đường bộ."}}, {_id:2423, properties:{index:1, content:"đường bộ được chia theo cấp kỹ thuật gồm đường cao tốc và các cấp kỹ thuật khác."}}, {_id:2424, properties:{index:2, content:"đường bộ xây dựng mới phải bảo đảm tiêu chuẩn kỹ thuật của từng cấp đường; các tuyến đường bộ đang khai thác chưa vào cấp phải được cải tạo, nâng cấp để đạt tiêu chuẩn kỹ thuật của cấp đường phù hợp; đối với đường chuyên dùng còn phải áp dụng cả tiêu chuẩn riêng theo quy định của pháp luật."}}, {_id:2425, properties:{index:3, content:"trách nhiệm của các bộ quy định như sau:"}}, {_id:2428, properties:{index:4, content:"trường hợp áp dụng tiêu chuẩn kỹ thuật đường bộ của nước ngoài thì phải được chấp thuận của cơ quan quản lý nhà nước có thẩm quyền."}}, {_id:2430, properties:{index:1, content:"quỹ đất dành cho kết cấu hạ tầng giao thông đường bộ được xác định tại quy hoạch kết cấu hạ tầng giao thông đường bộ"}}, {_id:2431, properties:{index:2, content:"ủy ban nhân dân cấp tỉnh xác định và quản lý quỹ đất dành cho dự án xây dựng kết cấu hạ tầng giao thông đường bộ theo quy hoạch đã được phê duyệt."}}, {_id:2432, properties:{index:3, content:"tỷ lệ quỹ đất giao thông đô thị so với đất xây dựng đô thị phải bảo đảm từ 16% đến 26%"}}, {_id:2433, properties:{index:4, content:"chính phủ quy định cụ thể tỷ lệ quỹ đất phù hợp với loại đô thị."}}, {_id:2435, properties:{index:1, content:"phạm vi đất dành cho đường bộ gồm đất của đường bộ và đất hành lang an toàn đường bộ."}}, {_id:2436, properties:{index:2, content:"trong phạm vi đất dành cho đường bộ, không được xây dựng các công trình khác, trừ một số công trình thiết yếu không thể bố trí ngoài phạm vi đó nhưng phải được cơ quan có thẩm quyền cho phép, gồm công trình phục vụ quốc phòng, an ninh, công trình phục vụ quản lý, khai thác đường bộ, công trình viễn thông, điện lực, đường ống cấp, thoát nước, xăng, dầu, khí."}}, {_id:2437, properties:{index:3, content:"trong phạm vi đất hành lang an toàn đường bộ, ngoài việc thực hiện quy định tại khoản 2 điều này, được tạm thời sử dụng vào mục đích nông nghiệp, quảng cáo nhưng không được làm ảnh hưởng đến an toàn công trình, an toàn giao thông đường bộ"}}, {_id:2438, properties:{index:4, content:"việc đặt biển quảng cáo trên đất hành lang an toàn đường bộ phải được cơ quan quản lý đường bộ có thẩm quyền đồng ý bằng văn bản."}}, {_id:2439, properties:{index:5, content:"người đang sử dụng đất được pháp luật thừa nhận mà đất đó nằm trong hành lang an toàn đường bộ thì được tiếp tục sử dụng đất theo đúng mục đích đã được xác định và không được gây cản trở cho việc bảo vệ an toàn công trình đường bộ.\n\ntrường hợp việc sử dụng đất gây ảnh hưởng đến việc bảo vệ an toàn công trình đường bộ thì chủ công trình và người sử dụng đất phải có biện pháp khắc phục, nếu không khắc phục được thì nhà nước thu hồi đất và bồi thường theo quy định của pháp luật."}}, {_id:2440, properties:{index:6, content:"chính phủ quy định cụ thể phạm vi đất dành cho đường bộ, việc sử dụng, khai thác đất hành lang an toàn đường bộ và việc xây dựng các công trình thiết yếu trong phạm vi đất dành cho đường bộ."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2442, properties:{index:1, content:"công trình đường bộ xây dựng mới, nâng cấp và cải tạo phải bảo đảm tiêu chuẩn kỹ thuật và điều kiện an toàn giao thông cho người, phương tiện tham gia giao thông đường bộ, trong đó có người đi bộ và người khuyết tật"}}, {_id:2443, properties:{index:2, content:"đường đô thị xây dựng phải có hè phố, phần đường, cầu vượt, hầm và tổ chức giao thông cho người đi bộ, người khuyết tật đi lại an toàn, thuận tiện."}}, {_id:2444, properties:{index:3, content:"công trình đường bộ phải được thẩm định về an toàn giao thông từ khi lập dự án, thiết kế, thi công, trước và trong quá trình khai thác"}}, {_id:2445, properties:{index:4, content:"người quyết định đầu tư, chủ đầu tư có trách nhiệm tiếp thu kết quả thẩm định an toàn giao thông để phê duyệt bổ sung vào dự án."}}, {_id:2446, properties:{index:5, content:"khu đô thị, khu công nghiệp, khu kinh tế, khu dân cư, khu thương mại - dịch vụ và công trình khác phải có hệ thống đường gom được xây dựng ngoài hành lang an toàn đường bộ; bảo đảm khoảng cách với quốc lộ theo quy định của chính phủ."}}, {_id:2447, properties:{index:6, content:"việc đấu nối được quy định như sau:"}}, {_id:2451, properties:{index:7, content:"bên cạnh tuyến quốc lộ đi qua khu dân cư phải có đường gom để phục vụ yêu cầu dân sinh."}}, {_id:2453, properties:{index:1, content:"công trình báo hiệu đường bộ bao gồm:"}}, {_id:2460, properties:{index:2, content:"đường bộ trước khi đưa vào khai thác phải được lắp đặt đầy đủ công trình báo hiệu đường bộ theo thiết kế được phê duyệt."}}, {_id:2461, properties:{index:3, content:"không được gắn vào công trình báo hiệu đường bộ các nội dung không liên quan tới ý nghĩa, mục đích của công trình báo hiệu đường bộ."}}, {_id:2463, properties:{index:1, content:"đầu tư xây dựng kết cấu hạ tầng giao thông đường bộ là việc đầu tư xây dựng mới, nâng cấp, cải tạo kết cấu hạ tầng giao thông đường bộ."}}, {_id:2464, properties:{index:2, content:"việc đầu tư xây dựng kết cấu hạ tầng giao thông đường bộ phải phù hợp với quy hoạch giao thông vận tải đường bộ đã được cấp có thẩm quyền phê duyệt; tuân thủ trình tự quản lý đầu tư xây dựng và các quy định khác của pháp luật; bảo đảm cấp kỹ thuật đường bộ, cảnh quan, bảo vệ môi trường."}}, {_id:2465, properties:{index:3, content:"tổ chức, cá nhân việt nam và nước ngoài được đầu tư xây dựng, kinh doanh khai thác kết cấu hạ tầng giao thông đường bộ theo quy định của pháp luật."}}, {_id:2466, properties:{index:4, content:"ủy ban nhân dân cấp có thẩm quyền chủ trì tổ chức giải phóng mặt bằng theo đúng quyết định thu hồi đất của cơ quan nhà nước có thẩm quyền và tạo điều kiện thuận lợi cho tổ chức, cá nhân đầu tư xây dựng, kinh doanh khai thác kết cấu hạ tầng giao thông đường bộ."}}, {_id:2467, properties:{index:5, content:"kết cấu hạ tầng giao thông đường bộ sau khi xây dựng, nâng cấp, cải tạo phải được cơ quan có thẩm quyền nghiệm thu, quyết định đưa vào khai thác theo quy định."}}, {_id:2469, properties:{index:1, content:"thi công công trình trên đường bộ đang khai thác chỉ được tiến hành khi có giấy phép của cơ quan nhà nước có thẩm quyền, thực hiện theo đúng nội dung của giấy phép và quy định của pháp luật về xây dựng."}}, {_id:2470, properties:{index:2, content:"trong quá trình thi công, đơn vị thi công phải bố trí báo hiệu, rào chắn tạm thời tại nơi thi công và thực hiện các biện pháp bảo đảm giao thông thông suốt, an toàn."}}, {_id:2471, properties:{index:3, content:"thi công công trình trên đường đô thị phải tuân thủ quy định tại khoản 1, khoản 2 điều này và các quy định sau đây:"}}, {_id:2475, properties:{index:4, content:"đơn vị thi công không thực hiện các biện pháp bảo đảm giao thông thông suốt, an toàn theo quy định, để xảy ra tai nạn giao thông, ùn tắc giao thông, ô nhiễm môi trường nghiêm trọng thì phải chịu trách nhiệm theo quy định của pháp luật."}}, {_id:2477, properties:{index:1, content:"bảo trì đường bộ là thực hiện các công việc bảo dưỡng và sửa chữa đường bộ nhằm duy trì tiêu chuẩn kỹ thuật của đường đang khai thác."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2478, properties:{index:2, content:"đường bộ đưa vào khai thác phải được quản lý, bảo trì với các nội dung sau đây:"}}, {_id:2481, properties:{index:3, content:"trách nhiệm tổ chức quản lý, bảo trì đường bộ quy định như sau:"}}, {_id:2484, properties:{index:4, content:"việc quản lý, bảo trì hệ thống đường huyện, đường xã do ủy ban nhân dân cấp tỉnh quy định;"}}, {_id:2486, properties:{index:5, content:"bộ trưởng bộ giao thông vận tải quy định việc quản lý, bảo trì đường bộ."}}, {_id:2488, properties:{index:1, content:"nguồn tài chính để quản lý, bảo trì hệ thống quốc lộ và đường địa phương được bảo đảm từ quỹ bảo trì đường bộ.\n\nnguồn tài chính để quản lý, bảo trì đường chuyên dùng, đường không do nhà nước quản lý khai thác, đường được đầu tư xây dựng không bằng nguồn vốn từ ngân sách nhà nước do tổ chức, cá nhân quản lý khai thác chịu trách nhiệm."}}, {_id:2489, properties:{index:2, content:"quỹ bảo trì đường bộ được hình thành từ các nguồn sau đây:"}}, {_id:2492, properties:{index:3, content:"chính phủ quy định cụ thể việc lập, quản lý và sử dụng quỹ bảo trì đường bộ ở trung ương và địa phương."}}, {_id:2495, properties:{index:1, content:"trong đô thị, khi xây dựng trụ sở cơ quan, trường học, bệnh viện, trung tâm thương mại - dịch vụ, văn hóa và khu dân cư phải xây dựng đủ nơi đỗ xe phù hợp với quy mô của công trình."}}, {_id:2496, properties:{index:2, content:"bến xe, bãi đỗ xe, trạm dừng nghỉ phải xây dựng theo quy hoạch đã được cơ quan nhà nước có thẩm quyền phê duyệt, bảo đảm tiêu chuẩn kỹ thuật."}}, {_id:2497, properties:{index:3, content:"trạm thu phí là nơi thực hiện việc thu phí phương tiện tham gia giao thông đường bộ, được xây dựng theo quy hoạch hoặc dự án đầu tư được cơ quan nhà nước có thẩm quyền phê duyệt"}}, {_id:2498, properties:{index:4, content:"hoạt động của trạm thu phí phải bảo đảm giao thông thông suốt, an toàn."}}, {_id:2499, properties:{index:5, content:"trạm kiểm tra tải trọng xe là nơi cơ quan quản lý đường bộ thực hiện việc thu thập, phân tích, đánh giá tác động của tải trọng xe, khổ giới hạn xe đến an toàn đường bộ; kiểm tra, xử lý vi phạm đối với xe quá khổ giới hạn, quá tải trọng cho phép của đường bộ và xe bánh xích lưu hành trên đường bộ, được xây dựng theo quy hoạch do bộ giao thông vận tải lập, trình thủ tướng chính phủ phê duyệt.\n\ntrường hợp cần thiết, để bảo vệ kết cấu hạ tầng giao thông đường bộ, bộ trưởng bộ giao thông vận tải quyết định thành lập trạm kiểm tra tải trọng xe tạm thời."}}, {_id:2500, properties:{index:6, content:"bộ trưởng bộ giao thông vận tải quy định về quy chuẩn kỹ thuật bến xe, bãi đỗ xe, trạm dừng nghỉ, trạm thu phí, trạm kiểm tra tải trọng xe; quy định về tổ chức, hoạt động của trạm thu phí, trạm kiểm tra tải trọng xe."}}, {_id:2502, properties:{index:1, content:"bảo vệ kết cấu hạ tầng giao thông đường bộ gồm hoạt động bảo đảm an toàn và tuổi thọ của công trình đường bộ, biện pháp phòng ngừa, ngăn chặn và xử lý hành vi xâm phạm kết cấu hạ tầng giao thông đường bộ.\n\nphạm vi bảo vệ kết cấu hạ tầng giao thông đường bộ gồm đất của đường bộ, hành lang an toàn đường bộ, phần trên không, phần dưới mặt đất, phần dưới mặt nước có liên quan đến an toàn công trình và an toàn giao thông đường bộ."}}, {_id:2503, properties:{index:2, content:"tổ chức, cá nhân được phép xây dựng, cải tạo, mở rộng, bảo trì công trình và tiến hành hoạt động khác trong phạm vi bảo vệ kết cấu hạ tầng giao thông đường bộ phải thực hiện theo quy định của pháp luật."}}, {_id:2504, properties:{index:3, content:"đơn vị quản lý công trình đường bộ có trách nhiệm bảo đảm an toàn kỹ thuật của công trình, liên đới chịu trách nhiệm đối với tai nạn giao thông xảy ra do chất lượng quản lý, bảo trì công trình; trường hợp phát hiện công trình bị hư hỏng hoặc có nguy cơ gây mất an toàn giao thông thì phải xử lý, sửa chữa kịp thời, có biện pháp phòng, chống và khắc phục kịp thời hậu quả do thiên tai gây ra đối với công trình đường bộ."}}, {_id:2505, properties:{index:4, content:"trách nhiệm bảo vệ kết cấu hạ tầng giao thông đường bộ quy định như sau:"}}, {_id:2511, properties:{index:5, content:"người nào phát hiện công trình đường bộ bị hư hỏng hoặc bị xâm hại, hành lang an toàn đường bộ bị lấn chiếm phải kịp thời báo cho uỷ ban nhân dân, cơ quan quản lý đường bộ hoặc cơ quan công an nơi gần nhất để xử lý; trường hợp cần thiết có biện pháp báo hiệu ngay cho người tham gia giao thông biết"}}, {_id:2512, properties:{index:6, content:"khi nhận được tin báo, cơ quan có trách nhiệm phải nhanh chóng thực hiện các biện pháp khắc phục để bảo đảm giao thông thông suốt, an toàn."}}, {_id:2514, properties:{index:1, content:"xe ô tô đúng kiểu loại được phép tham gia giao thông phải bảo đảm các quy định về chất lượng, an toàn kỹ thuật và bảo vệ môi trường sau đây:"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2525, properties:{index:2, content:"xe mô tô hai bánh, xe mô tô ba bánh, xe gắn máy đúng kiểu loại được phép tham gia giao thông phải bảo đảm các quy định về chất lượng, an toàn kỹ thuật và bảo vệ môi trường quy định tại các điểm a, b, d, đ, e, h, i và k khoản 1 điều này."}}, {_id:2526, properties:{index:3, content:"xe cơ giới phải đăng ký và gắn biển số do cơ quan nhà nước có thẩm quyền cấp."}}, {_id:2527, properties:{index:4, content:"chính phủ quy định niên hạn sử dụng đối với xe cơ giới."}}, {_id:2528, properties:{index:5, content:"bộ trưởng bộ giao thông vận tải quy định về chất lượng an toàn kỹ thuật và bảo vệ môi trường của xe cơ giới được phép tham gia giao thông, trừ xe cơ giới của quân đội, công an sử dụng vào mục đích quốc phòng, an ninh."}}, {_id:2530, properties:{index:1, content:"xe cơ giới có nguồn gốc hợp pháp, bảo đảm tiêu chuẩn chất lượng, an toàn kỹ thuật và bảo vệ môi trường theo quy định của luật này được cơ quan nhà nước có thẩm quyền cấp đăng ký và biển số."}}, {_id:2531, properties:{index:2, content:"bộ trưởng bộ công an quy định và tổ chức cấp, thu hồi đăng ký, biển số các loại xe cơ giới; bộ trưởng bộ quốc phòng quy định và tổ chức cấp, thu hồi đăng ký, biển số các loại xe cơ giới của quân đội sử dụng vào mục đích quốc phòng."}}, {_id:2533, properties:{index:1, content:"việc sản xuất, lắp ráp, cải tạo, sửa chữa, bảo dưỡng và nhập khẩu xe cơ giới tham gia giao thông đường bộ phải tuân theo quy định về chất lượng an toàn kỹ thuật và bảo vệ môi trường"}}, {_id:2534, properties:{index:2, content:"không được cải tạo các xe ô tô khác thành xe ô tô chở khách."}}, {_id:2535, properties:{index:3, content:"chủ phương tiện không được tự thay đổi kết cấu, tổng thành, hệ thống của xe không đúng với thiết kế của nhà chế tạo hoặc thiết kế cải tạo đã được cơ quan có thẩm quyền phê duyệt."}}, {_id:2536, properties:{index:4, content:"xe ô tô và rơ moóc, sơ mi rơ moóc được kéo bởi xe ô tô tham gia giao thông đường bộ phải được kiểm tra định kỳ về an toàn kỹ thuật và bảo vệ môi trường (sau đây gọi là kiểm định)."}}, {_id:2537, properties:{index:5, content:"người đứng đầu cơ sở đăng kiểm và người trực tiếp thực hiện việc kiểm định phải chịu trách nhiệm về việc xác nhận kết quả kiểm định."}}, {_id:2538, properties:{index:6, content:"chủ phương tiện, người lái xe ô tô chịu trách nhiệm duy trì tình trạng an toàn kỹ thuật của phương tiện theo tiêu chuẩn quy định khi tham gia giao thông đường bộ giữa hai kỳ kiểm định."}}, {_id:2539, properties:{index:7, content:"bộ trưởng bộ giao thông vận tải quy định điều kiện, tiêu chuẩn và cấp giấy phép cho cơ sở đăng kiểm xe cơ giới; quy định và tổ chức thực hiện kiểm định xe cơ giới"}}, {_id:2540, properties:{index:8, content:"bộ trưởng bộ quốc phòng, bộ trưởng bộ công an quy định và tổ chức kiểm định xe cơ giới của quân đội, công an sử dụng vào mục đích quốc phòng, an ninh."}}, {_id:2542, properties:{index:1, content:"khi tham gia giao thông, xe thô sơ phải bảo đảm điều kiện an toàn giao thông đường bộ."}}, {_id:2543, properties:{index:2, content:"ủy ban nhân dân cấp tỉnh quy định cụ thể điều kiện, phạm vi hoạt động của xe thô sơ tại địa phương mình."}}, {_id:2545, properties:{index:1, content:"bảo đảm các quy định về chất lượng an toàn kỹ thuật và bảo vệ môi trường sau đây:"}}, {_id:2550, properties:{index:2, content:"có đăng ký và gắn biển số do cơ quan nhà nước có thẩm quyền cấp."}}, {_id:2551, properties:{index:3, content:"hoạt động trong phạm vi quy định, bảo đảm an toàn cho người, phương tiện và công trình đường bộ khi di chuyển."}}, {_id:2552, properties:{index:4, content:"việc sản xuất, lắp ráp, cải tạo, sửa chữa và nhập khẩu xe máy chuyên dùng phải tuân theo quy định về chất lượng an toàn kỹ thuật và bảo vệ môi trường."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2553, properties:{index:5, content:"chủ phương tiện và người điều khiển xe máy chuyên dùng chịu trách nhiệm duy trì tình trạng an toàn kỹ thuật và kiểm định theo quy định đối với xe máy chuyên dùng khi tham gia giao thông đường bộ."}}, {_id:2554, properties:{index:6, content:"bộ trưởng bộ giao thông vận tải quy định cụ thể về chất lượng an toàn kỹ thuật và bảo vệ môi trường, cấp, thu hồi đăng ký, biển số; quy định danh mục xe máy chuyên dùng phải kiểm định và tổ chức việc kiểm định; bộ trưởng bộ quốc phòng, bộ trưởng bộ công an quy định và tổ chức việc cấp, thu hồi đăng ký, biển số và kiểm định xe máy chuyên dùng của quân đội, công an sử dụng vào mục đích quốc phòng, an ninh."}}, {_id:2556, properties:{index:1, content:"người lái xe tham gia giao thông phải đủ độ tuổi, sức khoẻ quy định tại điều 60 của luật này và có giấy phép lái xe phù hợp với loại xe được phép điều khiển do cơ quan nhà nước có thẩm quyền cấp.\n\nngười tập lái xe ô tô khi tham gia giao thông phải thực hành trên xe tập lái và có giáo viên bảo trợ tay lái."}}, {_id:2557, properties:{index:2, content:"người lái xe khi điều khiển phương tiện phải mang theo các giấy tờ sau:"}}, {_id:2563, properties:{index:1, content:"căn cứ vào kiểu loại, công suất động cơ, tải trọng và công dụng của xe cơ giới, giấy phép lái xe được phân thành giấy phép lái xe không thời hạn và giấy phép lái xe có thời hạn."}}, {_id:2564, properties:{index:2, content:"giấy phép lái xe không thời hạn bao gồm các hạng sau đây:"}}, {_id:2568, properties:{index:3, content:"người khuyết tật điều khiển xe mô tô ba bánh dùng cho người khuyết tật được cấp giấy phép lái xe hạng a1."}}, {_id:2569, properties:{index:4, content:"giấy phép lái xe có thời hạn gồm các hạng sau đây:"}}, {_id:2577, properties:{index:5, content:"giấy phép lái xe có giá trị sử dụng trong phạm vi lãnh thổ việt nam và lãnh thổ của nước hoặc vùng lãnh thổ mà việt nam ký cam kết công nhận giấy phép lái xe của nhau."}}, {_id:2579, properties:{index:1, content:"độ tuổi của người lái xe quy định như sau:"}}, {_id:2586, properties:{index:2, content:"người lái xe phải có sức khỏe phù hợp với loại xe, công dụng của xe"}}, {_id:2587, properties:{index:3, content:"bộ trưởng bộ y tế chủ trì, phối hợp với bộ trưởng bộ giao thông vận tải quy định về tiêu chuẩn sức khỏe của người lái xe, việc khám sức khỏe định kỳ đối với người lái xe ô tô và quy định về cơ sở y tế khám sức khoẻ của người lái xe."}}, {_id:2589, properties:{index:1, content:"cơ sở đào tạo lái xe là loại hình cơ sở dạy nghề, phải có đủ điều kiện về lớp học, sân tập lái, xe tập lái, đội ngũ giáo viên, giáo trình, giáo án và phải được giấy cấp phép theo quy định."}}, {_id:2590, properties:{index:2, content:"cơ sở đào tạo lái xe phải thực hiện đúng nội dung và chương trình quy định cho từng loại, hạng giấy phép lái xe."}}, {_id:2591, properties:{index:3, content:"người có nhu cầu được cấp giấy phép lái xe hạng a1, a2, a3, a4, b1 phải được đào tạo"}}, {_id:2592, properties:{index:4, content:"người có nhu cầu được cấp giấy phép lái xe hạng b2, c, d, e và các giấy phép lái xe hạng f phải được đào tạo tập trung tại cơ sở đào tạo."}}, {_id:2593, properties:{index:5, content:"việc đào tạo để nâng hạng giấy phép lái xe thực hiện cho những trường hợp sau đây:"}}, {_id:2599, properties:{index:6, content:"người có nhu cầu được đào tạo nâng hạng giấy phép lái xe theo quy định tại khoản 4 điều này còn phải có đủ thời gian và số ki-lô-mét lái xe an toàn quy định cho từng hạng giấy phép lái xe; người có nhu cầu được đào tạo nâng hạng giấy phép lái xe lên hạng d, e tối thiểu phải có trình độ văn hóa trung học cơ sở."}}, {_id:2600, properties:{index:7, content:"việc đào tạo lái xe ô tô chở người từ 10 chỗ ngồi trở lên và lái xe kéo rơ moóc chỉ được thực hiện bằng hình thức đào tạo nâng hạng với các điều kiện quy định tại khoản 4 và khoản 5 điều này."}}, {_id:2601, properties:{index:8, content:"việc sát hạch để cấp giấy phép lái xe ô tô phải thực hiện tại các trung tâm sát hạch lái xe"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2602, properties:{index:9, content:"các trung tâm sát hạch lái xe phải được xây dựng theo quy hoạch, có đủ cơ sở vật chất - kỹ thuật đáp ứng yêu cầu sát hạch lái xe theo quy định."}}, {_id:2603, properties:{index:10, content:"người sát hạch lái xe phải có thẻ sát hạch viên theo quy định và phải chịu trách nhiệm về kết quả sát hạch của mình."}}, {_id:2604, properties:{index:11, content:"người đã qua đào tạo và đạt kết quả kỳ sát hạch được cấp giấy phép lái xe đúng hạng đã trúng tuyển.\n\ntrường hợp giấy phép lái xe có thời hạn, trước khi hết thời hạn sử dụng người lái xe phải khám sức khỏe và làm các thủ tục theo quy định để được đổi giấy phép lái xe."}}, {_id:2605, properties:{index:12, content:"bộ trưởng bộ giao thông vận tải quy định điều kiện, tiêu chuẩn và cấp giấy phép cho cơ sở đào tạo; quy định hình thức, nội dung, chương trình đào tạo; sát hạch và cấp, đổi, thu hồi giấy phép lái xe; bộ trưởng bộ quốc phòng, bộ trưởng bộ công an quy định về cơ sở đào tạo, tổ chức đào tạo, sát hạch và cấp, đổi, thu hồi giấy phép lái xe cho lực lượng quân đội, công an làm nhiệm vụ quốc phòng, an ninh."}}, {_id:2607, properties:{index:1, content:"người điều khiển xe máy chuyên dùng tham gia giao thông phải đủ độ tuổi, sức khỏe phù hợp với ngành nghề lao động và có chứng chỉ bồi dưỡng kiến thức pháp luật về giao thông đường bộ, bằng hoặc chứng chỉ điều khiển xe máy chuyên dùng do cơ sở đào tạo người điều khiển xe máy chuyên dùng cấp."}}, {_id:2608, properties:{index:2, content:"người điều khiển xe máy chuyên dùng khi tham gia giao thông phải mang theo các giấy tờ sau đây:"}}, {_id:2612, properties:{index:1, content:"có sức khỏe bảo đảm điều khiển xe an toàn."}}, {_id:2613, properties:{index:2, content:"hiểu biết quy tắc giao thông đường bộ."}}, {_id:2615, properties:{index:1, content:"hoạt động vận tải đường bộ gồm hoạt động vận tải không kinh doanh và hoạt động kinh doanh vận tải đường bộ"}}, {_id:2616, properties:{index:2, content:"kinh doanh vận tải đường bộ là ngành nghề kinh doanh có điều kiện theo quy định của pháp luật."}}, {_id:2617, properties:{index:3, content:"kinh doanh vận tải đường bộ gồm kinh doanh vận tải hành khách, kinh doanh vận tải hàng hóa."}}, {_id:2618, properties:{index:4, content:"hoạt động vận tải đường bộ phải phù hợp với quy hoạch giao thông vận tải đường bộ và mạng lưới tuyến vận tải."}}, {_id:2620, properties:{index:1, content:"thời gian làm việc của người lái xe ô tô không được quá 10 giờ trong một ngày và không được lái xe liên tục quá 4 giờ."}}, {_id:2621, properties:{index:2, content:"người vận tải và người lái xe ô tô chịu trách nhiệm thực hiện quy định tại khoản 1 điều này."}}, {_id:2623, properties:{index:1, content:"kinh doanh vận tải hành khách bằng xe ô tô bao gồm:"}}, {_id:2629, properties:{index:2, content:"kinh doanh vận tải hàng hóa bằng xe ô tô bao gồm:"}}, {_id:2634, properties:{index:3, content:"chính phủ quy định cụ thể về kinh doanh vận tải bằng xe ô tô."}}, {_id:2636, properties:{index:1, content:"doanh nghiệp, hợp tác xã, hộ kinh doanh hoạt động kinh doanh vận tải bằng xe ô tô phải có đủ các điều kiện sau đây:"}}, {_id:2642, properties:{index:2, content:"chỉ các doanh nghiệp, hợp tác xã mới được kinh doanh vận tải hành khách theo tuyến cố định, kinh doanh vận tải hành khách bằng xe buýt, bằng xe taxi và phải có đủ các điều kiện sau đây:"}}, {_id:2646, properties:{index:3, content:"chỉ các doanh nghiệp, hợp tác xã mới được kinh doanh vận tải hàng hóa bằng công-ten-nơ và phải có đủ các điều kiện quy định tại khoản 1, điểm b khoản 2 điều này."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2647, properties:{index:4, content:"chính phủ quy định cụ thể điều kiện và việc cấp giấy phép kinh doanh vận tải bằng xe ô tô."}}, {_id:2649, properties:{index:1, content:"người vận tải, người lái xe khách phải chấp hành các quy định sau đây:"}}, {_id:2655, properties:{index:2, content:"bộ trưởng bộ giao thông vận tải quy định về tổ chức và quản lý hoạt động vận tải hành khách bằng xe ô tô."}}, {_id:2657, properties:{index:1, content:"người kinh doanh vận tải hành khách có các quyền sau đây:"}}, {_id:2660, properties:{index:2, content:"người kinh doanh vận tải hành khách có các nghĩa vụ sau đây:"}}, {_id:2666, properties:{index:3, content:"bộ trưởng bộ tài chính chủ trì, phối hợp với bộ trưởng bộ giao thông vận tải quy định về vé và chứng từ thu cước, phí vận tải hành khách."}}, {_id:2668, properties:{index:1, content:"kiểm tra các điều kiện bảo đảm an toàn của xe trước khi khởi hành."}}, {_id:2669, properties:{index:2, content:"có thái độ văn minh, lịch sự, hướng dẫn hành khách ngồi đúng nơi quy định."}}, {_id:2670, properties:{index:3, content:"kiểm tra việc sắp xếp, chằng buộc hành lý, hàng hóa bảo đảm an toàn."}}, {_id:2671, properties:{index:4, content:"có biện pháp bảo vệ tính mạng, sức khỏe, tài sản của hành khách đi xe, giữ gìn trật tự, vệ sinh trong xe."}}, {_id:2672, properties:{index:5, content:"đóng cửa lên xuống của xe trước và trong khi xe chạy."}}, {_id:2674, properties:{index:1, content:"hành khách có các quyền sau đây:"}}, {_id:2678, properties:{index:2, content:"hành khách có các nghĩa vụ sau đây:"}}, {_id:2683, properties:{index:1, content:"việc vận chuyển hàng hóa bằng xe ô tô phải chấp hành các quy định sau đây:"}}, {_id:2688, properties:{index:3, content:"bộ trưởng bộ giao thông vận tải quy định về tổ chức và quản lý hoạt động vận tải hàng hóa bằng xe ô tô."}}, {_id:2690, properties:{index:1, content:"người kinh doanh vận tải hàng hóa có các quyền sau đây:"}}, {_id:2695, properties:{index:2, content:"người kinh doanh vận tải hàng hóa có các nghĩa vụ sau đây:"}}, {_id:2699, properties:{index:3, content:"chính phủ quy định giới hạn trách nhiệm của người kinh doanh vận tải hàng hóa."}}, {_id:2701, properties:{index:1, content:"người thuê vận tải hàng hóa có các quyền sau đây:"}}, {_id:2705, properties:{index:2, content:"người thuê vận tải hàng hóa có các nghĩa vụ sau đây:"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2710, properties:{index:1, content:"người nhận hàng có các quyền sau đây:"}}, {_id:2714, properties:{index:2, content:"người nhận hàng có các nghĩa vụ sau đây:"}}, {_id:2718, properties:{index:1, content:"hàng siêu trường, siêu trọng là hàng có kích thước hoặc trọng lượng vượt quá giới hạn quy định nhưng không thể tháo rời ra được."}}, {_id:2719, properties:{index:2, content:"việc vận chuyển hàng siêu trường, siêu trọng phải sử dụng xe vận tải phù hợp với loại hàng và phải có giấy phép sử dụng đường bộ do cơ quan nhà nước có thẩm quyền cấp."}}, {_id:2720, properties:{index:3, content:"xe vận chuyển hàng siêu trường, siêu trọng phải chạy với tốc độ quy định trong giấy phép và phải có báo hiệu kích thước của hàng, trường hợp cần thiết phải bố trí người chỉ dẫn giao thông để bảo đảm an toàn giao thông."}}, {_id:2721, properties:{index:4, content:"bộ trưởng bộ giao thông vận tải quy định cụ thể về vận chuyển hàng siêu trường, siêu trọng."}}, {_id:2723, properties:{index:1, content:"tùy theo loại động vật sống, người kinh doanh vận tải yêu cầu người thuê vận tải bố trí người áp tải để chăm sóc trong quá trình vận tải."}}, {_id:2724, properties:{index:2, content:"người thuê vận tải chịu trách nhiệm về việc xếp, dỡ động vật sống theo hướng dẫn của người kinh doanh vận tải; trường hợp người thuê vận tải không thực hiện được thì phải trả cước, phí xếp, dỡ cho người kinh doanh vận tải."}}, {_id:2725, properties:{index:3, content:"việc vận chuyển động vật sống trên đường phải tuân theo quy định của pháp luật về vệ sinh, phòng dịch và bảo vệ môi trường."}}, {_id:2727, properties:{index:1, content:"xe vận chuyển hàng nguy hiểm phải có giấy phép do cơ quan nhà nước có thẩm quyền cấp."}}, {_id:2728, properties:{index:2, content:"xe vận chuyển hàng nguy hiểm không được dừng, đỗ ở nơi đông người, những nơi dễ xảy ra nguy hiểm."}}, {_id:2729, properties:{index:3, content:"chính phủ quy định danh mục hàng nguy hiểm, vận chuyển hàng nguy hiểm và thẩm quyền cấp giấy phép vận chuyển hàng nguy hiểm."}}, {_id:2731, properties:{index:1, content:"xe buýt phải chạy đúng tuyến, đúng lịch trình và dừng, đỗ đúng nơi quy định."}}, {_id:2732, properties:{index:2, content:"người lái xe taxi khách, xe taxi tải đón, trả hành khách, hàng hóa theo thỏa thuận giữa hành khách, chủ hàng và người lái xe nhưng phải chấp hành các quy định về bảo đảm an toàn giao thông."}}, {_id:2733, properties:{index:3, content:"xe chở hàng phải hoạt động theo đúng tuyến, phạm vi và thời gian quy định đối với từng loại xe."}}, {_id:2734, properties:{index:4, content:"xe vệ sinh môi trường, xe ô tô chở phế thải, vật liệu rời phải được che phủ kín không để rơi, vãi trên đường phố; trường hợp để rơi, vãi thì người vận tải phải chịu trách nhiệm thu dọn ngay."}}, {_id:2735, properties:{index:5, content:"ủy ban nhân dân cấp tỉnh quy định cụ thể về hoạt động vận tải đường bộ trong đô thị và tỷ lệ phương tiện vận tải hành khách đáp ứng nhu cầu đi lại của người khuyết tật."}}, {_id:2737, properties:{index:1, content:"việc sử dụng xe thô sơ, xe gắn máy, xe mô tô hai bánh, xe mô tô ba bánh và các loại xe tương tự để vận chuyển hành khách, hàng hóa phải theo đúng quy định về trật tự, an toàn giao thông."}}, {_id:2738, properties:{index:2, content:"bộ trưởng bộ giao thông vận tải quy định việc thực hiện khoản 1 điều này."}}, {_id:2739, properties:{index:3, content:"căn cứ quy định của bộ trưởng bộ giao thông vận tải, ủy ban nhân dân cấp tỉnh quy định cụ thể việc tổ chức thực hiện tại địa phương."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2741, properties:{index:1, content:"vận tải đa phương thức quy định trong luật này là việc vận chuyển hàng hóa từ địa điểm nhận hàng đến địa điểm trả hàng cho người nhận hàng bằng ít nhất hai phương thức vận tải, trong đó có phương thức vận tải bằng đường bộ trên cơ sở một hợp đồng vận tải đa phương thức."}}, {_id:2742, properties:{index:2, content:"chính phủ quy định cụ thể về vận tải đa phương thức."}}, {_id:2744, properties:{index:1, content:"dịch vụ hỗ trợ vận tải đường bộ gồm dịch vụ tại bến xe, bãi đỗ xe, trạm dừng nghỉ, đại lý vận tải, đại lý bán vé, dịch vụ thu gom hàng, dịch vụ chuyển tải, dịch vụ kho hàng, dịch vụ cứu hộ vận tải đường bộ."}}, {_id:2745, properties:{index:2, content:"bộ trưởng bộ giao thông vận tải quy định cụ thể về dịch vụ hỗ trợ vận tải đường bộ."}}, {_id:2747, properties:{index:1, content:"hoạt động của bến xe ô tô khách, bến xe ô tô hàng, bãi đỗ xe, trạm dừng nghỉ phải bảo đảm trật tự, an toàn, vệ sinh môi trường, phòng, chống cháy nổ và chịu sự quản lý của cơ quan quản lý nhà nước có thẩm quyền ở địa phương."}}, {_id:2748, properties:{index:2, content:"doanh nghiệp, hợp tác xã khai thác bến xe ô tô khách có quyền, nghĩa vụ sắp xếp nơi bán vé hoặc tổ chức bán vé cho hành khách theo hợp đồng với người kinh doanh vận tải; sắp xếp xe ô tô có đủ điều kiện kinh doanh vận tải vào bến đón, trả khách đúng tuyến."}}, {_id:2749, properties:{index:3, content:"doanh nghiệp, hợp tác xã khai thác bến xe ô tô hàng có quyền, nghĩa vụ sắp xếp xe ô tô vào bến xếp dỡ hàng hóa, dịch vụ kho bãi, ký gửi, đóng gói, bảo quản hàng hóa."}}, {_id:2750, properties:{index:4, content:"doanh nghiệp, hợp tác xã khai thác bãi đỗ xe có quyền, nghĩa vụ tổ chức dịch vụ trông giữ phương tiện."}}, {_id:2751, properties:{index:5, content:"doanh nghiệp, hợp tác xã khai thác trạm dừng nghỉ có quyền, nghĩa vụ tổ chức dịch vụ phục vụ người và phương tiện tham gia giao thông đường bộ; thực hiện công việc theo hợp đồng ủy thác với người vận tải."}}, {_id:2752, properties:{index:6, content:"ủy ban nhân dân cấp tỉnh căn cứ vào loại bến xe ô tô để quy định giá dịch vụ xe ra, vào bến xe ô tô."}}, {_id:2754, properties:{index:1, content:"xây dựng quy hoạch, kế hoạch và chính sách phát triển giao thông đường bộ; xây dựng và chỉ đạo thực hiện chương trình quốc gia về an toàn giao thông đường bộ."}}, {_id:2755, properties:{index:2, content:"ban hành và tổ chức thực hiện các văn bản quy phạm pháp luật về giao thông đường bộ; quy chuẩn, tiêu chuẩn về giao thông đường bộ."}}, {_id:2756, properties:{index:3, content:"tuyên truyền, phổ biến, giáo dục pháp luật về giao thông đường bộ."}}, {_id:2757, properties:{index:4, content:"tổ chức quản lý, bảo trì, bảo vệ kết cấu hạ tầng giao thông đường bộ."}}, {_id:2758, properties:{index:5, content:"đăng ký, cấp, thu hồi biển số phương tiện giao thông đường bộ; cấp, thu hồi giấy chứng nhận chất lượng, an toàn kỹ thuật và bảo vệ môi trường của phương tiện giao thông đường bộ."}}, {_id:2759, properties:{index:6, content:"quản lý đào tạo, sát hạch lái xe; cấp, đổi, thu hồi giấy phép lái xe, chứng chỉ bồi dưỡng kiến thức pháp luật về giao thông đường bộ"}}, {_id:2760, properties:{index:7, content:"quản lý hoạt động vận tải và dịch vụ hỗ trợ vận tải; tổ chức cứu nạn giao thông đường bộ."}}, {_id:2761, properties:{index:8, content:"tổ chức nghiên cứu, ứng dụng khoa học và công nghệ về giao thông đường bộ; đào tạo cán bộ và công nhân kỹ thuật giao thông đường bộ."}}, {_id:2762, properties:{index:9, content:"kiểm tra, thanh tra, giải quyết khiếu nại, tố cáo; xử lý vi phạm pháp luật về giao thông đường bộ."}}, {_id:2763, properties:{index:10, content:"hợp tác quốc tế về giao thông đường bộ."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2765, properties:{index:1, content:"chính phủ thống nhất quản lý nhà nước về giao thông đường bộ."}}, {_id:2766, properties:{index:2, content:"bộ giao thông vận tải chịu trách nhiệm trước chính phủ thực hiện quản lý nhà nước về giao thông đường bộ."}}, {_id:2767, properties:{index:3, content:"bộ công an thực hiện các nhiệm vụ quản lý nhà nước về giao thông đường bộ theo quy định của luật này và các quy định khác của pháp luật có liên quan; thực hiện các biện pháp bảo đảm trật tự, an toàn giao thông; phối hợp với bộ giao thông vận tải bảo vệ kết cấu hạ tầng giao thông đường bộ.\n\nbộ công an, bộ giao thông vận tải có trách nhiệm phối hợp trong việc cung cấp số liệu đăng ký phương tiện giao thông đường bộ, dữ liệu về tai nạn giao thông và cấp, đổi, thu hồi giấy phép lái xe."}}, {_id:2768, properties:{index:4, content:"bộ quốc phòng thực hiện các nhiệm vụ quản lý nhà nước về giao thông đường bộ theo quy định của luật này và các quy định khác của pháp luật có liên quan."}}, {_id:2769, properties:{index:5, content:"bộ, cơ quan ngang bộ trong phạm vi nhiệm vụ, quyền hạn của mình có trách nhiệm phối hợp với bộ giao thông vận tải thực hiện quản lý nhà nước về giao thông đường bộ."}}, {_id:2770, properties:{index:6, content:"ủy ban nhân dân các cấp trong phạm vi nhiệm vụ, quyền hạn của mình tổ chức thực hiện quản lý nhà nước về giao thông đường bộ theo quy định của luật này và các quy định khác của pháp luật có liên quan trong phạm vi địa phương."}}, {_id:2772, properties:{index:1, content:"thanh tra đường bộ thực hiện chức năng thanh tra chuyên ngành về giao thông đường bộ."}}, {_id:2773, properties:{index:2, content:"thanh tra đường bộ có các nhiệm vụ và quyền hạn sau đây:"}}, {_id:2777, properties:{index:3, content:"việc thanh tra đào tạo, sát hạch, cấp, đổi, thu hồi giấy phép lái xe của lực lượng quân đội, công an do bộ trưởng bộ quốc phòng, bộ trưởng bộ công an quy định;"}}, {_id:2779, properties:{index:4, content:"tổ chức và hoạt động của thanh tra đường bộ thực hiện theo quy định của luật này và pháp luật về thanh tra.\n\nbộ trưởng bộ giao thông vận tải quy định cụ thể nhiệm vụ, quyền hạn của thanh tra đường bộ."}}, {_id:2781, properties:{index:1, content:"cảnh sát giao thông đường bộ thực hiện việc tuần tra, kiểm soát để kiểm soát người và phương tiện tham gia giao thông đường bộ; xử lý vi phạm pháp luật về giao thông đường bộ đối với người và phương tiện tham gia giao thông đường bộ và chịu trách nhiệm trước pháp luật về quyết định của mình; phối hợp với cơ quan quản lý đường bộ phát hiện, ngăn chặn hành vi vi phạm quy định bảo vệ công trình đường bộ và hành lang an toàn đường bộ."}}, {_id:2782, properties:{index:2, content:"bộ trưởng bộ công an quy định cụ thể nhiệm vụ, quyền hạn, hình thức, nội dung tuần tra, kiểm soát của cảnh sát giao thông đường bộ."}}, {_id:2783, properties:{index:3, content:"chính phủ quy định việc huy động các lực lượng cảnh sát khác và công an xã phối hợp với cảnh sát giao thông đường bộ tham gia tuần tra, kiểm soát trật tự, an toàn giao thông đường bộ trong trường hợp cần thiết."}}, {_id:2785, properties:{index:1, content:"luật này có hiệu lực thi hành từ ngày 01 tháng 7 năm 2009."}}, {_id:2786, properties:{index:2, content:"luật này thay thế luật giao thông đường bộ ngày 29 tháng 6 năm 2001."}}, {_id:2790, properties:{index:1, content:"đường bộ trong khu vực đông dân cư là đoạn đường bộ nằm trong khu vực nội thành phố, nội thị xã và những đoạn đường có dân cư sinh sống sát dọc theo đường, có các hoạt động có thể ảnh hưởng đến an toàn giao thông đường bộ và được xác định bằng biển báo là đường qua khu đông dân cư (khi cần thiết có thể xác định riêng cho từng chiều đường).\n\nđối với đoạn đường nằm trong khu vực nội thành phố, nội thị xã: căn cứ vào mức độ đô thị hóa và mật độ dân cư sinh sống bên đường để đặt biển báo hiệu “bắt đầu khu đông dân cư” (biển số 420) và biển báo hiệu “hết khu đông dân cư” (biển số 421) trên các tuyến đường ở vị trí vào, ra đô thị cho phù hợp (không đặt biển báo theo địa giới hành chính được quy hoạch, nếu chưa đô thị hóa hoặc dân cư thưa thớt); biển số 420 có hiệu lực khu đông dân cư đối với tất cả các tuyến đường nằm trong khu vực đô thị cho đến vị trí đặt biển số 421.\n\nđối với đoạn đường nằm ngoài nội thành phố, nội thị xã: đoạn đường được xác định là qua khu đông dân cư khi có chiều dài từ 500 m trở lên, các lối ra vào nhà trực tiếp với đường có cự ly trung bình từ 6 m trở xuống theo chiều ngang; mật độ các lối ra vào nhà trung bình dưới 10 m."}}, {_id:2791, properties:{index:2, content:"xe cơ giới gồm xe ô tô; máy kéo; rơ moóc hoặc sơ mi rơ moóc được kéo bởi xe ô tô, máy kéo; xe mô tô hai bánh; xe mô tô ba bánh; xe gắn máy (kể cả xe máy điện) và các loại xe tương tự."}}, {_id:2792, properties:{index:3, content:"ô tô chuyên dùng là ô tô có kết cấu và trang bị để thực hiện một chức năng công dụng đặc biệt, bao gồm: ô tô quét đường; ô tô hút chất thải; ô tô trộn vữa; ô tô trộn bê tông; ôtô bơm bê tông; ô tô cần cẩu; ô tô thang; ô tô khoan; ô tô cứu hộ giao thông; ô tô chuyên dùng loại khác như ô tô truyền hình lưu động, ô tô đo sóng truyền hình lưu động, ô tô kiểm tra và bảo dưỡng cầu, ô tô kiểm tra cáp điện ngầm, ô tô chụp x-quang, ô tô phẫu thuật lưu động."}}, {_id:2793, properties:{index:4, content:"xe máy chuyên dùng gồm xe máy thi công, xe máy nông nghiệp, lâm nghiệp và các loại xe đặc chủng khác sử dụng vào mục đích quốc phòng, an ninh có tham gia giao thông đường bộ."}}, {_id:2794, properties:{index:5, content:"dải phân cách giữa là bộ phận của đường mà xe không chạy trên đó, được dùng để phân chia mặt đường thành hai phần xe chạy ngược chiều riêng biệt (các dạng chủ yếu: bó vỉa, dải phân cách kết cấu bê tông, hộ lan tôn sóng, hoặc dải đất dự trữ)."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2795, properties:{index:6, content:"đường đôi là đường có chiều đi và về được phân biệt bằng dải phân cách giữa."}}, {_id:2862, properties:{index:1, content:"đường bộ gồm đường, cầu đường bộ, hầm đường bộ, bến phà đường bộ"}}, {_id:2910, properties:{index:2, content:"công trình đường bộ gồm đường bộ, nơi dừng xe, đỗ xe trên đường bộ, đèn tín hiệu, biển báo hiệu, vạch kẻ đường, cọc tiêu, rào chắn, đảo giao thông, dải phân cách, cột cây số, tường, kè, hệ thống thoát nước, trạm kiểm tra tải trọng xe, trạm thu phí và các công trình, thiết bị phụ trợ đường bộ khác"}}, {_id:2912, properties:{index:3, content:"kết cấu hạ tầng giao thông đường bộ gồm công trình đường bộ, bến xe, bãi đỗ xe, trạm dừng nghỉ và các công trình phụ trợ khác trên đường bộ phục vụ giao thông và hành lang an toàn đường bộ"}}, {_id:2914, properties:{index:4, content:"đất của đường bộ là phần đất trên đó công trình đường bộ được xây dựng và phần đất dọc hai bên đường bộ để quản lý, bảo trì, bảo vệ công trình đường bộ"}}, {_id:2919, properties:{index:5, content:"hành lang an toàn đường bộ là dải đất dọc hai bên đất của đường bộ, tính từ mép ngoài đất của đường bộ ra hai bên để bảo đảm an toàn giao thông đường bộ"}}, {_id:2924, properties:{index:6, content:"phần đường xe chạy là phần của đường bộ được sử dụng cho phương tiện giao thông qua lại"}}, {_id:2930, properties:{index:7, content:"làn đường là một phần của phần đường xe chạy được chia theo chiều dọc của đường, có bề rộng đủ cho xe chạy an toàn"}}, {_id:2931, properties:{index:8, content:"khổ giới hạn của đường bộ là khoảng trống có kích thước giới hạn về chiều cao, chiều rộng của đường, cầu, bến phà, hầm đường bộ để các xe kể cả hàng hóa xếp trên xe đi qua được an toàn"}}, {_id:2936, properties:{index:9, content:"đường phố là đường đô thị, gồm lòng đường và hè phố"}}, {_id:2945, properties:{index:10, content:"dải phân cách là bộ phận của đường để phân chia mặt đường thành hai chiều xe chạy riêng biệt hoặc để phân chia phần đường của xe cơ giới và xe thô sơ"}}, {_id:2953, properties:{index:11, content:"nơi đường giao nhau cùng mức (sau đây gọi là nơi đường giao nhau) là nơi hai hay nhiều đường bộ gặp nhau trên cùng một mặt phẳng, gồm cả mặt bằng hình thành vị trí giao nhau đó"}}, {_id:2958, properties:{index:12, content:"đường cao tốc là đường dành cho xe cơ giới, có dải phân cách chia đường cho xe chạy hai chiều riêng biệt; không giao nhau cùng mức với một hoặc các đường khác; được bố trí đầy đủ trang thiết bị phục vụ, bảo đảm giao thông liên tục, an toàn, rút ngắn thời gian hành trình và chỉ cho xe ra, vào ở những điểm nhất định"}}, {_id:2962, properties:{index:13, content:"đường chính là đường bảo đảm giao thông chủ yếu trong khu vực"}}, {_id:2970, properties:{index:14, content:"đường nhánh là đường nối vào đường chính"}}, {_id:2972, properties:{index:15, content:"đường ưu tiên là đường mà trên đó phương tiện tham gia giao thông đường bộ được các phương tiện giao thông đến từ hướng khác nhường đường khi qua nơi đường giao nhau, được cắm biển báo hiệu đường ưu tiên"}}, {_id:2975, properties:{index:16, content:"đường gom là đường để gom hệ thống đường giao thông nội bộ của các khu đô thị, công nghiệp, kinh tế, dân cư, thương mại - dịch vụ và các đường khác vào đường chính hoặc vào đường nhánh trước khi đấu nối vào đường chính"}}, {_id:2982, properties:{index:17, content:"phương tiện giao thông đường bộ gồm phương tiện giao thông cơ giới đường bộ, phương tiện giao thông thô sơ đường bộ"}}, {_id:2985, properties:{index:18, content:"phương tiện giao thông cơ giới đường bộ (sau đây gọi là xe cơ giới) gồm xe ô tô; máy kéo; rơ moóc hoặc sơ mi rơ moóc được kéo bởi xe ô tô, máy kéo; xe mô tô hai bánh; xe mô tô ba bánh; xe gắn máy (kể cả xe máy điện) và các loại xe tương tự"}}, {_id:2990, properties:{index:19, content:"phương tiện giao thông thô sơ đường bộ (sau đây gọi là xe thô sơ) gồm xe đạp (kể cả xe đạp máy), xe xích lô, xe lăn dùng cho người khuyết tật, xe súc vật kéo và các loại xe tương tự"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Clause;
UNWIND [{_id:2992, properties:{index:20, content:"xe máy chuyên dùng gồm xe máy thi công, xe máy nông nghiệp, lâm nghiệp và các loại xe đặc chủng khác sử dụng vào mục đích quốc phòng, an ninh có tham gia giao thông đường bộ"}}, {_id:2999, properties:{index:21, content:"phương tiện tham gia giao thông đường bộ gồm phương tiện giao thông đường bộ và xe máy chuyên dùng"}}, {_id:3001, properties:{index:22, content:"người tham gia giao thông gồm người điều khiển, người sử dụng phương tiện tham gia giao thông đường bộ; người điều khiển, dẫn dắt súc vật; người đi bộ trên đường bộ"}}, {_id:3004, properties:{index:23, content:"người điều khiển phương tiện gồm người điều khiển xe cơ giới, xe thô sơ, xe máy chuyên dùng tham gia giao thông đường bộ"}}, {_id:3011, properties:{index:24, content:"người lái xe là người điều khiển xe cơ giới"}}, {_id:3012, properties:{index:25, content:"người điều khiển giao thông là cảnh sát giao thông; người được giao nhiệm vụ hướng dẫn giao thông tại nơi thi công, nơi ùn tắc giao thông, ở bến phà, tại cầu đường bộ đi chung với đường sắt"}}, {_id:3018, properties:{index:26, content:"hành khách là người được chở trên phương tiện vận tải hành khách đường bộ, có trả tiền"}}, {_id:3019, properties:{index:27, content:"hành lý là vật phẩm mà hành khách mang theo trên cùng phương tiện hoặc gửi theo phương tiện khác"}}, {_id:3023, properties:{index:28, content:"hàng hóa là máy móc, thiết bị, nguyên vật liệu, nhiên liệu, hàng tiêu dùng, động vật sống và các động sản khác được vận chuyển bằng phương tiện giao thông đường bộ"}}, {_id:3027, properties:{index:29, content:"hàng nguy hiểm là hàng hóa có chứa các chất nguy hiểm khi chở trên đường có khả năng gây nguy hại tới tính mạng, sức khỏe con người, môi trường, an toàn và an ninh quốc gia"}}, {_id:3032, properties:{index:30, content:"vận tải đường bộ là hoạt động sử dụng phương tiện giao thông đường bộ để vận chuyển người, hàng hóa trên đường bộ"}}, {_id:3039, properties:{index:31, content:"người vận tải là tổ chức, cá nhân sử dụng phương tiện giao thông đường bộ để thực hiện hoạt động vận tải đường bộ"}}, {_id:3043, properties:{index:32, content:"cơ quan quản lý đường bộ là cơ quan thực hiện chức năng quản lý nhà nước chuyên ngành thuộc bộ giao thông vận tải; cơ quan chuyên môn thuộc ủy ban nhân dân tỉnh, thành phố trực thuộc trung ương (sau đây gọi chung là cấp tỉnh), ủy ban nhân dân huyện, quận, thị xã, thành phố thuộc tỉnh (sau đây gọi chung là cấp huyện); ủy ban nhân dân xã, phường, thị trấn (sau đây gọi chung là cấp xã)"}}, {_id:3048, properties:{index:1, content:"nghị định này quy định về hành vi vi phạm hành chính; hình thức, mức xử phạt, biện pháp khắc phục hậu quả đối với từng hành vi vi phạm hành chính; thẩm quyền lập biên bản, thẩm quyền xử phạt, mức phạt tiền cụ thể theo từng chức danh đối với hành vi vi phạm hành chính trong lĩnh vực giao thông đường bộ và đường sắt."}}, {_id:3054, properties:{index:2, content:"các hành vi vi phạm hành chính trong các lĩnh vực quản lý nhà nước khác liên quan đến lĩnh vực giao thông đường bộ và đường sắt mà không quy định tại nghị định này thì áp dụng quy định tại các nghị định quy định về xử phạt vi phạm hành chính trong các lĩnh vực đó để xử phạt."}}, {_id:3063, properties:{index:1, content:"cá nhân, tổ chức có hành vi vi phạm hành chính trong lĩnh vực giao thông đường bộ, đường sắt trên lãnh thổ nước cộng hòa xã hội chủ nghĩa việt nam."}}, {_id:3064, properties:{index:2, content:"tổ chức quy định tại khoản 1 điều này gồm:"}}] AS row
