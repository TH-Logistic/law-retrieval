UNWIND [{_id:1902, properties:{index:1, content:"cảnh cáo hoặc phạt tiền 100.000 đồng đến 200.000 đồng đối với hành vi sử dụng vé tàu giả để đi tàu."}}, {_id:1903, properties:{index:2, content:"phạt tiền từ 2.000.000 đồng đến 4.000.000 đồng đối với cá nhân thực hiện một trong các hành vi vi phạm sau đây:"}}, {_id:1906, properties:{index:3, content:"phạt tiền từ 10.000.000 đồng đến 15.000.000 đồng đối với cá nhân, từ 20.000.000 đồng đến 30.000.000 đồng đối với tổ chức thực hiện một trong các hành vi vi phạm sau đây:"}}, {_id:1913, properties:{index:5, content:"ngoài việc bị áp dụng hình thức xử phạt, cá nhân, tổ chức thực hiện hành vi vi phạm quy định tại khoản 2, khoản 3 điều này còn bị áp dụng biện pháp khắc phục hậu quả buộc nộp lại số lợi bất hợp pháp có được do thực hiện vi phạm hành chính."}}, {_id:1915, properties:{index:1, content:"phạt tiền từ 100.000 đồng đến 200.000 đồng đối với cá nhân thực hiện một trong các hành vi vi phạm sau đây:"}}, {_id:1919, properties:{index:2, content:"phạt tiền từ 300.000 đồng đến 500.000 đồng đối với cá nhân thực hiện một trong các hành vi vi phạm sau đây:"}}, {_id:750, properties:{index:6, content:"phạt tiền từ 4.000.000 đồng đến 6.000.000 đồng đối với cá nhân, từ 8.000.000 đồng đến 12.000.000 đồng đối với tổ chức thực hiện một trong các hành vi vi phạm sau đây:"}}, {_id:1926, properties:{index:3, content:"phạt tiền từ 500.000 đồng đến 1.000.000 đồng đối với cá nhân thực hiện hành vi ném gạch, đất, đá, cát hoặc các vật thể khác vào tàu."}}, {_id:1927, properties:{index:4, content:"phạt tiền từ 2.000.000 đồng đến 3.000.000 đồng đối với cá nhân thực hiện hành vi đe dọa, xâm phạm sức khỏe của hành khách, nhân viên đường sắt đang thi hành nhiệm vụ."}}, {_id:1928, properties:{index:5, content:"ngoài việc bị áp dụng hình thức xử phạt, cá nhân thực hiện hành vi vi phạm quy định tại điểm c, điểm d, điểm đ, điểm e khoản 2 điều này còn bị áp dụng các biện pháp khắc phục hậu quả: buộc đưa động vật có dịch bệnh, thi hài, hài cốt, chất dễ cháy, dễ nổ, vũ khí, hàng nguy hiểm, động vật sống xuống tàu (tại ga đến gần nhất trong trường hợp tàu đang chạy), ra ga để xử lý theo quy định."}}, {_id:1930, properties:{index:1, content:"quy trình sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ của các cơ quan, tổ chức được giao quản lý, khai thác, bảo trì kết cấu hạ tầng giao thông đường bộ."}}, {_id:1931, properties:{index:2, content:"việc sử dụng kết quả thu được từ phương tiện, thiết bị kỹ thuật nghiệp vụ do các cơ quan, tổ chức được giao quản lý, khai thác, bảo trì kết cấu hạ tầng giao thông đường bộ cung cấp làm căn cứ để xác định vi phạm hành chính và xử phạt vi phạm hành chính trong lĩnh vực giao thông đường bộ."}}, {_id:760, properties:{index:7, content:"phạt tiền từ 6.000.000 đồng đến 8.000.000 đồng đối với cá nhân, từ 12.000.000 đồng đến 16.000.000 đồng đối với tổ chức thực hiện hành vi chiếm dụng lòng đường đô thị hoặc hè phố từ 10 m2 đến dưới 20 m2 làm nơi trông, giữ xe."}}, {_id:761, properties:{index:8, content:"phạt tiền từ 10.000.000 đồng đến 15.000.000 đồng đối với cá nhân, từ 20.000.000 đồng đến 30.000.000 đồng đối với tổ chức thực hiện một trong các hành vi vi phạm sau đây:"}}, {_id:764, properties:{index:9, content:"phạt tiền từ 15.000.000 đồng đến 20.000.000 đồng đối với cá nhân, từ 30.000.000 đồng đến 40.000.000 đồng đối với tổ chức thực hiện một trong các hành vi vi phạm sau đây:"}}, {_id:1933, properties:{index:1, content:"các cơ quan quản lý đường bộ, bao gồm tổng cục đường bộ việt nam, cục quản lý đường bộ, chi cục quản lý đường bộ và sở giao thông vận tải."}}, {_id:1934, properties:{index:2, content:"các cơ quan thực hiện chức năng thanh tra đường bộ, bao gồm thanh tra bộ giao thông vận tải, thanh tra sở giao thông vận tải, các cơ quan được giao thực hiện chức năng thanh tra chuyên ngành đường bộ."}}, {_id:767, properties:{index:10, content:"ngoài việc bị áp dụng hình thức xử phạt, cá nhân, tổ chức thực hiện hành vi vi phạm còn bị áp dụng các biện pháp khắc phục hậu quả sau đây:"}}, {_id:1935, properties:{index:3, content:"các tổ chức được giao khai thác, bảo trì kết cấu hạ tầng giao thông đường bộ và tổ chức, cá nhân có liên quan."}}, {_id:1937, properties:{index:1, content:"phương tiện, thiết bị kỹ thuật nghiệp vụ gồm thiết bị cân hoặc bộ cân (sau đây gọi chung là thiết bị cân) kiểm tra tải trọng xe cơ giới và thiết bị ghi hình."}}] AS row
