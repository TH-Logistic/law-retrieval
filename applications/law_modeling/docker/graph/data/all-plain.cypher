UNWIND [{_id:2639, properties:{index:"c", content:"bảo đảm số lượng lái xe, nhân viên phục vụ trên xe phù hợp với phương án kinh doanh và phải có hợp đồng lao động bằng văn bản; nhân viên phục vụ trên xe phải được tập huấn nghiệp vụ kinh doanh vận tải, an toàn giao thông; không được sử dụng người lái xe đang trong thời kỳ bị cấm hành nghề theo quy định của pháp luật;"}}, {_id:2640, properties:{index:"d", content:"người trực tiếp điều hành hoạt động vận tải của doanh nghiệp, hợp tác xã phải có trình độ chuyên môn về vận tải;"}}, {_id:2641, properties:{index:"đ", content:"có nơi đỗ xe phù hợp với quy mô của doanh nghiệp, hợp tác xã, hộ kinh doanh, bảo đảm yêu cầu về trật tự, an toàn, phòng, chống cháy nổ và vệ sinh môi trường."}}, {_id:2643, properties:{index:"a", content:"các điều kiện quy định tại khoản 1 điều này;"}}, {_id:2644, properties:{index:"b", content:"có bộ phận quản lý các điều kiện về an toàn giao thông;"}}, {_id:2645, properties:{index:"c", content:"đăng ký tiêu chuẩn chất lượng dịch vụ vận tải hành khách với cơ quan có thẩm quyền và phải niêm yết công khai."}}, {_id:2650, properties:{index:"a", content:"đón, trả hành khách đúng nơi quy định;"}}, {_id:2651, properties:{index:"b", content:"không chở hành khách trên mui, trong khoang chở hành lý hoặc để hành khách đu, bám bên ngoài xe;"}}, {_id:2652, properties:{index:"c", content:"không chở hàng nguy hiểm, hàng có mùi hôi thối hoặc động vật, hàng hóa khác có ảnh hưởng đến sức khỏe của hành khách;"}}, {_id:2653, properties:{index:"d", content:"không chở hành khách, hành lý, hàng hóa vượt quá trọng tải, số người theo quy định;"}}, {_id:2654, properties:{index:"đ", content:"không để hàng hóa trong khoang chở hành khách; có biện pháp giữ gìn vệ sinh trong xe."}}, {_id:2658, properties:{index:"a", content:"thu cước, phí vận tải;"}}, {_id:2659, properties:{index:"b", content:"từ chối vận chuyển trước khi phương tiện rời bến xe, rời vị trí đón, trả hành khách theo hợp đồng vận chuyển những người đã có vé hoặc người trong danh sách hợp đồng có hành vi gây rối trật tự công cộng, gây cản trở công việc của người kinh doanh vận tải, ảnh hưởng đến sức khoẻ, tài sản của người khác, gian lận vé hoặc hành khách đang bị dịch bệnh nguy hiểm."}}, {_id:2661, properties:{index:"a", content:"thực hiện đầy đủ các cam kết về chất lượng vận tải, hợp đồng vận tải;"}}, {_id:2662, properties:{index:"b", content:"mua bảo hiểm cho hành khách; phí bảo hiểm được tính vào giá vé hành khách;"}}, {_id:2663, properties:{index:"c", content:"giao vé, chứng từ thu cước, phí vận tải cho hành khách;"}}, {_id:2664, properties:{index:"d", content:"bồi thường thiệt hại do người làm công, người đại diện gây ra trong khi thực hiện công việc được người kinh doanh vận tải giao;"}}, {_id:2665, properties:{index:"đ", content:"chịu trách nhiệm về hậu quả mà người làm công, người đại diện gây ra do thực hiện yêu cầu của người kinh doanh vận tải trái quy định của luật này."}}, {_id:2675, properties:{index:"a", content:"được vận chuyển theo đúng hợp đồng vận tải, cam kết của người kinh doanh vận tải về chất lượng vận tải;"}}, {_id:2676, properties:{index:"b", content:"được miễn cước hành lý với trọng lượng không quá 20 kg và với kích thước phù hợp với thiết kế của xe;"}}] AS row
