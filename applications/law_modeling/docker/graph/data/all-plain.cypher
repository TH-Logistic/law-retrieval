UNWIND [{_id:2677, properties:{index:"c", content:"được từ chối chuyến đi trước khi phương tiện khởi hành và được trả lại tiền vé theo quy định của bộ trưởng bộ giao thông vận tải."}}, {_id:2679, properties:{index:"a", content:"mua vé và trả cước, phí vận tải hành lý mang theo quá mức quy định;"}}, {_id:2680, properties:{index:"b", content:"có mặt tại nơi xuất phát đúng thời gian đã thỏa thuận; chấp hành quy định về vận chuyển; thực hiện đúng hướng dẫn của lái xe, nhân viên phục vụ trên xe về các quy định bảo đảm trật tự, an toàn giao thông;"}}, {_id:2681, properties:{index:"c", content:"không mang theo hành lý, hàng hóa mà pháp luật cấm lưu thông."}}, {_id:2684, properties:{index:"a", content:"hàng vận chuyển trên xe phải được xếp đặt gọn gàng và chằng buộc chắc chắn;"}}, {_id:2685, properties:{index:"b", content:"khi vận chuyển hàng rời phải che đậy, không để rơi vãi."}}, {_id:2686, properties:{index:"a", content:"chở hàng vượt quá trọng tải thiết kế và quá kích thước giới hạn cho phép của xe;"}}, {_id:2687, properties:{index:"b", content:"chở người trong thùng xe, trừ trường hợp quy định tại khoản 1 điều 21 của luật này."}}, {_id:2691, properties:{index:"a", content:"yêu cầu người thuê vận tải cung cấp thông tin cần thiết về hàng hóa để ghi vào giấy vận chuyển và có quyền kiểm tra tính xác thực của thông tin đó;"}}, {_id:2692, properties:{index:"b", content:"yêu cầu người thuê vận tải thanh toán đủ cước, phí vận tải và chi phí phát sinh; yêu cầu người thuê vận tải bồi thường thiệt hại do vi phạm thỏa thuận trong hợp đồng;"}}, {_id:2693, properties:{index:"c", content:"từ chối vận chuyển nếu người thuê vận tải không giao hàng hóa theo thỏa thuận trong hợp đồng;"}}, {_id:2694, properties:{index:"d", content:"yêu cầu giám định hàng hóa khi cần thiết."}}, {_id:2696, properties:{index:"a", content:"cung cấp phương tiện đúng loại, thời gian, địa điểm và giao hàng hóa cho người nhận hàng theo thỏa thuận trong hợp đồng;"}}, {_id:2697, properties:{index:"b", content:"hướn