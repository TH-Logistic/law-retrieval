UNWIND [{_id:1205, properties:{index:"c", content:"không thực hiện đúng quy định về biển số, trừ các hành vi vi phạm quy định tại điểm i khoản 5 điều này."}}, {_id:3249, properties:{index:"g", content:"buộc phải bố trí phương tiện khác để chở số hành khách vượt quá quy định được phép chở của phương tiện;"}}, {_id:1207, properties:{index:"a", content:"tự ý cắt, hàn, đục lại số khung, số máy; đưa phương tiện đã bị cắt, hàn, đục lại số khung, số máy trái quy định tham gia giao thông;"}}, {_id:200, properties:{index:"e", content:"đỗ xe không sát theo lề đường, hè phố phía bên phải theo chiều đi hoặc bánh xe gần nhất cách lề đường, hè phố quá 0,25 m; đỗ xe trên đường xe điện, đường dành riêng cho xe buýt; đỗ xe trên miệng cống thoát nước, miệng hầm của đường điện thoại, điện cao thế, chỗ dành riêng cho xe chữa cháy lấy nước; đỗ, để xe ở hè phố trái quy định của pháp luật; đỗ xe nơi có biển “cấm đỗ xe” hoặc biển “cấm dừng xe và đỗ xe”, trừ hành vi vi phạm quy định tại điểm i khoản 4, điểm b khoản 6 điều này;"}}, {_id:3252, properties:{index:"h", content:"buộc phải đăng ký, niêm yết đầy đủ, chính xác các thông tin theo quy định;"}}, {_id:1208, properties:{index:"b", content:"tẩy xóa, sửa chữa hoặc giả mạo hồ sơ đăng ký xe;"}}, {_id:1209, properties:{index:"c", content:"tự ý thay đổi khung, máy, hình dáng, kích thước, đặc tính của xe;"}}, {_id:204, properties:{index:"g", content:"không sử dụng hoặc sử dụng không đủ đèn chiếu sáng trong thời gian từ 19 giờ ngày hôm trước đến 05 giờ ngày hôm sau, khi sương mù, thời tiết xấu hạn chế tầm nhìn; sử dụng đèn chiếu xa khi tránh xe đi ngược chiều;"}}, {_id:1210, properties:{index:"d", content:"khai báo không đúng sự thật hoặc sử dụng các giấy tờ, tài liệu giả để được cấp lại biển số, giấy đăng ký xe;"}}, {_id:206, properties:{index:"h", content:"điều khiển xe ô tô kéo theo xe khác, vật khác (trừ trường hợp kéo theo một rơ moóc, sơ mi rơ moóc hoặc một xe ô tô, xe máy chuyên dùng khác khi xe này không tự chạy được); điều khiển xe ô tô đẩy xe khác, vật khác; điều khiển xe kéo rơ moóc, sơ mi rơ moóc kéo thêm rơ moóc hoặc xe khác, vật khác; không nối chắc chắn, an toàn giữa xe kéo và xe được kéo khi kéo nhau;"}}, {_id:1211, properties:{index:"đ", content:"giao xe hoặc để cho người không đủ điều kiện theo quy định tại khoản 1 điều 58 của luật giao thông đường bộ điều khiển xe tham gia giao thông (bao gồm cả trường hợp người điều khiển phương tiện có giấy phép lái xe nhưng đã hết hạn sử dụng hoặc đang trong thời gian bị tước quyền sử dụng);"}}, {_id:1212, properties:{index:"e", content:"không chấp hành việc thu hồi giấy đăng ký xe, biển số xe theo quy định;"}}, {_id:3254, properties:{index:"i", content:"buộc phải gắn hộp đèn với chữ “taxi” hoặc buộc phải niêm yết cụm từ “xe taxi”, “xe hợp đồng”, “xe du lịch” theo đúng quy định;"}}, {_id:1213, properties:{index:"g", content:"đưa phương tiện không có giấy đăng ký xe tham gia giao thông hoặc có nhưng đã hết hạn sử dụng; đưa phương tiện có giấy chứng nhận đăng ký xe tạm thời tham gia giao thông quá thời hạn, tuyến đường, phạm vi cho phép;"}}, {_id:1214, properties:{index:"h", content:"đưa phương tiện có giấy đăng ký xe nhưng không do cơ quan có thẩm quyền cấp hoặc bị tẩy xóa tham gia giao thông; đưa phương tiện có giấy đăng ký xe nhưng không đúng với số khung số máy của xe tham gia giao thông;"}}, {_id:3256, properties:{index:"k", content:"buộc phải cấp “thẻ nhận dạng lái xe” cho lái xe theo quy định;"}}, {_id:1215, properties:{index:"i", content:"lắp đặt, sử dụng thiết bị thay đổi biển số trên xe trái quy định;"}}, {_id:1216, properties:{index:"k", content:"đưa phương tiện không gắn biển số (đối với loại xe có quy định phải gắn biển số) tham gia giao thông; đưa phương tiện gắn biển số không đúng với giấy đăng ký xe hoặc gắn biển số không do cơ quan có thẩm quyền cấp tham gia giao thông."}}, {_id:210, properties:{index:"i", content:"chở người trên xe được kéo, trừ người điều khiển;"}}, {_id:3261, properties:{index:"l", content:"buộc phải tổ chức tập huấn nghiệp vụ hoặc tổ chức khám sức khỏe định kỳ cho lái xe và nhân viên phục vụ trên xe theo quy định;"}}] AS row
