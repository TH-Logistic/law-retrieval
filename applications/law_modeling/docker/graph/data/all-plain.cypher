UNWIND [{_id:551, properties:{index:"h", content:"vượt bên phải trong trường hợp không được phép;"}}, {_id:552, properties:{index:"i", content:"đi vào khu vực cấm, đường có biển báo hiệu có nội dung cấm đi vào đối với loại phương tiện đang điều khiển, trừ các hành vi vi phạm quy định tại khoản 5, điểm b khoản 6 điều này và các trường hợp xe ưu tiên đang đi làm nhiệm vụ khẩn cấp theo quy định;"}}, {_id:1725, properties:{index:"c", content:"không bố trí hoặc bố trí không đúng vị trí quy định, không đủ biển báo, tín hiệu phòng vệ theo quy định, không có biện pháp bảo đảm an toàn giao thông đường sắt trong quá trình thi công;"}}, {_id:553, properties:{index:"k", content:"người đang điều khiển xe hoặc chở người ngồi trên xe bám, kéo, đẩy xe khác, vật khác, dẫn dắt súc vật, mang vác vật cồng kềnh; chở người đứng trên yên, giá đèo hàng hoặc ngồi trên tay lái; xếp hàng hóa trên xe vượt quá giới hạn quy định; điều khiển xe kéo theo xe khác, vật khác;"}}, {_id:554, properties:{index:"l", content:"chở hàng vượt trọng tải thiết kế được ghi trong giấy đăng ký xe đối với loại xe có quy định về trọng tải thiết kế;"}}, {_id:555, properties:{index:"m", content:"chạy trong hầm đường bộ không sử dụng đèn chiếu sáng gần."}}, {_id:557, properties:{index:"a", content:"điều khiển xe chạy quá tốc độ quy định từ 10 km/h đến 20 km/h;"}}, {_id:558, properties:{index:"b", content:"dừng xe, đỗ xe trong hầm đường bộ không đúng nơi quy định;"}}, {_id:559, properties:{index:"c", content:"vượt xe trong những trường hợp không được vượt, vượt xe tại đoạn đường có biển báo hiệu có nội dung cấm vượt đối với loại phương tiện đang điều khiển, trừ các hành vi vi phạm quy định tại điểm h khoản 3 điều này;"}}, {_id:560, properties:{index:"d", content:"vượt xe trong hầm đường bộ không đúng nơi quy định; quay đầu xe trong hầm đường bộ;"}}, {_id:561, properties:{index:"đ", content:"không nhường đường hoặc gây cản trở xe được quyền ưu tiên đang phát tín hiệu ưu tiên đi làm nhiệm vụ;"}}, {_id:1726, properties:{index:"d", content:"điều khiển phương tiện, thiết bị thi công mà không có bằng, chứng chỉ chuyên môn theo quy định;"}}, {_id:562, properties:{index:"e", content:"không chấp hành hiệu lệnh của đèn tín hiệu giao thông;"}}, {_id:563, properties:{index:"g", content:"không chấp hành hiệu lệnh, hướng dẫn của người điều khiển giao thông hoặc người kiểm soát giao thông;"}}, {_id:564, properties:{index:"h", content:"người đang điều khiển xe sử dụng ô (dù), điện thoại di động, thiết bị âm thanh, trừ thiết bị trợ thính."}}, {_id:567, properties:{index:"a", content:"sử dụng chân chống hoặc vật khác quệt xuống đường khi xe đang chạy;"}}, {_id:568, properties:{index:"b", content:"điều khiển xe đi vào đường cao tốc, trừ xe phục vụ việc quản lý, bảo trì đường cao tốc;"}}, {_id:1727, properties:{index:"đ", content:"thu hồi tín hiệu phòng vệ khi chưa kết thúc thi công, chưa kiểm tra trạng thái đường, chưa kiểm tra giới hạn tiếp giáp kiến trúc đủ điều kiện bảo đảm an toàn chạy tàu;"}}, {_id:570, properties:{index:"a", content:"điều khiển xe chạy quá tốc độ quy định trên 20 km/h;"}}, {_id:1728, properties:{index:"e", content:"thi công công trình đường sắt có giấy phép thi công nhưng đã hết thời hạn ghi trong giấy phép hoặc có văn bản chấp thuận của cơ quan có thẩm quyền nhưng đã hết thời hạn thi công ghi trong văn bản;"}}] AS row
