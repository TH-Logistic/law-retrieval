UNWIND [{_id:1132, properties:{index:"r", content:"sử dụng xe kinh doanh vận tải hành khách theo hợp đồng, xe kinh doanh vận tải khách du lịch mà xe đó có số chuyến trùng lặp điểm đầu và trùng lặp điểm cuối vượt quá quy định;"}}, {_id:3157, properties:{index:"e", content:"xe đạp máy là xe thô sơ hai bánh có lắp động cơ, vận tốc thiết kế lớn nhất không lớn hơn 25 km/h và khi tắt máy thì đạp xe đi được (kể cả xe đạp điện)."}}, {_id:1133, properties:{index:"s", content:"kinh doanh vận tải hành khách theo hợp đồng, kinh doanh vận tải khách du lịch không thực hiện đúng quy định về đón, trả khách tại trụ sở chính, trụ sở chi nhánh, văn phòng đại diện hoặc tại một địa điểm cố định khác do đơn vị kinh doanh vận tải thuê, hợp tác kinh doanh;"}}, {_id:107, properties:{index:"v", content:"buộc thu hồi và tiêu hủy bằng, chứng chỉ chuyên môn đã cấp trái phép;"}}, {_id:1134, properties:{index:"t", content:"sử dụng xe taxi, xe kinh doanh vận tải hành khách theo hợp đồng, xe kinh doanh vận tải khách du lịch có trên 70% tổng thời gian hoạt động trong 01 tháng (của xe) tại địa bàn của một địa phương (tỉnh, thành phố trực thuộc trung ương) mà không có phù hiệu do sở giao thông vận tải địa phương đó cấp theo quy định."}}, {_id:1137, properties:{index:"a", content:"tổ chức hoạt động khai thác bến xe, bãi đỗ xe, trạm dừng nghỉ khi chưa được cơ quan có thẩm quyền cho phép theo quy định;"}}, {_id:114, properties:{index:"x", content:"buộc phải đưa thi hài, hài cốt, động vật sống, động vật có dịch bệnh, chất dễ cháy, dễ nổ, vũ khí, hàng nguy hiểm, xuống tàu (tại ga đến gần nhất trong trường hợp tàu đang chạy), ra ga để xử lý theo quy định;"}}, {_id:116, properties:{index:"y", content:"buộc phải thực hiện ngay nhiệm vụ vận tải đặc biệt, an sinh xã hội theo yêu cầu của người đứng đầu cơ quan quản lý nhà nước có thẩm quyền theo quy định."}}, {_id:1138, properties:{index:"b", content:"để xe ô tô không đủ điều kiện kinh doanh vận tải khách vào bến xe ô tô khách đón khách;"}}, {_id:1139, properties:{index:"c", content:"không thực hiện việc cung cấp, cập nhật, truyền, lưu trữ, quản lý các thông tin từ thiết bị giám sát hành trình theo quy định; không cung cấp tên đăng nhập, mật khẩu truy cập vào phần mềm xử lý dữ liệu từ thiết bị giám sát hành trình của các xe ô tô thuộc đơn vị cho cơ quan nhà nước có thẩm quyền;"}}, {_id:1140, properties:{index:"d", content:"không thực hiện đúng các nội dung đã đăng ký, niêm yết về: hành trình chạy xe; điểm đầu, điểm cuối của tuyến; giá cước; giá dịch vụ; tiêu chuẩn chất lượng dịch vụ vận tải, dịch vụ hỗ trợ vận tải;"}}, {_id:3163, properties:{index:"a", content:"dốc gù là hệ thống thiết bị phục vụ công tác dồn tàu mà khi đầu máy đẩy đoàn toa xe đến đỉnh dốc, sẽ tiến hành tác nghiệp cắt nối toa xe để các toa xe lợi dụng thế năng của đỉnh dốc tự chạy vào các đường trong bãi dồn;"}}, {_id:129, properties:{index:"a", content:"không chấp hành hiệu lệnh, chỉ dẫn của biển báo hiệu, vạch kẻ đường, trừ các hành vi vi phạm quy định tại điểm a, điểm d, điểm g, điểm h, điểm i, điểm k khoản 2; điểm a, điểm d, điểm đ, điểm e, điểm k, điểm l, điểm o, điểm r, điểm s khoản 3; điểm b, điểm c, điểm d, điểm đ, điểm i khoản 4; điểm a, điểm c, điểm d, điểm đ, điểm e, điểm g, điểm i khoản 5; điểm a, điểm b khoản 6; điểm a, điểm c khoản 7; điểm a khoản 8 điều này;"}}, {_id:131, properties:{index:"b", content:"chuyển hướng không nhường quyền đi trước cho: người đi bộ, xe lăn của người khuyết tật qua đường tại nơi có vạch kẻ đường dành cho người đi bộ; xe thô sơ đang đi trên phần đường dành cho xe thô sơ;"}}, {_id:132, properties:{index:"c", content:"chuyển hướng không nhường đường cho: các xe đi ngược chiều; người đi bộ, xe lăn của người khuyết tật đang qua đường tại nơi không có vạch kẻ đường cho người đi bộ;"}}, {_id:135, properties:{index:"d", content:"khi dừng xe, đỗ xe không có tín hiệu báo cho người điều khiển phương tiện khác biết;"}}, {_id:137, properties:{index:"đ", content:"khi đỗ xe chiếm một phần đường xe chạy không đặt ngay báo hiệu nguy hiểm theo quy định, trừ hành vi vi phạm quy định tại điểm c khoản 6 điều này và trường hợp đỗ xe tại vị trí quy định được phép đỗ xe;"}}, {_id:140, properties:{index:"e", content:"không gắn biển báo hiệu ở phía trước xe kéo, phía sau xe được kéo; điều khiển xe kéo rơ moóc không có biển báo hiệu theo quy định;"}}, {_id:143, properties:{index:"g", content:"bấm còi trong đô thị và khu đông dân cư trong thời gian từ 22 giờ ngày hôm trước đến 05 giờ ngày hôm sau, trừ các xe ưu tiên đang đi làm nhiệm vụ theo quy định."}}, {_id:3166, properties:{index:"b", content:"dồn phóng là phương pháp lợi dụng động năng của đoàn dồn để phóng toa xe hoặc cụm toa xe vào các đường trong bãi dồn;"}}] AS row
