UNWIND [{_id:2485, properties:{index:"a", content:"đường chuyên dùng, đường không do nhà nước quản lý khai thác, đường được đầu tư xây dựng không bằng nguồn vốn từ ngân sách nhà nước do chủ đầu tư tổ chức quản lý, bảo trì theo quy định."}}, {_id:2490, properties:{index:"a", content:"ngân sách nhà nước phân bổ hàng năm;"}}, {_id:2491, properties:{index:"b", content:"các nguồn thu liên quan đến sử dụng đường bộ và các nguồn thu khác theo quy định của pháp luật."}}, {_id:2506, properties:{index:"a", content:"bộ giao thông vận tải tổ chức và hướng dẫn thực hiện việc bảo vệ kết cấu hạ tầng giao thông đường bộ; kiểm tra, thanh tra việc thực hiện các quy định của pháp luật về quản lý, bảo vệ kết cấu hạ tầng giao thông đường bộ;"}}, {_id:2507, properties:{index:"b", content:"bộ công an chỉ đạo, hướng dẫn lực lượng công an kiểm tra, xử lý vi phạm pháp luật về bảo vệ kết cấu hạ tầng giao thông đường bộ theo thẩm quyền;"}}, {_id:2508, properties:{index:"c", content:"uỷ ban nhân dân các cấp tổ chức bảo vệ kết cấu hạ tầng giao thông đường bộ trong phạm vi địa phương; bảo vệ hành lang an toàn giao thông đường bộ theo quy định của pháp luật;"}}, {_id:2509, properties:{index:"d", content:"bộ, cơ quan ngang bộ trong phạm vi nhiệm vụ, quyền hạn của mình có trách nhiệm phối hợp bảo vệ kết cấu hạ tầng giao thông đường bộ;"}}, {_id:2510, properties:{index:"đ", content:"chính phủ quy định việc phối hợp giữa các bộ, cơ quan ngang bộ, ủy ban nhân dân về bảo vệ kết cấu hạ tầng giao thông đường bộ."}}, {_id:2515, properties:{index:"a", content:"có đủ hệ thống hãm có hiệu lực;"}}, {_id:2516, properties:{index:"b", content:"có hệ thống chuyển hướng có hiệu lực;"}}, {_id:2517, properties:{index:"c", content:"tay lái của xe ô tô ở bên trái của xe; trường hợp xe ô tô của người nước ngoài đăng ký tại nước ngoài có tay lái ở bên phải tham gia giao thông tại việt nam thực hiện theo quy định của chính phủ;"}}, {_id:2518, properties:{index:"d", content:"có đủ đèn chiếu sáng gần và xa, đèn soi biển số, đèn báo hãm, đèn tín hiệu;"}}, {_id:2519, properties:{index:"đ", content:"có bánh lốp đúng kích cỡ và đúng tiêu chuẩn kỹ thuật của từng loại xe;"}}, {_id:2520, properties:{index:"e", content:"có đủ gương chiếu hậu và các trang bị, thiết bị khác bảo đảm tầm nhìn cho người điều khiển;"}}, {_id:2521, properties:{index:"g", content:"kính chắn gió, kính cửa là loại kính an toàn;"}}, {_id:2522, properties:{index:"h", content:"có còi với âm lượng đúng quy chuẩn kỹ thuật;"}}, {_id:2523, properties:{index:"i", content:"có đủ bộ phận giảm thanh, giảm khói và các trang bị, thiết bị khác bảo đảm khí thải, tiếng ồn theo quy chuẩn môi trường;"}}, {_id:2524, properties:{index:"k", content:"các kết cấu phải đủ độ bền và bảo đảm tính năng vận hành ổn định."}}, {_id:2546, properties:{index:"c", content:"có đèn chiếu sáng;"}}, {_id:2547, properties:{index:"d", content:"bảo đảm tầm nhìn cho người điều khiển;"}}] AS row
