UNWIND [{_id:1960, properties:{index:2, content:"trường hợp nhân viên thuộc các cơ quan quản lý đường bộ và các tổ chức được giao khai thác, bảo trì kết cấu hạ tầng giao thông đường bộ không là công chức, viên chức thì phải được ký hợp đồng lao động không xác định thời hạn theo quy định của pháp luật về lao động;"}}, {_id:800, properties:{index:1, content:"phạt tiền từ 15.000.000 đồng đến 20.000.000 đồng đối với cá nhân, từ 30.000.000 đồng đến 40.000.000 đồng đối với tổ chức thực hiện một trong các hành vi vi phạm sau đây:"}}, {_id:803, properties:{index:2, content:"ngoài việc bị áp dụng hình thức xử phạt, cá nhân, tổ chức thực hiện hành vi vi phạm còn bị áp dụng các biện pháp khắc phục hậu quả sau đây:"}}, {_id:1963, properties:{index:3, content:"người sử dụng thiết bị ghi hình"}}, {_id:1966, properties:{index:4, content:"người sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ phải được tập huấn về quy trình, thao tác sử dụng, bảo quản phương tiện, thiết bị kỹ thuật nghiệp vụ và các quy định của pháp luật về xử phạt vi phạm hành chính trong lĩnh vực đường bộ có liên quan."}}, {_id:1967, properties:{index:5, content:"khi thực hiện nhiệm vụ, người sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ phải mặc trang phục của cơ quan, đơn vị; đeo thẻ tên, chức danh của cơ quan, đơn vị quản lý phương tiện, thiết bị kỹ thuật nghiệp vụ."}}, {_id:1968, properties:{index:6, content:"trách nhiệm của người sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ"}}, {_id:1972, properties:{index:1, content:"phương tiện, thiết bị kỹ thuật nghiệp vụ phải đáp ứng yêu cầu và sử dụng đúng nguyên tắc theo quy định tại khoản 2, khoản 3 điều 79 nghị định số 46/2016/nđ-cp ngày 26/5/2016 của chính phủ quy định xử phạt vi phạm hành chính trong lĩnh vực giao thông đường bộ và đường sắt."}}, {_id:1973, properties:{index:2, content:"đối với thiết bị cân kiểm tra tải trọng xe"}}, {_id:809, properties:{index:1, content:"cảnh cáo hoặc phạt tiền từ 60.000 đồng đến 100.000 đồng đối với cá nhân thực hiện một trong các hành vi vi phạm sau đây:"}}, {_id:1977, properties:{index:3, content:"đối với thiết bị ghi hình"}}, {_id:814, properties:{index:2, content:"phạt tiền từ 200.000 đồng đến 300.000 đồng đối với cá nhân, từ 400.000 đồng đến 600.000 đồng đối với tổ chức thực hiện một trong các hành vi vi phạm sau đây:"}}, {_id:1981, properties:{index:4, content:"kết quả thu được bằng phương tiện, thiết bị kỹ thuật nghiệp vụ phải được ghi nhận bằng phiếu cân kiểm tra tải trọng xe hoặc phiếu xác nhận kết quả thiết bị ghi hình."}}, {_id:817, properties:{index:3, content:"phạt tiền từ 6.000.000 đồng đến 8.000.000 đồng đối với tổ chức được giao quản lý, khai thác, bảo trì kết cấu hạ tầng giao thông đường bộ thực hiện một trong các hành vi vi phạm sau đây:"}}, {_id:1982, properties:{index:5, content:"thông tin, dữ liệu thu được từ phương tiện, thiết bị kỹ thuật nghiệp vụ phải được đơn vị trực tiếp quản lý, sử dụng phương tiện, thiết bị kỹ thuật nghiệp vụ được lưu trữ theo quy định của pháp luật về lưu trữ."}}, {_id:1984, properties:{index:1, content:"phiếu cân kiểm tra tải trọng xe (sau đây viết tắt là phiếu cân)"}}, {_id:1987, properties:{index:2, content:"phiếu xác nhận kết quả thiết bị ghi hình"}}, {_id:822, properties:{index:4, content:"phạt tiền từ 3.000.000 đồng đến 5.000.000 đồng đối với cá nhân, từ 6.000.000 đồng đến 10.000.000 đồng đối với tổ chức thực hiện một trong các hành vi vi phạm sau đây:"}}, {_id:1990, properties:{index:3, content:"quản lý phiếu cân kiểm tra tải trọng xe và phiếu xác nhận kết quả thiết bị ghi hình"}}, {_id:827, properties:{index:5, content:"phạt tiền từ 5.000.000 đồng đến 7.000.000 đồng đối với cá nhân, từ 10.000.000 đồng đến 14.000.000 đồng đối với tổ chức thực hiện một trong các hành vi vi phạm sau đây:"}}] AS row
