UNWIND [{_id:1708, properties:{index:"i", content:"không duy trì trạng thái kỹ thuật, chất lượng kết cấu hạ tầng đường sắt đã công bố hoặc để xảy ra sự cố công trình đường sắt do không thực hiện bảo trì công trình đường sắt theo quy định;"}}, {_id:1709, properties:{index:"k", content:"không kịp thời phát hiện hoặc không thực hiện theo quy định khi phát hiện, nhận được tin báo công trình đường sắt, bộ phận công trình đường sắt, thiết bị lắp đặt vào công trình đường sắt bị hư hỏng, xuống cấp về chất lượng, không đảm bảo an toàn cho việc khai thác, sử dụng;"}}, {_id:536, properties:{index:"a", content:"chuyển hướng không giảm tốc độ hoặc không có tín hiệu báo hướng rẽ (trừ trường hợp điều khiển xe đi theo hướng cong của đoạn đường bộ ở nơi đường không giao nhau cùng mức);"}}, {_id:1710, properties:{index:"l", content:"không thực hiện các thủ tục theo quy định đối với công trình đường sắt hết thời hạn sử dụng có nhu cầu tiếp tục sử dụng tiếp."}}, {_id:538, properties:{index:"b", content:"chở theo từ 03 người trở lên trên xe;"}}, {_id:1712, properties:{index:"a", content:"không tổ chức thu hẹp bề rộng hoặc xóa bỏ lối đi tự mở là vị trí nguy hiểm đối với an toàn giao thông đường sắt theo quy định;"}}, {_id:1713, properties:{index:"b", content:"không tổ chức cảnh giới, chốt gác tại lối đi tự mở là vị trí nguy hiểm đối với an toàn giao thông đường sắt theo quy định."}}, {_id:1715, properties:{index:"a", content:"không thực hiện chế độ kiểm tra hoặc thực hiện chế độ kiểm tra không đúng quy định;"}}, {_id:1716, properties:{index:"b", content:"để công trình đường sắt bị hư hỏng mà không kịp thời có biện pháp khắc phục, sửa chữa cần thiết;"}}, {_id:1717, properties:{index:"c", content:"không kịp thời tổ chức sửa chữa, bổ sung, gia cố, thay thế các hư hỏng kết cấu hạ tầng đường sắt để bảo đảm chất lượng theo công lệnh tốc độ, công lệnh tải trọng đã công bố;"}}, {_id:1718, properties:{index:"d", content:"không kiểm tra việc thực hiện phương án tổ chức thi công, biện pháp bảo đảm an toàn giao thông đường sắt tại vị trí thi công trên tuyến đường sắt theo công lệnh tải trọng, công lệnh tốc độ đã công bố."}}, {_id:541, properties:{index:"c", content:"bấm còi, rú ga (nẹt pô) liên tục trong đô thị, khu đông dân cư, trừ các xe ưu tiên đang đi làm nhiệm vụ theo quy định;"}}, {_id:1720, properties:{index:"a", content:"thực hiện hành vi quy định tại khoản 2; điểm a, điểm b khoản 3 điều này buộc phải thực hiện ngay các biện pháp bảo đảm an toàn giao thông đường sắt theo quy định;"}}, {_id:544, properties:{index:"d", content:"dừng xe, đỗ xe trên cầu;"}}, {_id:546, properties:{index:"đ", content:"điều khiển xe thành đoàn gây cản trở giao thông, trừ trường hợp được cơ quan có thẩm quyền cấp phép;"}}, {_id:1721, properties:{index:"b", content:"thực hiện hành vi quy định tại điểm c khoản 3 điều này buộc phải tổ chức sửa chữa, bổ sung, gia cố, thay thế các hư hỏng kết cấu hạ tầng đường sắt để bảo đảm chất lượng theo công lệnh tốc độ, công lệnh tải trọng đã công bố."}}, {_id:1723, properties:{index:"a", content:"thi công công trình không thông báo bằng văn bản cho tổ chức trực tiếp quản lý công trình đường sắt biết trước khi thi công;"}}, {_id:1724, properties:{index:"b", content:"không bố trí đủ thiết bị an toàn và tín hiệu theo quy định đối với phương tiện, thiết bị thi công;"}}, {_id:547, properties:{index:"e", content:"điều khiển xe có liên quan trực tiếp đến vụ tai nạn giao thông mà không dừng lại, không giữ nguyên hiện trường, không tham gia cấp cứu người bị nạn, trừ hành vi vi phạm quy định tại điểm đ khoản 8 điều này;"}}, {_id:550, properties:{index:"g", content:"điều khiển xe không đi bên phải theo chiều đi của mình; đi không đúng phần đường, làn đường quy định (làn cùng chiều hoặc làn ngược chiều); điều khiển xe đi qua dải phân cách cố định ở giữa hai phần đường xe chạy; điều khiển xe đi trên hè phố, trừ trường hợp điều khiển xe đi qua hè phố để vào nhà;"}}] AS row
