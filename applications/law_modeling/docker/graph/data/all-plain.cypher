UNWIND [{_id:2417, properties:{index:"a", content:"tên đường được đặt tên danh nhân, người có công hoặc tên di tích, sự kiện lịch sử, văn hóa, tên địa danh hoặc tên theo tập quán; số hiệu đường được đặt theo số tự nhiên kèm theo chữ cái nếu cần thiết; trường hợp đường đô thị trùng với quốc lộ thì sử dụng cả tên đường đô thị và tên, số hiệu quốc lộ;"}}, {_id:2418, properties:{index:"b", content:"tên, số hiệu đường bộ tham gia vào mạng lưới đường bộ trong khu vực, đường bộ quốc tế thực hiện theo thỏa thuận giữa việt nam với các quốc gia có liên quan"}}, {_id:2426, properties:{index:"a", content:"bộ giao thông vận tải xây dựng, hướng dẫn thực hiện tiêu chuẩn kỹ thuật các cấp đường;"}}, {_id:2427, properties:{index:"b", content:"bộ khoa học và công nghệ ban hành tiêu chuẩn kỹ thuật quốc gia các cấp đường."}}, {_id:2448, properties:{index:"a", content:"trường hợp có đường nhánh thì đường gom phải nối vào đường nhánh;"}}, {_id:2449, properties:{index:"b", content:"trường hợp đường nhánh, đường gom nối trực tiếp vào đường chính thì điểm đấu nối phải được cơ quan quản lý nhà nước có thẩm quyền về đường bộ cho phép từ khi lập dự án và thiết kế;"}}, {_id:2450, properties:{index:"c", content:"việc đấu nối các đường từ khu đô thị, khu công nghiệp, khu kinh tế, khu dân cư, khu thương mại - dịch vụ và công trình khác vào đường bộ theo quy định của bộ trưởng bộ giao thông vận tải."}}, {_id:2454, properties:{index:"a", content:"đèn tín hiệu giao thông;"}}, {_id:2455, properties:{index:"b", content:"biển báo hiệu;"}}, {_id:2456, properties:{index:"c", content:"cọc tiêu, rào chắn hoặc tường bảo vệ;"}}, {_id:2457, properties:{index:"d", content:"vạch kẻ đường;"}}, {_id:2458, properties:{index:"đ", content:"cột cây số;"}}, {_id:2459, properties:{index:"e", content:"công trình báo hiệu khác."}}, {_id:2472, properties:{index:"a", content:"chỉ được đào đường để sửa chữa công trình hoặc xây dựng mới hầm kỹ thuật dọc theo đường hoặc ngang qua đường nhưng phải có kế hoạch hàng năm thống nhất trước với cơ quan quản lý đường bộ, trừ trường hợp có sự cố đột xuất;"}}, {_id:2473, properties:{index:"b", content:"phải có phương án thi công và thời gian thi công thích hợp với đặc điểm từng đường phố để không gây ùn tắc giao thông;"}}, {_id:2474, properties:{index:"c", content:"khi thi công xong phải hoàn trả phần đường theo nguyên trạng; đối với công trình ngầm phải lập hồ sơ hoàn công và chuyển cho cơ quan quản lý đường bộ."}}, {_id:2479, properties:{index:"a", content:"theo dõi tình trạng công trình đường bộ; tổ chức giao thông; kiểm tra, thanh tra việc bảo vệ kết cấu hạ tầng giao thông đường bộ;"}}, {_id:2480, properties:{index:"b", content:"bảo dưỡng thường xuyên, sửa chữa định kỳ và sửa chữa đột xuất."}}, {_id:2482, properties:{index:"a", content:"hệ thống quốc lộ do bộ giao thông vận tải chịu trách nhiệm;"}}, {_id:2483, properties:{index:"b", content:"hệ thống đường tỉnh, đường đô thị do ủy ban nhân dân cấp tỉnh chịu trách nhiệm"}}] AS row
