UNWIND [{_id:280, properties:{index:"i", content:"lùi xe, quay đầu xe trong hầm đường bộ; dừng xe, đỗ xe, vượt xe trong hầm đường bộ không đúng nơi quy định."}}, {_id:1303, properties:{index:"a", content:"mang hóa chất độc hại, chất dễ cháy, nổ, hàng nguy hiểm hoặc hàng cấm lưu thông trên xe khách;"}}, {_id:1304, properties:{index:"b", content:"đu, bám vào thành xe; đứng, ngồi, nằm trên mui xe, nóc xe, trong khoang chở hành lý; tự ý mở cửa xe hoặc có hành vi khác không bảo đảm an toàn khi xe đang chạy."}}, {_id:287, properties:{index:"a", content:"không chấp hành hiệu lệnh của đèn tín hiệu giao thông;"}}, {_id:292, properties:{index:"b", content:"không chấp hành hiệu lệnh, hướng dẫn của người điều khiển giao thông hoặc người kiểm soát giao thông;"}}, {_id:1312, properties:{index:"a", content:"chở hàng vượt khổ giới hạn của cầu, đường ghi trong giấy phép lưu hành;"}}, {_id:1313, properties:{index:"b", content:"điều khiển xe bánh xích tham gia giao thông không có giấy phép lưu hành hoặc có giấy phép lưu hành nhưng không còn giá trị sử dụng theo quy định hoặc lưu thông trực tiếp trên đường mà không thực hiện biện pháp bảo vệ đường theo quy định;"}}, {_id:1314, properties:{index:"c", content:"điều khiển xe vượt quá khổ giới hạn của cầu, đường hoặc chở hàng vượt khổ giới hạn của cầu, đường tham gia giao thông, trừ trường hợp có giấy phép lưu hành còn giá trị sử dụng;"}}, {_id:1315, properties:{index:"d", content:"điều khiển xe mà tổng trọng lượng (khối lượng toàn bộ) của xe hoặc tải trọng trục xe (bao gồm cả hàng hóa xếp trên xe, người được chở trên xe) vượt quá tải trọng cho phép của cầu, đường trên 20% đến 50%, trừ trường hợp có giấy phép lưu hành còn giá trị sử dụng."}}, {_id:1317, properties:{index:"a", content:"điều khiển xe mà tổng trọng lượng (khối lượng toàn bộ) của xe hoặc tải trọng trục xe (bao gồm cả hàng hóa xếp trên xe, người được chở trên xe) vượt quá tải trọng cho phép của cầu, đường trên 50% đến 100%, trừ trường hợp có giấy phép lưu hành còn giá trị sử dụng;"}}, {_id:297, properties:{index:"c", content:"đi ngược chiều của đường một chiều, đi ngược chiều trên đường có biển “cấm đi ngược chiều”, trừ các hành vi vi phạm quy định tại điểm a khoản 8 điều này và các trường hợp xe ưu tiên đang đi làm nhiệm vụ khẩn cấp theo quy định;"}}, {_id:1318, properties:{index:"b", content:"điều khiển xe có giấy phép lưu hành còn giá trị sử dụng nhưng tổng trọng lượng (khối lượng toàn bộ) của xe hoặc tải trọng trục xe (bao gồm cả hàng hóa xếp trên xe nếu có) vượt quá quy định trong giấy phép lưu hành;"}}, {_id:1319, properties:{index:"c", content:"điều khiển xe có giấy phép lưu hành còn giá trị sử dụng nhưng đi không đúng tuyến đường quy định trong giấy phép lưu hành."}}, {_id:301, properties:{index:"d", content:"vượt xe trong những trường hợp không được vượt, vượt xe tại đoạn đường có biển báo hiệu có nội dung cấm vượt (đối với loại phương tiện đang điều khiển); không có báo hiệu trước khi vượt; vượt bên phải xe khác trong trường hợp không được phép, trừ trường hợp tại đoạn đường có nhiều làn đường cho xe đi cùng chiều được phân biệt bằng vạch kẻ phân làn đường mà xe chạy trên làn đường bên phải chạy nhanh hơn xe đang chạy trên làn đường bên trái;"}}, {_id:1322, properties:{index:"a", content:"điều khiển xe mà tổng trọng lượng (khối lượng toàn bộ) của xe hoặc tải trọng trục xe (bao gồm cả hàng hóa xếp trên xe, người được chở trên xe) vượt quá tải trọng cho phép của cầu, đường trên 150%, trừ trường hợp có giấy phép lưu hành còn giá trị sử dụng;"}}, {_id:1323, properties:{index:"b", content:"không chấp hành việc kiểm tra tải trọng, khổ giới hạn xe khi có tín hiệu, hiệu lệnh yêu cầu kiểm tra tải trọng, khổ giới hạn xe; chuyển tải hoặc dùng các thủ đoạn khác để trốn tránh việc phát hiện xe chở quá tải, quá khổ."}}, {_id:1325, properties:{index:"a", content:"thực hiện hành vi quy định tại khoản 1, khoản 3, điểm a khoản 4 điều này còn bị tước quyền sử dụng giấy phép lái xe (khi điều khiển ô tô, máy kéo và các loại xe tương tự ô tô), chứng chỉ bồi dưỡng kiến thức pháp luật về giao thông đường bộ (khi điều khiển xe máy chuyên dùng) từ 01 tháng đến 03 tháng;"}}, {_id:306, properties:{index:"đ", content:"điều khiển xe không đi bên phải theo chiều đi của mình; đi không đúng phần đường hoặc làn đường quy định (làn cùng chiều hoặc làn ngược chiều) trừ hành vi quy định tại điểm c khoản 4 điều này; điều khiển xe đi qua dải phân cách cố định ở giữa hai phần đường xe chạy; điều khiển xe đi trên hè phố, trừ trường hợp điều khiển xe đi qua hè phố để vào nhà;"}}, {_id:1326, properties:{index:"b", content:"thực hiện hành vi quy định tại điểm b, điểm c khoản 4; khoản 5 điều này còn bị tước quyền sử dụng giấy phép lái xe (khi điều khiển ô tô, máy kéo và các loại xe tương tự ô tô), chứng chỉ bồi dưỡng kiến thức pháp luật về giao thông đường bộ (khi điều khiển xe máy chuyên dùng) từ 02 tháng đến 04 tháng;"}}, {_id:1327, properties:{index:"c", content:"thực hiện hành vi quy định tại khoản 6 điều này còn bị tước quyền sử dụng giấy phép lái xe (khi điều khiển ô tô, máy kéo và các loại xe tương tự ô tô), chứng chỉ bồi dưỡng kiến thức pháp luật về giao thông đường bộ (khi điều khiển xe máy chuyên dùng) từ 03 tháng đến 05 tháng."}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Point;
UNWIND [{_id:311, properties:{index:"e", content:"tránh xe đi ngược chiều không đúng quy định, trừ hành vi vi phạm sử dụng đèn chiếu xa khi tránh xe đi ngược chiều quy định tại điểm g khoản 3 điều này; không nhường đường cho xe đi ngược chiều theo quy định tại nơi đường hẹp, đường dốc, nơi có chướng ngại vật;"}}, {_id:1332, properties:{index:"a", content:"tụ tập để cổ vũ, kích động hành vi điều khiển xe chạy quá tốc độ quy định, lạng lách, đánh võng, đuổi nhau trên đường hoặc đua xe trái phép;"}}, {_id:1333, properties:{index:"b", content:"đua xe đạp, đua xe đạp máy, đua xe xích lô, đua xe súc vật kéo, cưỡi súc vật chạy đua trái phép trên đường giao thông."}}, {_id:314, properties:{index:"g", content:"không tuân thủ các quy định khi vào hoặc ra đường cao tốc; điều khiển xe chạy ở làn dừng xe khẩn cấp hoặc phần lề đường của đường cao tốc; chuyển làn đường không đúng nơi cho phép hoặc không có tín hiệu báo trước khi chạy trên đường cao tốc; không tuân thủ quy định về khoảng cách an toàn đối với xe chạy liền trước khi chạy trên đường cao tốc;"}}, {_id:1337, properties:{index:"a", content:"thực hiện hành vi quy định tại điểm b khoản 1 điều này bị tịch thu phương tiện (trừ súc vật kéo, cưỡi);"}}, {_id:1338, properties:{index:"b", content:"thực hiện hành vi quy định tại khoản 2, khoản 3 điều này bị tước quyền sử dụng giấy phép lái xe từ 03 tháng đến 05 tháng và tịch thu phương tiện."}}, {_id:1342, properties:{index:"a", content:"điều khiển phương tiện không gắn ký hiệu phân biệt quốc gia theo quy định;"}}, {_id:1343, properties:{index:"b", content:"giấy tờ của phương tiện không có bản dịch sang tiếng anh hoặc tiếng việt theo quy định;"}}, {_id:1344, properties:{index:"c", content:"xe chở khách không có danh sách hành khách theo quy định."}}, {_id:321, properties:{index:"h", content:"không nhường đường hoặc gây cản trở xe được quyền ưu tiên đang phát tín hiệu ưu tiên đi làm nhiệm vụ;"}}, {_id:1346, properties:{index:"a", content:"lưu hành phương tiện trên lãnh thổ việt nam quá thời hạn quy định;"}}, {_id:1347, properties:{index:"b", content:"hoạt động quá phạm vi được phép hoạt động;"}}, {_id:1348, properties:{index:"c", content:"điều khiển phương tiện không có giấy phép vận tải quốc tế, phù hiệu vận tải quốc tế liên vận theo quy định hoặc có nhưng đã hết giá trị sử dụng;"}}, {_id:1349, properties:{index:"d", content:"điều khiển phương tiện không gắn biển số tạm thời hoặc gắn biển số tạm thời không do cơ quan có thẩm quyền cấp (nếu có quy định phải gắn biển số tạm thời);"}}, {_id:1350, properties:{index:"đ", content:"vận chuyển hành khách hoặc hàng hóa không đúng với quy định tại hiệp định vận tải đường bộ, nghị định thư đã ký kết;"}}, {_id:1351, properties:{index:"e", content:"điều khiển xe ô tô có tay lái bên phải, xe ô tô hoặc xe mô tô của người nước ngoài vào việt nam du lịch tham gia giao thông mà không có xe dẫn đường theo quy định;"}}, {_id:325, properties:{index:"i", content:"điều khiển xe chạy quá tốc độ quy định từ 10 km/h đến 20 km/h."}}, {_id:1352, properties:{index:"g", content:"điều khiển xe ô tô có tay lái bên phải, xe ô tô gắn biển số nước ngoài tham gia giao thông mà người điều khiển xe không đúng quốc tịch theo quy định."}}, {_id:332, properties:{index:"a", content:"điều khiển xe chạy quá tốc độ quy định trên 20 km/h đến 35 km/h;"}}, {_id:333, properties:{index:"b", content:"dừng xe, đỗ xe trên đường cao tốc không đúng nơi quy định; không có báo hiệu để người lái xe khác biết khi buộc phải dừng xe, đỗ xe trên đường cao tốc không đúng nơi quy định; quay đầu xe trên đường cao tốc;"}}] AS row
CREATE (n:`UNIQUE IMPORT LABEL`{`UNIQUE IMPORT ID`: row._id}) SET n += row.properties SET n:Point;
UNWIND [{_id:1359, properties:{index:"a", content:"không có tờ khai phương tiện vận tải đường bộ tạm nhập, tái xuất theo quy định;"}}, {_id:1360, properties:{index:"b", content:"điều khiển xe không có phù hiệu kiểm soát theo quy định hoặc có nhưng đã hết giá trị sử dụng hoặc sử dụng phù hiệu không do cơ quan có thẩm quyền cấp."}}, {_id:1362, properties:{index:"a", content:"thực hiện hành vi quy định tại điểm b khoản 1, điểm b khoản 2 điều này trường hợp sử dụng phù hiệu đã hết giá trị sử dụng, phù hiệu không do cơ quan có thẩm quyền cấp còn bị tịch thu phù hiệu;"}}, {_id:1363, properties:{index:"b", content:"tái phạm hoặc vi phạm nhiều lần hành vi quy định tại khoản 1, khoản 2 điều này bị tịch thu phương tiện."}}, {_id:338, properties:{index:"c", content:"điều khiển xe trên đường mà trong máu hoặc hơi thở có nồng độ cồn nhưng chưa vượt quá 50 miligam/100 mililít máu hoặc chưa vượt quá 0,25 miligam/1 lít khí thở."}}, {_id:1368, properties:{index:"a", content:"giáo viên dạy thực hành để học viên không có phù hiệu “học viên tập lái xe” lái xe tập lái hoặc có phù hiệu nhưng không đeo khi lái xe tập lái;"}}, {_id:1369, properties:{index:"b", content:"giáo viên dạy thực hành chở người, hàng trên xe tập lái trái quy định;"}}, {_id:1370, properties:{index:"c", content:"giáo viên dạy thực hành chạy sai tuyến đường trong giấy phép xe tập lái; không ngồi bên cạnh để bảo trợ tay lái cho học viên thực hành lái xe (kể cả trong sân tập lái và ngoài đường giao thông công cộng);"}}, {_id:344, properties:{index:"a", content:"không chú ý quan sát, điều khiển xe chạy quá tốc độ quy định gây tai nạn giao thông; dừng xe, đỗ xe, quay đầu xe, lùi xe, tránh xe, vượt xe, chuyển hướng, chuyển làn đường không đúng quy định gây tai nạn giao thông; không đi đúng phần đường, làn đường, không giữ khoảng cách an toàn giữa hai xe theo quy định gây tai nạn giao thông hoặc đi vào đường có biển báo hiệu có nội dung cấm đi vào đối với loại phương tiện đang điều khiển, đi ngược chiều của đường một chiều, đi ngược chiều trên đường có biển “cấm đi ngược chiều” gây tai nạn giao thông, trừ các hành vi vi phạm quy định tại điểm a khoản 8 điều này;"}}, {_id:1371, properties:{index:"d", content:"không đeo phù hiệu “giáo viên dạy lái xe” khi giảng dạy;"}}, {_id:1372, properties:{index:"đ", content:"không có giáo án của môn học được phân công giảng dạy theo quy định hoặc có giáo án nhưng không phù hợp với môn được phân công giảng dạy;"}}, {_id:1373, properties:{index:"e", content:"giáo viên dạy thực hành không mang theo giấy phép xe tập lái hoặc mang theo giấy phép xe tập lái đã hết giá trị sử dụng."}}, {_id:348, properties:{index:"b", content:"điều khiển xe lạng lách, đánh võng; chạy quá tốc độ đuổi nhau trên đường bộ; dùng chân điều khiển vô lăng xe khi xe đang chạy trên đường;"}}, {_id:349, properties:{index:"c", content:"điều khiển xe chạy quá tốc độ quy định trên 35 km/h."}}, {_id:1375, properties:{index:"a", content:"sử dụng xe tập lái không có mui che mưa, nắng; không có ghế ngồi gắn chắc chắn trên thùng xe cho người học theo quy định;"}}, {_id:1376, properties:{index:"b", content:"không thực hiện việc ký hợp đồng đào tạo, thanh lý hợp đồng đào tạo với người học lái xe theo quy định hoặc có ký hợp đồng đào tạo, thanh lý hợp đồng đào tạo nhưng không do người học lái xe trực tiếp ký;"}}, {_id:1377, properties:{index:"c", content:"không công khai quy chế tuyển sinh, quản lý đào tạo và mức thu học phí theo quy định."}}, {_id:1378, properties:{index:"a", content:"cơ sở đào tạo lái xe không bố trí giáo viên dạy thực hành ngồi bên cạnh để bảo trợ tay lái cho học viên thực hành lái xe; bố trí giáo viên không đủ tiêu chuẩn để giảng dạy;"}}, {_id:1379, properties:{index:"b", content:"cơ sở đào tạo lái xe sử dụng xe tập lái không có “giấy phép xe tập lái” hoặc có nhưng hết hạn, không gắn biển xe \"tập lái\" trên xe theo quy định, không ghi tên cơ sở đào tạo, số điện thoại ở mặt ngoài hai bên cánh cửa hoặc hai bên thành xe theo quy định;"}}, {_id:1380, properties:{index:"c", content:"cơ sở đào tạo lái xe sử dụng xe tập lái không trang bị thêm bộ phận hãm phụ hoặc có nhưng không có tác dụng;"}}] AS row
